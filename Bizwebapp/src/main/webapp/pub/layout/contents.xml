<?xml version="1.0" encoding="UTF-8"?>
<html xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ev="http://www.w3.org/2001/xml-events"
    xmlns:w2="http://www.inswave.com/websquare" xmlns:xf="http://www.w3.org/2002/xforms">
    <head>
    	<w2:type>DEFAULT</w2:type>
        <w2:buildDate/>
        <xf:model>
            <xf:instance>
                <data xmlns=""/>
            </xf:instance>
            <w2:dataCollection baseNode="map">
            </w2:dataCollection>
            <w2:workflowCollection>
            </w2:workflowCollection>
        </xf:model>
    <script type="text/javascript" lazy="false"><![CDATA[
        
	scwin.onpageload = function() {
		$p.$("#grp_left").resizable({
			handleSelector: "#" + grp_splitter.getID(),
			resizeHeight: false
		});
		
		$p.$("#grp_right").resizable({
			handleSelector: ".splitter-horizontal",
			resizeWidth: false
		});
	};
	
	scwin.onpageunload = function() {
		
	};
	
	]]></script>	
    </head>
    <body ev:onpageload="scwin.onpageload">
    	<xf:group class="locationSection" id="" style="">
    		<xf:group class="subject" id="">
    			<w2:textbox class="tit_page" id="" label="재택근무신청조회" style="" tagname="h2"></w2:textbox>
    			<w2:anchor class="ico bookmark on" id="" outerDiv="false" style="" tooltip="즐겨찾기">
    				<xf:label><![CDATA[즐겨찾기]]></xf:label>
    			</w2:anchor>
    			<w2:anchor class="ico help" id="" outerDiv="false" style="" tooltip="도움말">
    				<xf:label><![CDATA[도움말]]></xf:label>
    			</w2:anchor>
    			<w2:anchor class="ico refresh" id="" outerDiv="false" style="" tooltip="새로고침">
    				<xf:label><![CDATA[새로고침]]></xf:label>
    			</w2:anchor>
    		</xf:group>
    		<xf:group class="breadcrumb" id="">
    			<w2:textbox class="ico home" id="" label="Home" style=""></w2:textbox>
    			<w2:span id="" label="1Depth" style=""></w2:span>
    			<w2:span id="" label="2Depth" style=""></w2:span>
    			<w2:span class="here" id="" label="현재페이지" style=""></w2:span>
    		</xf:group>
    	</xf:group>
    	<xf:group id="" class="pageWrap">
    		<xf:group id="" class="panel-container">
    			<xf:group id="grp_left" class="panel-left">
    				<w2:wframe id="" src="/pub/layout/formlist.xml" style=""></w2:wframe>
    			</xf:group>
    			<xf:group id="grp_splitter" class="splitter"></xf:group>
    			<xf:group id="grp_right" class="panel-right">
    				<w2:wframe id="" src="/pub/sample_form.xml" style=""></w2:wframe>
    			</xf:group>
    		</xf:group>
    	</xf:group>
    </body>
</html>
