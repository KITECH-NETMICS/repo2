         <script type="text/javascript" lazy="false"><![CDATA[
	/****************************************************
 	 * 화면 설명 : 기업부담금 입금의뢰
 	 * 변경 이력 : 2023-12-14	이영희  최초 작성
 	 * 화면아이디 : S962
 	 ****************************************************/  
 	 /*
     * 페이지 열기
     */        
	scwin.onpageload = function() {
		// 첨부파일 컴포넌트 초기화
		com.file.init();
		com.file.create(upload_A.getID(), {fileKey: "S962", mode: 'edit'});	
		
		// parameter setting
		dma_search.setJSON(com.getParameter());
		
		var codeOptions = [ { code : "FAA", compID :"sbx_bank" } // 은행명 공통코드
			];
		com.setCmnCd(codeOptions, function() {
			if(dma_search.get("req_no") == ""){
				ica_req_ymd.setValue(com.getServerDateTime());
				
				com.pop.open(com.pop.TYPE.HUM,{empno:com.getLoginInfo("empno")}, function(ret){
					dma_detls.set("req_psn_empno", ret.empno);
					dma_detls.set("req_psn_nm", ret.nm);
					dma_detls.set("req_psn", ret.syspayno);
					dma_detls.set("dept_nm", ret.dept_nm);
					dma_detls.set("dept_cd", ret.dept_cd);
					dma_detls.set("dept_new_ymd", ret.dept_new_ymd);
				});
			}
			
			scwin.search();
		});
	};
	
	scwin.onpageunload = function() {
	};
	
	/*
     * 조회실행
     */ 
	scwin.search = function() {
		com.execute(sbm_selectSptTltDepstInfoDetail, {
			successCallback : function(e) {
				dma_info.setJSON(	e.responseJSON.elData.SptTltDepstInfoVo); // 입금관리정보
				dlt_item.setJSON(	e.responseJSON.elData.SptTltDepstItemVoList); // 기업지원직내역
				dlt_detls.setJSON(	e.responseJSON.elData.SptTltDepstDetlsVoList); // 입금의뢰이력
				scwin.search_after();
			}
		});
	};
	scwin.search_after = function(){
		if(dma_search.get("req_no") != '') 	scwin.search_req();
		else 								scwin.upload_setting('S962','edit');
	};
	scwin.search_req = function() {
		// svrSptTltBasicService51 | SptTltBasicService | SptTltBasicBiz | selectSptTltDepstReqDetlsInfo
		// aaa | SptTltDepstInfoService
		// 입금의뢰 	: kr.re.kitech.biz.sup.tlt.selectSptTltDepstDetlsList ( SptTltDepstDetls_SQL_informix_MyBatis.xml )
		// AS-IS	: kitech.sup.tlt.xda.SptTltDepstDetlsSS02
		com.execute(sbm_selectSptTltDepstDetlsDetail, {
			successCallback : function(e) {
				dma_detls.setJSON(	e.responseJSON.elData.SptTltDepstDetlsVo); // 입금의뢰
				scwin.search_req_after();
			}
		});
	};
	scwin.search_req_after = function(){
		// 결재상태와 권한에 따라 화면제어
		scwin.setArrangeObj();
	};
	
	// 계정번호 클릭시
	scwin.ibx_accnt_no_onclick = function(e) {
		// 계정정보 팝업 호출
		scwin.acnno_popup();
	};
	
	// 계정번호 찾기 버튼 클릭시
	scwin.btn_accnt_no_onclick = function(e) {
		// 계정정보 팝업 호출
		scwin.acnno_popup();
	};
	
	// 계정정보 팝업 호출
	scwin.acnno_popup = function() {
		var data = { ref_yn : "Y" }; // ref_yn : 팝업조회여부(Y 가 조회안함)
		// TODO
		//data.accnt_no = dma_search.get('accnt_no');
		com.pop.open( com.pop.TYPE.ACNNO, data, function(ret){
			scwin.acnno_popup_callback(ret);
		});
	};
	scwin.acnno_popup_callback = function(ret) {
		dma_detls.set('accnt_no', ret.accnt_no);
	};
	
	// 저장 버튼 클릭
	scwin.btn_save_onclick = function(e) {
		if(!scwin.pre_save()) return false;
	
		dma_detls.set('apr_state','XAD010');
		dma_detls.set('depst_state','RDF003'); // 입금의뢰
		com.confirm("message.xom.wq.017"/*저장 하시겠습니까?*/, function(ret){
			if(ret){
				scwin.save();
			}
		});
	};
	scwin.pre_save = function(){
		// 벨리데이션 체크
		if(!com.validateData(dma_detls,[
			{ id : "req_ymd", mandatory : true }
		   ,{ id : "accnt_no", mandatory : true } 
		   ,{ id : "rmk", mandatory : true } 
		], false)) return false;
		
		if(dma_detls.get('depst_ymd') == ''){
			alert("입금조회를 조회해 주십시오.");
			return false;
		}
		
		return true;
	};
	
	
	//////////////////////////////////////////////////////
	// 저장 처리 
	scwin.save = function(e) {
		alert('준비중'); return;
	};
	
	
	
	
	
	
	// 목록 버튼 클릭 시
	scwin.btn_list_onclick = function(e) {
		com.moveList("S961");
	};
	
	// 결재상태와 권한에 따라 화면제어
	scwin.setArrangeObj = function() {
		var attach_file_no = dma_detls.get('attach_file_no').trim();
		if(attach_file_no == '') attach_file_no = 'S962';
		scwin.upload_setting(attach_file_no,'edit');
		
		//alert(dma_detls.get('apr_state') +'/'+dma_detls.get('depst_state'));
		
		//com.biz.disabledButtons(dma_detls.get('apr_state'),dma_detls.get('req_no')); // btn_del , btn_apr , btn_save , btn_upd
		if(dma_detls.get('apr_state') > 'XAD010'){ // 작성중 이상 ( 작성중 이거나 저장중 )
			btn_depst_req.setDisabled(true);
			btn_bill.setDisabled(true);
			btn_save.setDisabled(true);
			btn_del.setDisabled(true);
			btn_apr.setDisabled(true);
		}else{
			if(dma_detls.get('depst_state') == 'RDF003'){ // 입금의뢰
				btn_depst_req.setDisabled(true);
				btn_bill.setDisabled(false);
				btn_save.setDisabled(false);
				btn_del.setDisabled(false);
				btn_apr.setDisabled(false);
			}else if(dma_detls.get('depst_state') == 'RDF002'){ // 계산서발행
				btn_depst_req.setDisabled(true);
				btn_bill.setDisabled(false);
				btn_save.setDisabled(true);
				btn_del.setDisabled(true);
				btn_apr.setDisabled(false);
			}else if(dma_detls.get('depst_state') == 'RDF004'){ // 결의생성
				if(dma_detls.get('apr_state') == 'XAD010'){ // 작성중일 경우
					btn_depst_req.setDisabled(true);
					btn_bill.setDisabled(false);
					btn_save.setDisabled(true);
					btn_del.setDisabled(true);
					btn_apr.setDisabled(false);
				}else{
					btn_depst_req.setDisabled(true);
					btn_bill.setDisabled(true);
					btn_save.setDisabled(true);
					btn_del.setDisabled(true);
					btn_apr.setDisabled(true);
				}
			}
		}
	};
	
	// exceptionCallback
	scwin.exceptionCallback = function(resObj){
		var resultCd = gcm.sbm.getResCode(resObj);
		var msg = gcm.lang.getMsg(resultCd);
		var resJSON = resObj.responseJSON || null;
		var resultMsg = gcm.sbm.getResMsg(resObj); 
		
		if (gcm.util.isEmpty(msg) || msg == resultCd || msg.indexOf("{0}") != -1) { 
			// resultCd가 등록되지 않은 메시지 코드인 경우 resultMsg alert
			msg = resultMsg;
		}
		if (!com.isEmpty(resJSON.userHeader.errBusiness)) {
			msg = "["+ resJSON.userHeader.errBusiness +"] "+ msg;
		}
		if (resJSON.userHeader.errKind == "ALERT") {
			com.alert(msg);
		} else if (resJSON.userHeader.errKind == "ERROR") {
			com.error(msg);
		} else {
			com.warn(msg);
		}
	};
	
	// 첨부파일 세팅
	scwin.upload_setting = function(attach_file_no, mode){
		// 첨부파일 세팅
		var objs = [
			{uploadId:upload_A.getID(), fileKey : attach_file_no, mode: mode},
		];
		com.file.createMulti(objs, objs.shift(), function() {
		});	
	};
	
	// 인쇄 버튼 클릭 시
	scwin.btn_print_onclick = function(e) {
		com.oz.openPopup( {
		    formAlias : "S15",
		    reqNo : "",//dma_info.get("req_no"),
		    snapFrame : grp_docMain.getID()
		});
	};
	
	// 삭제 버튼 클릭
	scwin.btn_del_onclick = function(e) {
		com.confirm("message.xom.wq.022"/*삭제 하시겠습니까?*/, function(ret) {
			if(ret){
				alert('준비중'); return;
			}
		});
	};
	
	
	
	
	
	// 결재신청 버튼 클릭 시
	scwin.btn_apr_onclick = function(e) {
		// SptTltDepstReq
		alert('준비중'); return;
	};
	
	// 계산서 버튼 클릭
	scwin.btn_bill_onclick = function(e) {
		var crudTag = '';
		if(dma_detls.get('depst_state') == 'RDF003')		crudTag = 'I';
		else if(dma_detls.get('depst_state') == 'RDF002')	crudTag = 'U'; 
		else if(dma_detls.get('depst_state') == 'RDF004') 	crudTag = 'R';
		
		var depstClsf = dma_info.get('depst_clsf');
		var itemNm = '';
		if(depstClsf == 'STX010') itemNm = '기술인재지원사업 사업비';
		
		var data={
			 searchYn 			: 'Y'
			,gfnYn 				: 'Y'
			,tax_bill_no 		: dma_detls.get('bill_no') 		// 계산서 번호
			,accnt_no 			: dma_detls.get('accnt_no') 	// 계정번호
			,vend_cd 			: dma_info.get('cust_no') 	// 업체코드(공급자)
			,suply_value 		: dma_detls.get('depst_amt') 	// 의뢰금액(공급가액)
			,req_no 			: dma_info.get('req_no')
			,demnd_recv_clsf 	: '2' 							// 발행구분([1]처리(청구계산서), [2]영수(영수계산서))
			,bill_clsf 			: '' 							// 계산서구분(FBF)
			,bill_clsf1 		: '' 							// 수금구분(FBE)(입금구분)
			,tax_flag 			: 'S' 							// 업무구분자([R]연구, [F]재무)
			,crud_tag 			: crudTag 						// 신규등록(I) 또는 상세내역 조회/수정(U) 태그
			,depst_clsf_nm 		: itemNm
			,depst_mngmt_no 	: dma_info.get('depst_mngmt_no')
			
			//demnd_recv_clsf : "2",
			//biz_clsf : dma_search.get("biz_clsf"),
			//tax_type : (scwin.data.tax_cd == "FTX001")? "01" : "03",
			//ref_yn : "Y"
		};
		com.pop.open(com.pop.TYPE.RVN_BILL, data, function(ret){
		    if(ret){
				scwin.search();
		    }
		});
	};
	
	// 입금조회 버튼 클릭
	scwin.btn_depst_req_onclick = function(e) {
		alert('준비중'); return;
	};
	
	
	// AS-IS 스크립트 확인
	/*
	1. 저장 성공시 : <!-- 입금의뢰취소 -->
	2. 조회 성공시 : <!-- 조회 -->
