         <script type="text/javascript" lazy="false"><![CDATA[
        
	scwin.onpageload = function() {
		scwin.in_it();
	};
	
	scwin.onpageunload = function() {
	};
	
	scwin.in_it = function(){
		com.setEnterKeyEvent(grp_search, scwin.search);
		// 검색 영역 기본값 세팅
		dma_search.set("rpt_yrmon", gcm.date.addMonth(com.getServerDateTime(),-1).substring(0,6));
		
		// 부서정보 조회
		scwin.search_dept();
		// 화면 로딩시 조회진행
		scwin.search();
	};
	
	// 조회 버튼 클릭
	scwin.btn_search_onclick = function(e) {
		scwin.search();
	};
	
	// 조회 전 체크
	scwin.pre_search = function(e) {
		if(!com.validateData(dma_search,[
			{ id : "rpt_yrmon", mandatory : true}
		], false)) return false;
		
		dma_search.set("date",dma_search.get("rpt_yrmon"));
		// 조회를 위한 테스트 데이터
		//dma_search.set("date","20111201"); // syspayno = '20130576' , cust_no = '149557' , cmpy_nm_krchar = '모터웰(주)'
		
		return true;
	};
	
	// 조회 실행
	scwin.search = function() {
		if(!scwin.pre_search()) return;
		// SvcSUPTLTRPTMR01.pwkjson | SptTltActivRptService.java
		// kr.re.kitech.biz.sup.tlt.selectSptTltActivRptList ( SptTltActivRpt_SQL_informix_MyBatis.xml )
		// AS-IS kitech.sup.tlt.xda.SptTltActivRptMS02
		com.execute(sbm_selectTltRptList, {
			successCallback : function(e) {
				dlt_rpt.setJSON(e.responseJSON.sptTltActivRptVoList);
				tbx_result.setValue(dlt_rpt.getTotalRow());
				scwin.search_after();
			},
			excelCallback : function(e) {
				// DB 다운로드
				var option = {
					fileName : "활동보고서목록.xlsx"
					, useDataFormat : true
					, providerInfo : {
						  service : "sptTltActivRptServiceImpl"
						, method : "selectSptTltActivRptList"
						, vo : "sup.tlt.vo.SptTltActivRptVo"
						, paramMap : dma_search
					}
				};
				com.downloadGridExcel(grd_rpt, option);
			},
			isImplExcel: true
		});
	};
	scwin.search_after = function(){
		if(dlt_rpt.getTotalRow() > 0){
			btn_down_excel.setDisabled(false);
		}else{
			btn_down_excel.setDisabled(true);
		}
		
		for(var i=0;i<dlt_rpt.getTotalRow();i++){
			if(dlt_rpt.getCellData(i, 'chk_req_no') == 'N'){
				dlt_rpt.setCellData(i, 'req_no','미작성');
				dlt_rpt.setCellData(i, 'rpt_yrmon','미작성');
			}
			if(dlt_rpt.getCellData(i, 'attach_file_no').trim() == ''){
				grd_rpt.setCellDisabled(i, 'attach_file_no_img', true);
				grd_rpt.setCellReadOnly(i, 'attach_file_no_img', true);
			}else{
				grd_rpt.setCellDisabled(i, 'attach_file_no_img', false);
				grd_rpt.setCellReadOnly(i, 'attach_file_no_img', false);
			}
		}
	};
	
	//사용자 찾기 버튼 클릭
	scwin.btn_rspns_onclick = function(e) {
		// 인사정보 팝업 호출
		scwin.hum_popup();
	};
	
	// 인사정보 팝업 호출
	scwin.hum_popup = function() {
		var nm = dma_search.get('nm');
		var empNo = dma_search.get('empno');
		var sPayNo = dma_search.get('syspayno');
		
		var empno = '';
		if(empNo != '') empno = empNo;
		else if(nm != '') empno = nm;
		//var data = { nm : nm, empNo : empNo , sPayNo : sPayNo , work_clsf : "HAG010" }
		var data = { empno : empno , work_clsf : "HAG010" }
		com.pop.open( com.pop.TYPE.HUM, data, function(ret){
			scwin.hum_popup_callback(ret);
		});
	};
	// 인사정보 팝업 호출 콜백
	scwin.hum_popup_callback = function(ret) {
		dma_search.set('nm'			, ret.nm);
		dma_search.set('empno'		, ret.empno);
		dma_search.set('syspayno'	, ret.syspayno);
	};
	
	// 사용자 엔터시
	scwin.ibx_nm_onkeyup = function(e) {
		if(e.keyCode == 13){
			if(ibx_nm.getValue() == '') return;
			dma_user.set('empno', ibx_nm.getValue());
			scwin.search_user();
		}else if((e.keyCode != 9) && (e.keyCode != 229)){
			dma_search.set('empno', '');
			dma_search.set('syspayno', '');
		}
	};
	
	// 인사정보 실행
	scwin.search_user = function() {
		dma_user.set('work_clsf', 'HAG010');
		com.execute(sbm_selectUserList, {
			successCallback : function(e) {
				dlt_user.setJSON(e.responseJSON.elData.humComUserSelVoList);
				scwin.search_user_after();
			}
		});
	};
	scwin.search_user_after = function(){
		var hum_cnt = dlt_user.getTotalRow();
		if(hum_cnt == 0){
			alert("조회정보가 없습니다!");
		}else if(hum_cnt == 1){
			dma_search.set('nm'			, dlt_user.getCellData(0, 'nm'));
			dma_search.set('empno'		, dlt_user.getCellData(0, 'empno'));
			dma_search.set('syspayno'	, dlt_user.getCellData(0, 'syspayno'));
		}else{ // hum_cnt >1
			scwin.hum_popup();
		}
	};
	
	// 부서정보 실행
	scwin.search_dept = function() {
		// SvcSUPTLTXODMS01.pwkjson | SptTltActivRptService.java
		// kr.re.kitech.biz.sup.tlt.selectSptTltXodhDeptList ( SptTltXodhDeptInfo_SQL_informix_MyBatis.xml )
		// AS-IS kitech.sup.tlt.xda.SptTltXodhDeptInfoMS01
		com.execute(sbm_selectDeptList, {
			successCallback : function(e) {
				dlt_dept.setJSON(e.responseJSON.elData.SptTltXodhDeptInfoVoList);
			}
		});
	};
	
	// 신규 버튼 클릭
	scwin.btn_req_onclick = function(e) {
		var data={openerId:'S676'};
		data.rpt_yrmon = dma_search.get('rpt_yrmon');
		data.mng_yn = 'Y';
		var tgId = 'S672';
		scwin.menu_move(data, tgId);
	};
	
	// 엑셀다운로드 버튼 클릭
	scwin.btn_down_excel_onclick = function(e) {
		var option = {
		    fileName : "활동보고서목록.xlsx"
		  , useDataFormat : true
		};
		com.downloadGridExcel(grd_rpt, option);
	};
	
	// 그리드 클릭 이벤트
	scwin.grd_rpt_oncellclick = function(row,col,colId) {
		if(colId == "req_no"){ // 보고서번호
			if(dlt_rpt.getCellData(row, 'chk_req_no') == 'N'){
				// 미작성 클릭시 return
				alert("신규작성 해주십시요!");
				return;
			}
			var data={openerId:'S675'};
			data.req_no = dlt_rpt.getCellData(row, "req_no");
			var tgId = 'S672';
			// 1005232 : ???? , 1010001 : 시스템관리자 A , 1010109 : 개발자공통
			if (/1005232|1010001|1010109/.test(com.getLoginInfo('roleCode'))) {
				tgId = 'S681';
			}
			scwin.menu_move(data, tgId);
		}else if(colId == "attach_file_no_img"){ // 첨부파일	
			// mode : download/view/edit
			var data = { fileKey : 'S672' , mode : 'download' , attach_file_no : dlt_rpt.getCellData(row, "attach_file_no") };
			com.pop.open( com.pop.TYPE.ATTACH, data);
		}
	};
	
	
	// 검색년월 데이터 변경시
	scwin.ica_rpt_yrmon_onviewchange = function(info) {
		scwin.search();
	};
	
	// 화면이동
	scwin.menu_move = function(data, tgId){
		com.openTabMenu(tgId, data, function(retObj) {
		//com.openTabUrl("활동보고서등록 " , "/ui/sup/tlt/SUP_TLT_"+tgId+"_00D.xml", 1, data, function(retObj) { // 승인
			if(retObj){
				scwin.search();	
			}
		});
	};
	
	scwin.btn_sms_email_onclick = function(e) {
		if(!scwin.btn_sms_email()) return;
		biz.sup.pop.open(biz.sup.pop.TYPE.TLT_MAIL, dlt_mail.getAllJSON(), function(ret){
		    if(ret){
				scwin.search();
		    }
		});
	};
	scwin.btn_sms_email = function() {
	// SUP_TLT_S676_01P
	// S672_01P
		dlt_mail.removeAll();
		
		var rows = grd_rpt.getCheckedIndex("chk");
		if(rows.length == 0){
			alert("이메일&문자를 보낼 행을 선택해 주십시오");
			return false;
		}
		var rowIndex = 0;
		rows.forEach(function(row) {
			var cust_chk = true;
			var cust_no = dlt_rpt.getCellData(row, 'cust_no');
			var cust_nm = dlt_rpt.getCellData(row, 'cmpy_nm_krchar');
			for(var i=0;i<dlt_mail.getRowCount();i++){
				if(dlt_mail.getCellData(i, 'cust_no') == cust_no) cust_chk = false;
			}
			if(cust_chk){
				dlt_mail.insertRow(rowIndex);
				dlt_mail.setCellData(rowIndex, 'cust_no'		, cust_no);
				dlt_mail.setCellData(rowIndex, 'cust_nm'		, cust_nm);
				//dlt_mail.setCellData(rowIndex, 'cn'				, '');
				//dlt_mail.setCellData(rowIndex, 'sn'				, '');
				//dlt_mail.setCellData(rowIndex, 'celphonenumber'	, celphonenumber);
				dlt_mail.setCellData(rowIndex, 'serviceId'		, 'S676');
				//dlt_mail.setCellData(rowIndex, 'email'			, '');
				rowIndex = rowIndex+1;
			}
		});
		
		return true;
	};
	
	
	
	
	
	
	
	
	
	
