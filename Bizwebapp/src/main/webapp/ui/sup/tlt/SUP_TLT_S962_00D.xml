<?xml version="1.0" encoding="UTF-8"?>
<html xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ev="http://www.w3.org/2001/xml-events"
    xmlns:w2="http://www.inswave.com/websquare" xmlns:xf="http://www.w3.org/2002/xforms">
    <head meta_programName="기업부담금 입금예정관리">
    	<w2:type>DEFAULT</w2:type>
    	<w2:buildDate />
    	<xf:model>
    		<xf:instance>
    			<data xmlns="" />
    		</xf:instance>
    		<w2:dataCollection baseNode="map">
    			<w2:dataMap baseNode="map" id="dma_search">
    				<w2:keyInfo>
    					<w2:key id="depst_mngmt_no" name="입금관리번호" dataType="text"></w2:key>
    					<w2:key id="cust_no" name="고객번호" dataType="text"></w2:key>
    					<w2:key id="mngmt_no" name="고객관리번호" dataType="text"></w2:key>
    				</w2:keyInfo>
    			</w2:dataMap>
    			<w2:dataMap baseNode="map" id="dma_detl">
    				<w2:keyInfo>
    					<w2:key id="depst_mngmt_no" name="입금관리번호" dataType="text"></w2:key>
    					<w2:key id="mngmt_no" name="관리번호" dataType="text"></w2:key>
    					<w2:key id="cust_no" name="고객번호" dataType="text"></w2:key>
    					<w2:key id="req_no" name="입금의뢰번호" dataType="text"></w2:key>
    					<w2:key id="unslip_no" name="결의번호" dataType="text"></w2:key>
    					<w2:key id="depst_clsf" name="입금예정구분" dataType="text"></w2:key>
    					<w2:key id="depst_schdl_amt" name="입금예정금액" dataType="text"></w2:key>
    					<w2:key id="depst_schdl_ymd" name="입금예정일" dataType="text"></w2:key>
    					<w2:key id="real_amt" name="실입금금액" dataType="text"></w2:key>
    					<w2:key id="rmk" name="비고" dataType="text"></w2:key>
    					<w2:key id="cmpy_nm_krchar" name="기업명" dataType="text"></w2:key>
    					<w2:key id="bsns_psn_regst_no" name="사업자번호" dataType="text"></w2:key>
    					<w2:key id="reprs_psn" name="대표자명" dataType="text"></w2:key>
    					<w2:key id="cmpy_post_no" name="우편번호" dataType="text"></w2:key>
    					<w2:key id="cmpy_addr" name="주소" dataType="text"></w2:key>
    					<w2:key id="objct_clsf_nm" name="대상분야" dataType="text"></w2:key>
    					<w2:key id="contrct_ymd" name="최초선정일자" dataType="text"></w2:key>
    					<w2:key id="attach_file_no" name="첨부파일" dataType="text"></w2:key>
    					<w2:key id="validAttachNo" name="첨부파일번" dataType="text"></w2:key>
    				</w2:keyInfo>
    			</w2:dataMap>
    			<w2:dataList baseNode="list" repeatNode="map" id="dlt_item" saveRemovedData="true">
    				<w2:columnInfo>
    					<w2:column id="depst_mngmt_no" name="입금관리번호" dataType="text"></w2:column>
    					<w2:column id="syspayno" name="시스템개인번호" dataType="text"></w2:column>
    					<w2:column id="nm" name="성명" dataType="text"></w2:column>
    					<w2:column id="posi" name="직위" dataType="text"></w2:column>
    					<w2:column id="send_clsf" name="파견현황" dataType="text"></w2:column>
    					<w2:column id="dispatch_term" name="파견계약기간" dataType="text"></w2:column>
    					<w2:column id="posi_an" name="년차" dataType="text"></w2:column>
    					<w2:column id="dispatch_start_ymd" name="파견계약(시작일)" dataType="text"></w2:column>
    					<w2:column id="dispatch_cls_ymd" name="파견계약(종료일)" dataType="text"></w2:column>
    					<w2:column id="salry_start_ymd" name="급여지급(시작일)" dataType="text"></w2:column>
    					<w2:column id="salry_cls_ymd" name="급여지급(종료일)" dataType="text"></w2:column>
    					<w2:column id="depst_schdl_amt" name="입금예정금액" dataType="text"></w2:column>
    					<w2:column id="cud_type" name="cud타입" dataType="text"></w2:column>
    					<w2:column id="chk" name="선택" dataType="text"></w2:column>
    				</w2:columnInfo>
    			</w2:dataList>
    			<w2:dataList baseNode="list" repeatNode="map" id="dlt_psn" saveRemovedData="true">
    				<w2:columnInfo>
    					<w2:column id="syspayno" name="시스템개인번호" dataType="text"></w2:column>
    					<w2:column id="nm" name="성명" dataType="text"></w2:column>
    					<w2:column id="posi" name="직위" dataType="text"></w2:column>
    					<w2:column id="send_clsf" name="파견현황" dataType="text"></w2:column>
    					<w2:column id="dispatch_term" name="파견계약기간" dataType="text"></w2:column>
    					<w2:column id="posi_an" name="년차" dataType="text"></w2:column>
    					<w2:column id="dispatch_start_ymd" name="파견계약(시작일)" dataType="text"></w2:column>
    					<w2:column id="dispatch_cls_ymd" name="파견계약(종료일)" dataType="text"></w2:column>
    				</w2:columnInfo>
    			</w2:dataList>
    			<w2:dataList baseNode="list" repeatNode="map" id="dlt_detls" saveRemovedData="true">
    				<w2:columnInfo>
    					<w2:column id="req_no" name="신청번호" dataType="text"></w2:column>
    					<w2:column id="accnt_no" name="계정번호" dataType="text"></w2:column>
    					<w2:column id="accnt_no_nm" name="계정명" dataType="text"></w2:column>
    					<w2:column id="bankaccnt_no" name="계좌번호" dataType="text"></w2:column>
    					<w2:column id="bank" name="은행" dataType="text"></w2:column>
    					<w2:column id="depst_amt" name="입금의뢰금액" dataType="text"></w2:column>
    					<w2:column id="bill_no" name="영수계산서번호" dataType="text"></w2:column>
    					<w2:column id="depst_ymd" name="입금일자" dataType="text"></w2:column>
    					<w2:column id="req_psn" name="신청자" dataType="text"></w2:column>
    					<w2:column id="req_psn_nm" name="신청자명" dataType="text"></w2:column>
    					<w2:column id="attach_file_no" name="첨부파일번호" dataType="text"></w2:column>
    					<w2:column id="unslip_no" name="정산결의번호" dataType="text"></w2:column>
    					<w2:column id="apr_state" name="결재상태" dataType="text"></w2:column>
    					<w2:column id="req_dept" name="신청부서" dataType="text"></w2:column>
    					<w2:column id="dept_nm" name="신청부서명" dataType="text"></w2:column>
    					<w2:column id="depst_mngmt_no" name="입금관리번호" dataType="text"></w2:column>
    					<w2:column id="state_nm" name="전자발행상태" dataType="text"></w2:column>
    					<w2:column id="rmk" name="비고" dataType="text"></w2:column>
    				</w2:columnInfo>
    			</w2:dataList>
    		</w2:dataCollection>
    		<w2:workflowCollection />
    		<!-- 조회 -->
    		<xf:submission id="sbm_selectData" ref='data:json,dma_search'
    			target='data:json,[{"id":"dma_detl","key":"depstVo"},{"id":"dlt_item","key":"itemList"},{"id":"dlt_detls","key":"detlsList"}]' action="SvcSUPTLTDEPSTSR01.pwkjson"
    			method="post" mediatype="application/json" encoding="UTF-8" mode="asynchronous">
    		</xf:submission>
    		<!-- 업체정보 조회 -->
    		<xf:submission id="sbm_selectCust" ref='data:json,dma_search' target='data:json,dma_detl' action="SvcSPTTLTDEPR01.pwkjson"
    			method="post" mediatype="application/json" encoding="UTF-8" mode="asynchronous">
    		</xf:submission>
    		<!-- 기업지원연구직 콤보박스 조회 -->
    		<xf:submission id="sbm_selectPsn" ref='data:json,dma_search' target='' action="SvcSPTTLTDEPR02.pwkjson" method="post"
    			mediatype="application/json" encoding="UTF-8" mode="asynchronous">
    		</xf:submission>
    		<!-- 저장 -->
    		<xf:submission id="sbm_saveData" ref='data:json,[{"id":"dma_detl","key":"depstVo"},{"id":"dlt_item","key":"itemList"}]'
    			target='data:json,dma_search' action="SvcSPTTLTDEPM01.pwkjson"
    			method="post" mediatype="application/json" encoding="UTF-8" mode="asynchronous">
    		</xf:submission>
    		<!-- 삭제 -->
    		<xf:submission id="sbm_saveData" ref='data:json,dma_search'
    			target='' action="SvcSPTTLTDEPD01.pwkjson"
    			method="post" mediatype="application/json" encoding="UTF-8" mode="asynchronous">
    		</xf:submission>
    		<!-- 결재신청 -->
    		<xf:submission id="sbm_saveApr" ref='data:json,dma_detl'
    			target='' action="SvcSPTTLTDEPM03.pwkjson"
    			method="post" mediatype="application/json" encoding="UTF-8" mode="asynchronous">
    		</xf:submission>
    	</xf:model>    	
    	<script type="text/javascript" lazy="false"><![CDATA[
	/****************************************************
 	 * 화면 설명 : 기업부담금 입금예정관리
 	 * 변경 이력 : 2023-12-13	이영희  최초 작성
 	 ****************************************************/  
 	scwin.saveFlag = false;
    /*
     * 페이지 열기
     */          
	scwin.onpageload = function() {
		// 첨부파일 컴포넌트 초기화
		com.file.init();
		com.file.create(upload_A.getID(), {fileKey: "S961", mode: 'edit'});	
		
		grp_depst.hide();
		
		// parameter
		dma_search.setJSON(com.getParameter());
		// enter key event
		com.setEnterKeyEvent(grp_vend, scwin.btn_vend_onclick);
		
		var codeOptions = [ { code : "STX", compID :"sbx_depst_clsf" }, // 입금예정구분 공통코드
							{ code : "STW", compID :"grd_item:send_clsf" }, // 파견현황
							{ code : "XAD", compID :"grd_depst:apr_state" } // 결재상태
			];
		com.setCmnCd(codeOptions, function() {
			if(dma_search.get("depst_mngmt_no") != ""){
				// 기업지원관리직 콤보박스 조회
				scwin.getAttendPsn("OnLoad");				
			}
		});
		
	};
	
	scwin.onpageunload = function() {
		
	};
	
	/*
     * 조회
     */
	scwin.search = function(){
		com.execute(sbm_selectData, {
			successCallback : function(e){
				grp_vend.setDisabled(true);
				
				grp_depst.show("");
				
				if(dma_detl.get("real_amt") > '0'){
					btn_save.setDisabled(true);	
					btn_del.setDisabled(true);	
				}
				
				if(dma_detl.get("attach_file_no") != ""){
					com.file.create(upload_A.getID(), {fileKey: dma_detl.get("attach_file_no"), mode: 'edit'});	
				}
			}
		});
	};
	
	/*
     * 기업지원관리직 조회
     */
	scwin.getAttendPsn = function(flag){
		com.execute(sbm_selectPsn, {
			successCallback : function(e){
				dlt_psn.setJSON(e.responseJSON.sptTltDepstItemVoList);
				
				if(flag == "OnLoad"){
					// 조회
					scwin.search();	
				}
			}
		});
	};
	
	/*
     * 저장전 체크
     */
	scwin.preSave = function(){
		if(dlt_item.getRowCount() == 0){
			alert("기업지원직 내역을 입력해 주시기바랍니다.");
			return false;
		}
		
		var validData=[
			{dataObj : dma_detl
			   , valInfoArr :[
				    { id : "cust_no", mandatory : true}
				   ,{ id : "depst_schdl_ymd", mandatory : true } 
				   ,{ id : "depst_schdl_amt", mandatory : true } 
				   ,{ id : "depst_clsf", mandatory : true } 
				]
			  , isCheckModified : false
			},
			{dataObj : dlt_item
			  , valInfoArr :[
				    { id : "syspayno", mandatory : true}
				   ,{ id : "depst_schdl_amt", mandatory : true, allowChar : "0-9" } 
			    ]
			   , isCheckModified : false
			}
		];
		
		if(!com.validateData(validData)) return false;
		
		return true;
	};
	
	//---------------------------------------------------------------- event --------------------------//
	/*
     * 기업검색 버튼 클릭 이벤트
     */
	scwin.btn_vend_onclick = function(e) {
		var data={
			vend_abbr : ibx_vend_nm.getValue(),
			ref_yn : "Y",
			biz_clsf : "SE"
		};
		
		com.pop.open(com.pop.TYPE.VEND, data, function(ret){
			if(ret){
				dma_search.set("cust_no", ret.vend_cd);
				
				com.execute(sbm_selectCust, {
					successCallback : function(e){
						dma_search.set("mngmt_no", dma_detl.get("mngmt_no"));
						
						scwin.getAttendPsn();
					}
				});
			}
		});
	};
	
	/*
     * 닫기 버튼 클릭 이벤트
     */
	scwin.btn_close_onclick = function(e) {		
		com.closePopup(scwin.saveFlag)
	};
	
	/*
     * 기업지원직 내역 추가 버튼 클릭 이벤트
     */
	scwin.btn_add_onclick = function(e) {
		var row = com.addRow(grd_item);
		dlt_item.setCellData(row, "cud_type", "C");
	};
	
	/*
     * 기업지원직 내역 삭제 버튼 클릭 이벤트
     */
	scwin.btn_minus_onclick = function(e) {
		var rows = grd_item.getCheckedIndex("chk");
		if(rows.length == 0){
			alert("삭제할 행을 선택하세요.");
			return;	
		}
		
		rows.reverse().forEach(function(row) {
			if(dlt_item.getRowStatus(row) != "C"){
				dlt_item.setCellData(row, "cud_type", "D");	
			}
			dlt_item.removeRow(row);
		});
		
	};
	
	/*
     * 기업지원직 내역 grid 변경 이벤트
     */
	scwin.grd_item_onviewchange = function(info) {
		var row = info.rowIndex;
		if(grd_item.getColumnID(info.colIndex) == "syspayno"){
			if(info.newValue != ""){
				var data = dlt_psn.getMatchedJSON("syspayno", info.newValue, true)[0];
				dlt_item.setRowJSON(row, data, true);
			}else{
				dlt_item.setRowData(row, ["N","","","","","","","",""], true);
			}
		}else if(grd_item.getColumnID(info.colIndex) == "depst_schdl_amt"){
			var amtSum = 0;
			for(var i=0; i<dlt_item.getRowCount(); i++){
				amtSum+= com.parseInt(dlt_item.getCellData(i, "depst_schdl_amt"));
			}
			
			ibx_depst_schdl_amt.setValue(amtSum);
		}
	};
	
	/*
     * 저장 버튼 클릭 이벤트
     */
	scwin.btn_save_onclick = function(e) {
		if(!scwin.preSave()) return;
		
		com.confirm("message.xom.wq.017"/*저장 하시겠습니까?*/, function(ret){
			if(ret){
				com.file.transport( function( attachObj){
					dma_detl.set("attach_file_no", attachObj.upload_A);
					dma_detl.set("validAttachNo", attachObj.upload_A);
					
					com.execute(sbm_saveData,{
						successCallback : function(e){
							com.alert("message.xom.wq.023"/*정상적으로 처리되었습니다.*/, function() {
								scwin.saveFlag = true;
								scwin.search();
							});
						}
					});
					
				});
			}
		});
	};
	
	/*
     * 입금예정구분 변경 이벤트
     */
	scwin.sbx_depst_clsf_onviewchange = function(info) {
		if(info.newValue == "" || info.newValue == "STX010"){
			ibx_unslip_no.setDisabled(true);
		}else{
			ibx_unslip_no.setDisabled(false);
		}
	};
	
	/*
     * 삭제 버튼 클릭 이벤트
     */
	scwin.btn_del_onclick = function(e) {
		if(dma_detl.get("real_amt") > '0'){
			alert("입금내역이 존재하므로 삭제할 수 없습니다.");
			return;	
		}
		
		if(dlt_detls.getCellData(0, "unslip_no") != ""){
			alert("지급결의번호가 존재하므로 삭제할 수 없습니다.");
			return;	
		}
		
		com.confirm("message.xom.wq.022"/*삭제 하시겠습니까?*/, function(ret) {
			if (ret) {
				com.file.deleteAll( function(){
					com.execute(sbm_deleteData, {
						successCallback : function(e) { // 성공 콜백
							com.alert("message.xom.wq.023"/*정상적으로 처리되었습니다.*/, function() {
								com.closePopup(true);
							});
						}
					});
				});
			}
		});
	};
	
	/*
     * 입금의뢰 버튼 클릭 이벤트
     */
	scwin.btn_req_onclick = function(e) {
		var data = {
			depst_clsf : dma_detl.get("depst_clsf"), // 입금예정구분
			vend_cd : dma_detl.get("cust_no"),
			vend_abbr : dma_detl.get("cmpy_nm_krchar"),
			depst_mngmt_no : dma_detl.get("depst_mngmt_no"),
			attach_file_no : dma_detl.get("attach_file_no"),
			totl_amt : dma_detl.get("depst_schdl_amt"),
			rmk : dma_detl.get("rmk"),
			biz_clsf : "ST",
			ref_yn : "Y"
		}; 
		
		com.pop.open(com.pop.TYPE.DEPST_REQ, data, function(ret){
			if(ret){
				scwin.search();	
			}
		});
	};
	
	/*
     * 목록 버튼 클릭 이벤트
     */
	scwin.btn_list_onclick = function(e) {
		com.moveList("S961");
	};
	
	/*
     * 결재신청 버튼 클릭 이벤트
     */
	scwin.btn_apr_onclick = function(e) {
		if(dlt_detls.getRowCount() == 0){
			alert("입금의뢰내역이 존재하지 않습니다.");
			return;	
		}
		
		for(var i=0; i<dlt_detls.getRowCount(); i++){
			if(dlt_detls.getCellData(i, "state_nm") != "국세청 전송완료"){
				alert("전자발행 후 결재신청할 수 있습니다.");
				return;	
			}	
		}
		
		dma_detl.set("req_no", dlt_detls.getCellData(0, "req_no"));
		dma_detl.set("unslip_no", dlt_detls.getCellData(0, "unslip_no"));
		
		com.execute(sbm_saveApr, {
			successCallback : function(e){
				com.movePageMenu("A001", {"reqNo" : dlt_detls.getCellData(0, "req_no")});			
			}
		});
	};
	
	scwin.grd_depst_oncellclick = function(row,col,colId) {
		if(colId == "req_no"){
			var data = {
				req_no : dlt_detls.getCellData(row, "req_no"), // 입금예정구분
				depst_clsf : dma_detl.get("depst_clsf"), // 입금예정구분
				totl_amt : dma_detl.get("depst_schdl_amt"),
				biz_clsf : "ST",
				ref_yn : "Y"
			}; 
			
			com.pop.open(com.pop.TYPE.DEPST_REQ, data, function(ret){
				if(ret){
					scwin.search();	
				}
			});	
