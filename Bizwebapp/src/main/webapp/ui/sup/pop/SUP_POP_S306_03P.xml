         <script type="text/javascript" lazy="false"><![CDATA[
	/****************************************************
 	 * 화면 설명 : 기술지원 SMS결제
 	 * 변경 이력 : 2023-07-04	이영희	최초 작성
 	 ****************************************************/      
 	scwin.data={};    
	scwin.onpageload = function() {
		// 거래취소 또는 환불요청시인데 구현이 되어있지 않음
		grp_cancel.hide();
		btn_del.hide();
		
		// parameter setting
		dma_appl.setJSON(com.getParameter());
		
		scwin.search();
	};
	
	scwin.onpageunload = function() {
		
	};	
	
	/**
	 * 조회
	 */
	scwin.search = function(){
		com.execute(sbm_selectData, {
			successCallback : function(e){				
				if(e.responseJSON.elData == undefined){
					return;
				}
				
				var data = e.responseJSON.elData;

				dma_appl.set("cno", data.cno);
				dma_appl.set("user_mail", data.user_mail);
				dma_appl.set("user_phone1", data.user_phone1);
				dma_appl.set("prcs_state", data.prcs_state);
				dma_appl.set("rcv_mobile_no", data.rcv_mobile_no);
				sbx_mgr_txtype.setValue("40"); // 즉시취소
				
				scwin.data.rcv_mobile_no = data.rcv_mobile_no;
				
				switch(data.prcs_state){
					case "STE056" : // SMS결제요청
						btn_save.setValue("SMS재발송");
						break;
					default :
						btn_save.setDisabled(true);
						break;
				}
			}
		});
	};	
	
	/**
	 * 파라미터 생성
	 */
	scwin.buildParam = function(epTrCd){
		// 공통요청정보
		dma_send.set("tr_cd", epTrCd); // 거래구분(수정불가)
		
		if(location.host == "nbiz.kitech.re.kr"){
			dma_send.set("mall_id", "05541950"); // 가맹점 ID(실ID)	
		}else{
			dma_send.set("mall_id", "T0009996"); // 가맹점 ID(test ID)	
		}
		dma_send.set("escrow_yn", "N"); // 에스크로여부(수정불가)
		dma_send.set("complex_yn", "N"); // 복합결제여부(수정불가)		
		
		var today = com.getServerDateTime("yyyyMMddHHmmss");
		// 가맹점 주문정보		
		dma_send.set("user_type", "1");   // 사용자구분(1 일반, 2 회원)
		dma_send.set("order_no", dma_appl.get("kolas_no"));  // 가맹점 주문번호
		dma_send.set("user_id", "USER_"+ today.substring(8,14));  // 가맹점 고객 ID
		dma_send.set("user_nm", dma_appl.get("cmpy_nm_krchar")); // 고객명 
		dma_send.set("user_mail", dma_appl.get("user_mail")); // 가맹점 고객 E-mail
		dma_send.set("user_phone1", dma_appl.get("user_phone1")); // 가맹점 고객 연락처1
		dma_send.set("user_addr", dma_appl.get("addr"));
		dma_send.set("product_type", "0");  // 상품구분 실물 0 컨텐츠 1
		dma_send.set("product_nm", dma_appl.get("kolas_no"));  // 상품명
		dma_send.set("product_amt", dma_appl.get("amount"));  // 상품금액
		
		// SMS발송 요청정보
		dma_send.set("reg_txtype", "51"); // SMS PHARM 발송 50 SMS PAY 발송 51
		dma_send.set("reg_subtype", "10"); // SMS 등록 10 SMS 재발송 110
		dma_send.set("tot_amt", dma_appl.get("amount"));  // 총 결제금액
		dma_send.set("currency", "00"); // 통화코드(00 : 원화)
		dma_send.set("pay_type", "11"); // 결제수단 신용카드 11 계좌이체 21 
		dma_send.set("cno", dma_appl.get("cno")); // PG거래번호
		
		
		dma_send.set("rcv_mobile_no", dma_appl.get("rcv_mobile_no")); //SMS수신 전화번호
		dma_send.set("snd_tel_no", "0415898114");  // SMS발신번호
		dma_send.set("snd_msg", "결제요청");  // SMS 문구	
		dma_send.set("mall_nm", "한국생산기술연");  // 상호명
		dma_send.set("member_no", "2");  // 회원사 시퀀스번호
		dma_send.set("own_cert_yn", "N");  // 본인인증여부
		dma_send.set("sms_pay_expr", today.substring(0,8) + "235959");  // 결제만료일시
	};
	
	//------------------------------------------------------------- event ---------------------------------//
	
	/**
	 * 닫기
	 */
	scwin.btn_close_onclick = function(e) {
		com.closePopup();
	};
	
	/**
	 * SMS결제 버튼 클릭 이벤트
	 */
	scwin.btn_save_onclick = function(e) {
		if(dma_appl.get("rcv_mobile_no") == ""){
			alert("SMS수신전화번호를 입력하세요.");
			return;
		}
		
		if(scwin.data.rcv_mobile_no == dma_appl.get("rcv_mobile_no")){
			if(dma_appl.get("prcs_state") == "STE056"){
				if(!confirm("발송한 정보가 있습니다. SMS 재발송하시겠습니까?")){
					dma_appl.set("cno", "");
				}
			}
		}else{
			dma_appl.set("cno", "");
		}
		
		scwin.buildParam("00301020");
		
		com.execute(sbm_saveData, {
			successCallback : function(e){
				if(e.responseJSON.elData.success == true){
					com.alert("message.xom.wq.023"/*정상적으로 처리되었습니다.*/, function() {
						com.closePopup({}); // 팝업 닫기 (콜백 수행을 위해 빈 객체 넘김)
					});
				}else{
					alert("실패하였습니다. 관리자에게 문의 바랍니다.");
				}
			}
		});
		
	};

	/**
	 *결제취소 버튼 클릭 이벤트
	 */
	scwin.btn_del_onclick = function(e) {
		scwin.buildParam("00201000");
		
		com.execute(sbm_saveData, {
			successCallback : function(e){
				if(dma_appl.get("success") == "true"){
					alert("실패하였습니다. 관리자에게 문의 바랍니다.");
				}else{
					com.alert("message.xom.wq.023"/*정상적으로 처리되었습니다.*/, function() {
						com.closePopup({}); // 팝업 닫기 (콜백 수행을 위해 빈 객체 넘김)
					});
				}
			}
		});
	};
	]]></script>
    </head>
