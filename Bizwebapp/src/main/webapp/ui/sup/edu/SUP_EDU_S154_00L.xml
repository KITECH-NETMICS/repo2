	scwin.search_user_after = function(){
		var gbn = dma_user.get('gbn');
		var hum_cnt = dlt_user.getTotalRow();
		if(hum_cnt == 0){
			alert("조회정보가 없습니다!");
		}else if(hum_cnt == 1){
			if(gbn == 'rspns'){
				ibx_rspns_nm.setValue(dlt_user.getCellData(0, 'nm'));
				ibx_rspns_empno.setValue(dlt_user.getCellData(0, 'empno'));
				dma_search.set('rspns_syspayno', dlt_user.getCellData(0, 'syspayno'));
			}else if(gbn == 'regst'){
				ibx_regst_nm.setValue(dlt_user.getCellData(0, 'nm'));
				ibx_regst_empno.setValue(dlt_user.getCellData(0, 'empno'));
				dma_search.set('regst_syspayno', dlt_user.getCellData(0, 'syspayno'));
			}
		}else{ // hum_cnt >1
			scwin.hum_popup(gbn);
		}
	}
	
