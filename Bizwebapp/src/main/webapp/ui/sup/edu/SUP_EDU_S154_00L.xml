         <script type="text/javascript" lazy="false"><![CDATA[
        
	scwin.onpageload = function() {
		var codeOptions = [ { code : "HCF", compID :"sbx_region_centr" } // 지역본부 공통코드
						,	{ code : "XAD", compID :"sbx_apr_state" } // 신청상태 공통코드
			];
		com.setCmnCd(codeOptions, function() {
			scwin.in_it();
		});
	};
	
	scwin.onpageunload = function() {
	};
	
	scwin.in_it = function(){
		com.setEnterKeyEvent(grp_search, scwin.search , [ibx_rspns_nm, ibx_regst_nm]);
		// 검색 영역 기본값 세팅
		var today = com.getServerDateTime();
		dma_search.set("educ_start_ymd_st", today.substring(0,6) + "01");
		dma_search.set("educ_start_ymd_ed", today);
		
		dma_search.set('biz_clsf','SEA020'); // SEA020 // 세미나
		dma_search.set('find_clsf','A');
        
		// 화면 로딩시 조회진행
		scwin.search();
	};
	
	
	/**
	 * 조회 버튼 클릭
	 */
	scwin.btn_search_onclick = function(e) {
		scwin.search();
	};
	
	/**
	 * 조회 실행
	 */
	scwin.search = function() {
		// debugger
		dma_search.set('find_clsf','A');
		// SvcSUPEDUSPTMR01.pwkjson | SptTechEducInfoService.java
		// kr.re.kitech.biz.sup.edu.selectSptTechEducInfoSmnrList ( SptTechEducInfo_SQL_informix_MyBatis.xml )
		// AS-IS kitech.sup.edu.xda.SptTechEducInfoMS01
		com.execute(sbm_selectSptTechEducList, {
			isImplExcel: true,
			successCallback : function(e) {
				dlt_smnr.setJSON(e.responseJSON.sptTechEducInfoVoList);
				tbx_result.setValue(dlt_smnr.getTotalRow());
				scwin.search_after();
			},
			excelCallback : function(e) {
				// DB 다운로드
				var option = {
					fileName : "기술세미나실적등록.xlsx"
		  			, useDataFormat : true
					, providerInfo : {
						  service : "sptTechEducInfoServiceImpl"
						, method : "selectSmnrList"
						, vo : "sup.edu.vo.SptTechEducInfoVo"
						, paramMap : dma_search
					}
				};
				com.downloadGridExcel(grd_smnr, option);
			},
		});
	};
	scwin.search_after = function(){
		for(var i=0;i<dlt_smnr.getTotalRow();i++){
			var attach_file_no_01 = dlt_smnr.getCellData(i, 'attach_file_no_01').trim();
			var attach_file_no_02 = dlt_smnr.getCellData(i, 'attach_file_no_02').trim();
			if(attach_file_no_01 == ''){
				grd_smnr.setCellDisabled(i, 'attach_file_no_01_img', true);
				grd_smnr.setCellReadOnly(i, 'attach_file_no_01_img', true);
			}else{
				grd_smnr.setCellDisabled(i, 'attach_file_no_01_img', false);
				grd_smnr.setCellReadOnly(i, 'attach_file_no_01_img', false);
			}
			if(attach_file_no_02 == ''){
				grd_smnr.setCellDisabled(i, 'attach_file_no_02_img', true);
				grd_smnr.setCellReadOnly(i, 'attach_file_no_02_img', true);
			}else{
				grd_smnr.setCellDisabled(i, 'attach_file_no_02_img', false);
				grd_smnr.setCellReadOnly(i, 'attach_file_no_02_img', false);
			}
		}
		if(dlt_smnr.getTotalRow() > 0){
			btn_down_excel.setDisabled(false);
		}else{
			btn_down_excel.setDisabled(true);
		}
	};
	
	// 책임자 돋보기 클릭
	scwin.btn_rspns_onclick = function(e) {
		// 인사정보 팝업 호출
		scwin.hum_popup('rspns');
	};
	
	// 등록자 돋보기 클릭
	scwin.btn_regst_onclick = function(e) {
		// 인사정보 팝업 호출
		scwin.hum_popup('regst');
	};
	
	// 인사정보 팝업 호출
	scwin.hum_popup = function(gbn) {
		var nm = '';
		var empNo = '';
		var sPayNo = '';
		if(gbn == 'rspns'){
			nm = ibx_rspns_nm.getValue();
			empNo = ibx_rspns_empno.getValue();
			sPayNo = dma_search.get('rspns_syspayno');
		}else if(gbn == 'regst'){
			nm = ibx_regst_nm.getValue();
			empNo = ibx_regst_empno.getValue();
			sPayNo = dma_search.get('regst_syspayno');
		}
		
		var empno = '';
		if(empNo != '') empno = empNo;
		else if(nm != '') empno = nm;
		//var data = { nm : nm, empNo : empNo , sPayNo : sPayNo , work_clsf : "HAG010" }
		var data = { empno : empno , work_clsf : "HAG010" }
		com.pop.open( com.pop.TYPE.HUM, data, function(ret){
			scwin.hum_popup_callback(ret, gbn);
		});
	};
	// 인사정보 팝업 호출 콜백
	scwin.hum_popup_callback = function(ret, gbn) {
		if(gbn == 'rspns'){
			ibx_rspns_nm.setValue(ret.nm);
			ibx_rspns_empno.setValue(ret.empno);
			dma_search.set('rspns_syspayno', ret.syspayno);
		}else if(gbn == 'regst'){
			ibx_regst_nm.setValue(ret.nm);
			ibx_regst_empno.setValue(ret.empno);
			dma_search.set('regst_syspayno', ret.syspayno);
		}
	};
	
	// 책임자 엔터시
	scwin.ibx_rspns_nm_onkeyup = function(e) {
		if(e.keyCode == 13){
			if(ibx_rspns_nm.getValue() == '') return;
			dma_user.set('gbn', 'rspns');
			dma_user.set('empno', ibx_rspns_nm.getValue());
			scwin.search_user();
		}else if((e.keyCode != 9) && (e.keyCode != 229)){
			ibx_rspns_empno.setValue('');
			dma_search.set('rspns_syspayno', '');
		}
	};
	
	// 등록자 엔터시
	scwin.ibx_regst_nm_onkeyup = function(e) {
		if(e.keyCode == 13){
			if(ibx_regst_nm.getValue() == '') return;
			dma_user.set('gbn', 'regst');
			//dma_user.set('empno', ibx_regst_nm.getValue());
			dma_user.set('empno', dma_search.get('regst_nm'));
			scwin.search_user();
		}else if((e.keyCode != 9) && (e.keyCode != 229)){
			ibx_regst_empno.setValue('');
			dma_search.set('regst_syspayno', '');
		}
	};
	
	/**
	 * 인사정보 실행
	 */
	scwin.search_user = function() {
		dma_user.set('work_clsf', 'HAG010');
		com.execute(sbm_selectUserList, {
			successCallback : function(e) {
				dlt_user.setJSON(e.responseJSON.elData.humComUserSelVoList);
				scwin.search_user_after();
