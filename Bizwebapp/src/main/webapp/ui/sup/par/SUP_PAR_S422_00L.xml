         <script type="text/javascript" lazy="false"><![CDATA[
        
	scwin.onpageload = function() {
		scwin.in_it();
	};
	
	scwin.onpageunload = function() {
	};
	
	scwin.in_it = function(){
		com.setEnterKeyEvent(grp_search, scwin.search , [ibx_regst_nm]);
		// 검색 영역 기본값 세팅
		var today = com.getServerDateTime();
		dma_search.set('eval_ymd_st',gcm.date.addMonth(today,-1));
		dma_search.set('eval_ymd_end',today);
        
		// 화면 로딩시 조회진행
		scwin.search();
	};
	
	
	/**
	 * 조회 버튼 클릭
	 */
	scwin.btn_search_onclick = function(e) {
		scwin.search();
	};
	
	/**
	 * 조회 실행
	 */
	scwin.search = function() {
		// SvcSUPPAREVALMR01.pwkjson | SptParInfoEvalService.java
		// kr.re.kitech.biz.sup.par.selectSptParInfoEvalList ( SptParInfoEval_SQL_informix_MyBatis.xml )
		// AS-IS kitech.sup.par.xda.SptParInfoEvalLstMS01
		com.execute(sbm_selectSptParEvalList, {
			isImplExcel: true,
			successCallback : function(e) {
				dlt_eval.setJSON(e.responseJSON.sptParInfoEvalVoList);
				tbx_result.setValue(dlt_eval.getTotalRow());
				//scwin.search_after();
			},
			excelCallback : function(e) {
				// DB 다운로드
				var option = {
					fileName : "파트너기업 유지심사서 리스트.xlsx"
					, useDataFormat : true
					, providerInfo : {
						  service : "sptParInfoEvalServiceImpl"
						, method : "selectSptParInfoEvalList"
						, vo : "sup.par.vo.SptParInfoEvalVo"
						, paramMap : dma_search
					}
				};
				com.downloadGridExcel(grd_eval, option);
			},
		});
	};
	
	// 작성자 엔터시
	scwin.ibx_regst_nm_onkeyup = function(e) {
		if(e.keyCode == 13){
			if(ibx_regst_nm.getValue() == '') return;
			dma_user.set('empno', dma_search.get('regst_nm'));
			scwin.search_user();
		}else if((e.keyCode != 9) && (e.keyCode != 229)){
			ibx_regst_empno.setValue('');
			dma_search.set('regst_syspayno', '');
		}
	};
	
	// 작성자 돋보기 클릭
	scwin.btn_regst_onclick = function(e) {
		// 인사정보 팝업 호출
		scwin.hum_popup();
		
	};
	
	/**
	 * 인사정보 실행
	 */
	scwin.search_user = function() {
		dma_user.set('work_clsf', 'HAG010');
		com.execute(sbm_selectUserList, {
			successCallback : function(e) {
				dlt_user.setJSON(e.responseJSON.elData.humComUserSelVoList);
				scwin.search_user_after();
			}
		});
	};
	scwin.search_user_after = function(){
		var hum_cnt = dlt_user.getTotalRow();
		if(hum_cnt == 0){
			alert("조회정보가 없습니다!");
		}else if(hum_cnt == 1){
			ibx_regst_nm.setValue(dlt_user.getCellData(0, 'nm'));
			ibx_regst_empno.setValue(dlt_user.getCellData(0, 'empno'));
			dma_search.set('regst_syspayno', dlt_user.getCellData(0, 'syspayno'));
		}else{ // hum_cnt >1
			scwin.hum_popup();
		}
	}
	
	// 인사정보 팝업 호출
	scwin.hum_popup = function() {
		var nm = '';
		var empNo = '';
		var sPayNo = '';
		nm = ibx_regst_nm.getValue();
		empNo = ibx_regst_empno.getValue();
		sPayNo = dma_search.get('regst_syspayno');
		
		var empno = '';
		if(empNo != '') empno = empNo;
		else if(nm != '') empno = nm;
		//var data = { nm : nm, empNo : empNo , sPayNo : sPayNo , work_clsf : "HAG010" }
		var data = { empno : empno , work_clsf : "HAG010" }
		com.pop.open( com.pop.TYPE.HUM, data, function(ret){
			scwin.hum_popup_callback(ret);
		});
	};
	// 인사정보 팝업 호출 콜백
	scwin.hum_popup_callback = function(ret) {
		ibx_regst_nm.setValue(ret.nm);
		ibx_regst_empno.setValue(ret.empno);
		dma_search.set('regst_syspayno', ret.syspayno);
	};
	
	// 엑셀다운로드 버튼 클릭
	scwin.btn_down_excel_onclick = function(e) {
		var option = {
		    fileName : "파트너기업 유지심사서 리스트.xlsx"
		  , useDataFormat : true
		};
		com.downloadGridExcel(grd_eval, option);
	};
	
	// 신규 버튼 클릭
	scwin.btn_req_onclick = function(e) {
		var data={openerId:'S422'};
		scwin.menu_move(data);
	};
	
	// 그리드 클릭
	scwin.grd_eval_oncellclick = function(row,col,colId) {
		if(colId == 'par_eval_req_no'){ // 유지심사서 번호
			var data={openerId:'S422'};
			data.req_no = dlt_eval.getCellData(row, "par_eval_req_no");
			scwin.menu_move(data);
		}
	};
	
	// 화면이동
	scwin.menu_move = function(data){
		//com.openTabUrl("파트너기업유지심사등록" , "/ui/sup/par/SUP_PAR_S421_00D.xml", 1, data, function(retObj) {
		com.openTabMenu("S421", data, function(retObj) {
			if(retObj){
				scwin.search();	
			}
		});
	};
	]]></script>
