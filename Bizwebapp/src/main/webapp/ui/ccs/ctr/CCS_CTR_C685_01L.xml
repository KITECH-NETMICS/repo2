         <script type="text/javascript" lazy="false"><![CDATA[
     /****************************************************
 	 * 화면 설명 : 대외활동신고관리
 	 * 변경 이력 : 2022-05-19   박영훈	최초 작성
 	            2023-11-17 이영희 변경(겸직, 타업종사, 외부강의 기타활동)
 	 * serviceId : C685
 	 ****************************************************/
 	 /*
     * 페이지 열기
     */ 
	scwin.onpageload = function() {
		var codeOptions = [ { code : "CCR", compID :"sbx_report_cd,grd_list:report_cd"},
							{ code : "CLA", compID :"sbx_act_clsf,grd_list:act_clsf"},
							{ code : "CCR", compID :"grd_list:report_cd"},
							{ code : "CTR", compID :"grd_list:trip_cd"},
							{ code : "XAD", compID :"grd_list:apr_state"},
							{ code : "HCN", compID :"grd_list:attnce_clsf"},
							{ code : "HCQ", compID :"grd_list:econo_gain_clsf"}
							];
		
		com.setCmnCd(codeOptions, function() {
			var today = com.getServerDateTime();
			
			st_daytm_sch.setValue(today.substring(0, 6));
			ed_daytm_sch.setValue(today.substring(0, 6));
			st_req_daytm_sch.setValue(com.addDate(today, -30));
			ed_req_daytm_sch.setValue(today);
			
			scwin.search();
		});
		
	};
	
	scwin.onpageunload = function() {
		
	};
	
	/*
     * 조회
     */
	scwin.search = function() {
		com.execute(sbm_selectTripList, {
			successCallback : function(e) {
				dlt_grd.setJSON(e.responseJSON.ctrTripInviteVoList);
				tbx_result.setValue(dlt_grd.getTotalRow());
				
			}
		});
	}
	
	/*
     * 조회 버튼 클릭 이벤트
     */
	scwin.btn_search_list_onclick = function(e) {
		scwin.search();
	};
	
	
	
	//날짜구하기
	function getDateStr(date) {
	    var sYear = date.getFullYear();
	    var sMonth = date.getMonth() + 1;
	    var sDate = date.getDate();
	
	    sMonth = sMonth > 9 ? sMonth : "0" + sMonth;
	    sDate  = sDate > 9 ? sDate : "0" + sDate;
	    return String(sYear) + String(sMonth) + String(sDate);
	};
	
	scwin.btn_emp_onclick = function(e) {
		var data={empno : inputreqPsnEmpNo.getValue()};
		
		com.pop.open( com.pop.TYPE.HUM, data, function(ret){
			dma_search.set("empno", ret.empno);
			dma_search.set("req_psn_nm", ret.nm);
			dma_search.set("syspayno", ret.syspayno);
		});
		
	};
	
	scwin.btn_down_onclick = function(e) {
		var option = {fileName : "외부강의신고서.xlsx", hiddenVisible : "true"};
		com.downloadGridExcel(grd_list, option);
	};
	
	scwin.btn_insert_onclick = function(e) {
		scwin.openDetail();
	};
	
	scwin.openDetail = function(data) {
		com.openTabMenu("C680", data, function(retObj) {
			// 콜백
			if(retObj){
				scwin.search();	
			}
		});	
	};
	
		
	scwin.openPop = function(data, row) {
		data.empno = dlt_grd.getCellData(row, "empno");
		data.req_daytm = dlt_grd.getCellData(row, "req_daytm").substring(0, 10);
		
		biz.ccs.pop.open(biz.ccs.pop.TYPE.CVN_MOD_HIS, data, function(ret) {
		});
	};
	
	
	scwin.grd_list_oncellclick = function(row,col,colId) {
		var data = { req_no : dlt_grd.getCellData(row, "req_no"),
					 syspayno : dlt_grd.getCellData(row, "syspayno")
				   };
		if(colId == "req_no"){
			scwin.openDetail(data);
		} else if (colId == "btn_modify"){
			scwin.openPop(data, row);
		}
		
	};
	
	
	scwin.ibx_psn_empno_onkeyup = function(e) {
		if(e.keyCode == "13"){
			btn_emp.trigger("click");
		}
	};
	
	/**
	* 활동유형 변경 이벤트
	*/
	scwin.sbx_activity_cd_onviewchange = function(info) {
		if(sbx_act_clsf.getValue() == ""){
			alert("활동구분을 먼저 선택하세요.");
			return;	
		}
	};
	
	/**
	* 활동구분 변경 이벤트
	*/
	scwin.sbx_act_clsf_onviewchange = function(info) {
		if(info.newValue != ""){
			var codeClsf ="";
			if(this.getValue() == "CLA001"){ // 겸직
				codeClsf = "CCB";
			}else if(this.getValue() == "CLA002"){ // 타업종사
				codeClsf = "CCE";
			}else if(this.getValue() == "CLA003"){ // 외부강의 등
				codeClsf = "CCA";
			}else if(this.getValue() == "CLA004"){ // 기타활동
				codeClsf = "CCF";
			}
			
			var codeOptions = [ { code : codeClsf, compID :"sbx_activity_cd"}];
			com.setCmnCd(codeOptions);
		}
	};
	]]></script>
