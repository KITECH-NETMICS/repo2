        <script type="text/javascript" lazy="false"><![CDATA[
	/****************************************************
	 * 화면 설명 : 직인인감날인관리
	 * 변경 이력 : 2023-08-07	lsh	최초 작성
	 * serviceId : C209
	 ****************************************************/

	scwin.onpageload = function() {
		
		// 검색조건 영역내의 컴포넌트에 엔터키 이벤트 발생시 조회 수행
		com.setEnterKeyEvent(grp_search, scwin.btn_search_onclick);
		com.setEnterKeyEvent(grd_signUse, scwin.btn_search_onclick);
		
		var cmd = com.getParameter("cmd");
		var strDate1 = com.getParameter("strDate1");
		var strDate2 = com.getParameter("strDate2");
		var sealClsf = com.getParameter("sealClsf");
		var reqState = com.getParameter("reqState");
		var strToday = com.getServerDateTime();
		
		// 기간역전 체크
		com.checkPeriod(inputStrDate, inputEndDate);
		
		var codeOptions = [{ code : "CGA", compID :"selectboxSealClsfCd" } 
		];
		com.setCmnCd(codeOptions, function() {
			// 공통코드 셋팅 콜백
			// 초기값을 셋팅하거나 자동 조회되도록 구현하려면 이곳에서 조회 submission 수행
		});
		
		if (cmd == "Vw") {
			if (strDate1 != "" || strDate2 != "") {
				inputStrDate.setValue(strDate1);
				inputEndDate.setValue(strDate2);
			} else {
				inputStrDate.setValue(com.addDate(strToday, -7)); // 7일전
				inputEndDate.setValue(strToday);
			}
			selectboxSealClsfCd.setValue(sealClsf);
			selectboxReqStateCd.setValue(reqState);
			scwin.search();
		} else {
			inputStrDate.setValue(com.addDate(strToday, -7)); // 7일전
			inputEndDate.setValue(strToday);
		}
	};
	
	scwin.onpageunload = function() {
		
	};
	
	scwin.search = function () {
		if(!scwin.validation()) return;
		
		com.execute(sbm_searchList, {
			successCallback : function(e) {
				tbx_result.setValue(dlt_list.getTotalRow());
			}
		});
	}
	
	scwin.save = function() {
		com.execute(sbm_updateList, {
			successCallback : function(e) { // 성공 콜백
				com.alert("message.xom.wq.023"/*정상적으로 처리되었습니다.*/, function() {
					scwin.search();	
				});
			}
		});
	}
	
	/**
	 * 필수값 체크
	 */
	scwin.validation = function() {
		return com.validateData(dma_search, [
				{ id : "req_ymd1", mandatory : true},
				{ id : "req_ymd2", mandatory : true}
			], false);
	}
	
	/**
	 * 화면이동
	 */
	scwin.openDetail = function(type, row) {
		var data={};
		
		if(type == "U"){
			data.req_no = dlt_list.getCellData(row, "req_no");
			data.req_state = dlt_list.getCellData(row, "req_state");	// 결재상태
			data.reqSyspayno = dlt_list.getCellData(row, "req_psn_syspayno")
			data.menuID = "C209"; // 현재 화면의 서비스 아이디
		}
		
		com.openTabMenu("C207", data, function(retObj) {
			// 콜백
			if(retObj){
				// 상세화면에서 이동시 실행되는 영역
				scwin.search();	
			}
		})	
	};
	
	/**
	 * 사용자검색 팝업 실행
	 */
	scwin.openHumUserPop = function(rowIndex){
		var vEmpno = grd_signUse.getCellDisplayData(rowIndex, "stamp_psn_nm");
		
		var data = { empno : vEmpno, work_clsf : "HAG010" };
		
		com.pop.open(com.pop.TYPE.HUM, data, function(ret){
			dlt_list.setCellData(rowIndex, dlt_list.getColumnIndex("stamp_psn_syspayno"), ret.syspayno);
			dlt_list.setCellData(rowIndex, dlt_list.getColumnIndex("stamp_psn_nm"), ret.nm);
		});
	}
	
	/**
	 * 조회 버튼 클릭 이벤트
	 */
	scwin.btn_search_onclick = function(e) {
		scwin.search();	
	};
	
	/**
	 * 엑셀다운로드 버튼 클릭 이벤트
	 */
	scwin.btn_down_excel_onclick = function(e) {
		if(dlt_list.getRowCount() > 0){
			var option = {
				    fileName : "인장사용관리.xls"
				  , removeColumns : "0,12, 13,15,16,17"
				  , useDataFormat : true
			};
			com.downloadGridExcel(grd_signUse, option);
		}else{
			com.alert("조회된 데이터가 없습니다.");
		}
		
	};
	
	/**
	 * 그리드 셀 클릭 이벤트
	 */
	scwin.grd_signUse_oncellclick = function(row,col,colId) {
		if (colId == "req_ymd"){
			var gubun = dlt_list.getCellData(row, "gubun");
			
			if("apr" == gubun){
				alert("[안내] 결재시스템의 외부시행문 참조정보입니다.\n 상세내역은 결재시스템에서 확인하십시요.");
				return;
			}else if("res" == gubun){
				alert("[안내] 사업비 청구서 참조정보입니다.\n 상세내역은 결재시스템에서 확인하십시요.");
				return;
			}else{
				scwin.openDetail("U", row);
			}
		}
		
		if(colId == "user_pop"){
			var vGubun = dlt_list.getCellData(row, "gubun");
			//var empNo = dlt_list.getCellData(row, "stamp_psn_syspayno");
			
			if(vGubun == "apr"){
				alert("[안내] 결재시스템의 외부시행문 참조정보입니다.\n 날인자를 변경 할 수 없습니다.");
			}else if(vGubun == "res"){
				alert("[안내] 사업비 청구서 참조정보입니다.\n 날인자를 변경 할 수 없습니다.");
			}else{
				scwin.openHumUserPop(row);
			}
		}
	};
	
	/**
	 * 저장버튼 클릭 이벤트
	 */
	scwin.trigger_save_onclick = function(e) {
		// 리스트 초기화
		dlt_updateList.removeAll();
		
		var idxArr = dlt_list.getRowIndexByStatus("U"); // 수정한 행의 인덱스를 배열로 받음
		
		for (var i = 0; i < idxArr.length; i++) {
			var rowData = dlt_list.getRowArray(idxArr[i]); // 변경된 행의 데이터를 JSON형식으로 가져온다
			
			dlt_updateList.setRowArray(i, rowData, false) // 수정할 리스트에 변경된 행의 데이터를 셋팅
		}
		
		if(dlt_updateList.getRowCount() > 0){
			scwin.save();
		}
	};
	
	/**
	 * 그리드 onkeyup 이벤트 - 날인자 컬럼에 엔터키 입력시 팝업검색
	 */	
	scwin.grd_signUse_onkeyup = function(e) {
		var colId = grd_signUse.getFocusedColumnID();
		var row = grd_signUse.getFocusedRowIndex();
		
		if (e.keyCode === 13) {
			if(colId == "stamp_psn_nm"){
				var vGubun = dlt_list.getCellData(row, "gubun");
				
				if(vGubun == "apr"){
					alert("[안내] 결재시스템의 외부시행문 참조정보입니다.\n 날인자를 변경 할 수 없습니다.");
				}else if(vGubun == "res"){
					alert("[안내] 사업비 청구서 참조정보입니다.\n 날인자를 변경 할 수 없습니다.");
				}else{
					scwin.openHumUserPop(row);
				}
			}
		}

	};
	]]></script>
