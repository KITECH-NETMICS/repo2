    	<script type="text/javascript" lazy="false"><![CDATA[	/**
	 * 	IP발급/변경 신청서
	 * 
	 * @Author Seongmin Hong
	 * @개발시작일 2023.07.11
	 * @TODO	부서 내용을 코드로 적을지 OR 이름으로 적을지 고민해야함
	 * 
	 * 변경내역
	 * --------------------------------
	 * 날짜			수정자			내용
	 * --------------------------------
	 * 2023.07.11	홍성민			최초저장
	 */
	scwin.onpageload = async function() {
		let params = com.getParameter();
		params.aply_no = "X912022001EB";
		params.aply_clsf = "1";

		await scwin.afterLoad();
		if (com.isEmpty(params.aply_no)) {
			await scwin.selectHumInfo(com.getLoginInfo("empno"));
			com.addRow(grd_usr);
		} else {
			dma_search.setJSON(params);
			scwin.selectData();
		}
	};
	
	/*******************************************************************************
	 * 
	 * 							사용자 함수
	 * 
	 *******************************************************************************/

	// 페이지 로드 후 초기 실행
	scwin.afterLoad = async function() {
		com.file.init();

		$("#" + grp_ipReq_guide.getID()).hover(
			null,
			function(){
				$("#" + grp_ipReq_guide.getID()).hide();
			}
		);
		var codeOptions = [ 
							{ code : "XBA", compID : "grd_usr:opr_sys"},
							{ code : "XBE", compID : "grd_usr:aset_type"},
							{ code : "XZA", compID : "grd_usr:kind_cd"} 
						];
		await com.setCmnCd(codeOptions);
	};

	scwin.selectData = async function(req_no) {
		if (com.isEmpty(req_no) && com.isEmpty(dma_search.get("aply_no"))) {
			await scwin.selectHumInfo(com.getLoginInfo("empno"));
			com.addRow(grd_usr);
		}

		await com.execute(sbm_selectIPReq);
		scwin.selectHumInfo(dma_reqInfo.get("aply_empno"));
	};

	scwin.selectHumInfo = async function(empno) {
		let data = {
			empno : empno
		}
		let retJson = await scwin.openPopupByColNm("use_syspaynm", data);
		let uid = retJson.email.substring(0, retJson.email.indexOf("@"));

		ibx_divsnDeptNm.setValue(retJson.divsn_dept_nm);
		ibx_deptNm.setValue(retJson.dept_level_4_nm || retJson.dept_nm);
		ibx_oficNm.setValue(retJson.dept_level_4_nm ? retJson.dept_nm : "");
		ibx_telNo.setValue(retJson.ext_no);
		ibx_empNo.setValue(retJson.empno);
		ibx_nm.setValue(retJson.nm);
		ibx_userId.setValue(uid);
		ibx_posiNm.setValue(retJson.posi_nm);
	};

	scwin.openPopupByColNm = async function(colNm, data, isGrid, row) {
		if (com.isEmpty(colNm)) return false;

		let retData = {}
		if (colNm.includes("use_syspaynm")) {
			retData = await com.pop.open(com.pop.TYPE.HUM, data) || {};
		} else if (colNm.includes("aset_no")) {
			retData = await com.pop.open(com.pop.TYPE.ASSPOP, data) || {};
		}

		if (isGrid && Object.keys(retData).length > 0) {
			if (colNm.includes("use_syspaynm")) {
				let use_nm_full = `${retData.nm} ${retData.posi_nm} ${retData.nm_eng}`;
				
				dlt_asset.setCellData(row, "use_syspaynm", retData.nm);
				dlt_asset.setCellData(row, "use_empno", retData.empno);
				dlt_asset.setCellData(row, "dept_nm", retData.dept_nm);
				dlt_asset.setCellData(row, "mobile", retData.mobile);
				dlt_asset.setCellData(row, "use_nm_full", use_nm_full);
				dlt_asset.setCellData(row, "email", retData.email);
			} else if (colNm.includes("aset_no")) {
				dlt_asset.setCellData(row, "aset_no", retData.aset_no);
			}
		}

		return retData;
	};

	/*******************************************************************************
	 * 
	 * 							이벤트 함수
	 * 
	 *******************************************************************************/

	scwin.btn_addPer_onclick = function(e) {
		com.addRow(grd_usr);
	};

	scwin.btn_delPer_onclick = function(e) {

	};

	scwin.lbl_warn_onmouseover = function(e) {
		if (grp_ipReq_guide.getStyleValue("display") === "none") {
			grp_ipReq_guide.setPosition(lbl_warn.getPosition("left"), lbl_warn.getPosition("top"));
			grp_ipReq_guide.show("");
		}
	};

	scwin.grd_usr_ontextimageclick = function(row,col) {
		let colNm = grd_usr.getColumnID(col);
		let data = null;

		if (colNm == "aset_no") {
			data = { aset_no : dlt_asset.getCellData(row, colNm), biz_clsf : "B"};
		} else {
			data = { empno : ""};
		}

		scwin.openPopupByColNm(colNm, data, true, row);
	};

	scwin.grd_usr_oneditkeyup = function(info) {
		if (info.keyCode == 13) {
			let colNm = info.colID;
			let row = info.rowIndex;
			let data = null;

			if (colNm == "aset_no") {
				data = { aset_no : dlt_asset.getCellData(row, colNm), biz_clsf : "B" };
			} else {
				data = { empno : dlt_asset.getCellData(row, colNm), work_clsf : "HAG010" }
			}

			scwin.openPopupByColNm(colNm, data, true, row);
		}
	};
]]></script>
