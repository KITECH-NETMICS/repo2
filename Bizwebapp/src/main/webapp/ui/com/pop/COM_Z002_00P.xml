        <script type="text/javascript" lazy="false"><![CDATA[
	/****************************************************
 	 * 화면 설명 : 인사정보 조회 공통팝업
 	 * 변경 이력 : 2022-03-21	박병목	최초 작성
 	            2023-12-13	이영희 	occutyp_cd 추가(기업부담금관리 기업지원연구직만 조회)
 	 ****************************************************/
	scwin.data={
		flag : false
	};
	/**
	 * 페이지 로딩 이벤트
	 */
	scwin.onpageload = function() {
		// 검색조건 영역내의 컴포넌트에 엔터키 이벤트 발생시 조회 수행
		com.setEnterKeyEvent(grp_search, scwin.btn_search_onclick);
		var param = com.getParameter();
		dma_user.setJSON(param);
		// TO-DO 결재정보에서는 사용자 GRID 영역을 숨기는 기능 필요
		//dma_user.set("empno", "170701");
		if (param.aprMode) {
			grp_search.hide("");
			grd_lybox.hide("");
			btn_schhum.hide("");
			com.execute(sbm_selectUserList, {
			successCallback : function(e) {
				var totalCnt = e.responseJSON.elData.totalCount;
				if (totalCnt == 1) {	// 단건일경우에만 세팅
					grd_user_info.setFocusedCell(0,0);
					personImage.show("");
					personImage.setSrc("http://biz.kitech.re.kr/photoimages/"+dlt_user.getDataRow(0).syspayno+".jpg");
				
					var today = gcm.date.getServerDateTime();
					dma_userattend.set("syspayno", dlt_user.getDataRow(0).syspayno);
					dma_userattend.set("biztrip_from_ymd", com.addDate(today, -7));
					dma_userattend.set("biztrip_to_ymd", today);
					com.execute(sbm_attend, {
					successCallback : function(e) {
						scwin.setData(today);
					}
					});
				}
				
			}
			});
		} else {
			com.execute(sbm_dept, {
				successCallback : function(e) {
					if (dma_user.get("empno") != "") {	// 초기값이 있을경우 부서조회 후 바로 사용자 검색
						com.execute(sbm_selectUserList);
					}
					
				}
			});
			
		}
		var codeOptions = [ { code : "HAG", compID :"hag_a, grd_user_info:work_clsf" } //useEx이 'N'인 항목은 재선택 불가
		];
		com.setCmnCd(codeOptions, function() {
			// 공통코드 셋팅 콜백에서 초기값 셋팅
			// 화면을 열때 자동조회되도록 구현하려면 이곳에서 조회 submission 수행
			dma_user.setJSON( { work_clsf:"HAG010"});
		});
	};

	/**
	 * 조회버튼 클릭
	 */
	scwin.btn_search_onclick = function() {
		if (inpEmpno.getValue() == "" && inpDept.getValue() == "") {
			com.alert("message.alert.com.pop.0001");	// 사번/이름 또는 부서명이 존재 해야 합니다.
			return false;
		}
		personImage.hide("");
		dma_user.set("dept_cd", "");
		scwin.search();
	};

	/**
	 * 조회
	 */
	scwin.search = function() {
		// 목록 조회
		com.execute(sbm_selectUserList, {
			successCallback : function(e) {
				var totalCnt = e.responseJSON.elData.totalCount;
				//grd_user_info.setFocusedCell(0,0);
			}
		});
	};

	
	scwin.trv_dept_onlabelclick = function(value,nodeElement,index) {
		var executeMoad = true;
		for (var i=0; i < nodeElement.childNodes.length; i++) {
			if (nodeElement.childNodes[i].nodeName == "map") {
				executeMoad = false;	 
			}
		}
		if (executeMoad) {
			work_con.setValue("");
			personImage.hide("");
			inpEmpno.setValue("");
			inpDept.setValue("");
			dma_user.set("dept_cd", value);
			com.execute(sbm_selectUserList);
		}
	};
	
	scwin.setData = function(today) {
		var contents = "";
		for (var i = 0; i < dlt_userattend.getRowCount(); i++) {
			contents = contents + dlt_userattend.getDataRow(i).ctrl_content + "\n";
		}
		content_work.setValue(contents);
		var workTypTitle = "근태현황<br>"+gcm.date.getServerDateTime("yyyy-MM-dd")+"<br>("+com.getDay(today)+")";  //center 정렬
		work_con.setValue(workTypTitle);
	}; 
	
	scwin.btn_schhum_onclick = function(e) {
		// 리턴값을 전달하면 콜백 함수 실행 후, 팝업창을 닫는다.
		scwin.setHumUser(grd_user_info.getFocusedRowIndex());
	};
	
	
	/**
	 * 선택한 행의 데이터 넘겨주기
	 */
	scwin.setHumUser = function(row){
		com.closePopup(dlt_user.getRowJSON(row));
	}
	
	scwin.btn_close_onclick = function(e) {
		com.closePopup();
	};
	
	scwin.setUserInfo = function(row){
		if(scwin.data.flag) return;
		
		var today = gcm.date.getServerDateTime();
		personImage.show("");
		personImage.setSrc("http://biz.kitech.re.kr/photoimages/"+dlt_user.getCellData(row, "syspayno")+".jpg");
		dma_userattend.set("syspayno", dlt_user.getCellData(row, "syspayno"));
		dma_userattend.set("biztrip_from_ymd", com.addDate(today, -7));
		dma_userattend.set("biztrip_to_ymd", today);
		if (dma_userattend.get("syspayno") != "") {
			com.execute(sbm_attend, {
			successCallback : function(e) {
				scwin.setData(today);
			}
			});
		} else {
			work_con.setValue("근태현황");
			content_work.setValue("");
		}
	};	
	
	scwin.grd_user_info_oncellclick = function(row,col,colId) {
		setTimeout(function(){
			scwin.setUserInfo(row);
		}, 300);
	};
	
	scwin.grd_user_info_oncelldblclick = function(row,col,colId) {
		scwin.data.flag = true;
		scwin.setHumUser(row);
	};
	
	scwin.btn_fold_onclick = function() {
		trv_dept.spanAll(true);
	};
	
	scwin.btn_expand_onclick = function() {
		trv_dept.spanAll(false);
	};
	]]></script>
    </head>
