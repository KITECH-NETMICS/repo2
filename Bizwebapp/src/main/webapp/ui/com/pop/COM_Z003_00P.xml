		<script type="text/javascript" lazy="false"><![CDATA[
    /****************************************************
 	 * 화면 설명 : 과제정보
 	 * 변경 이력 : 2022-04-01	박병목	최초 작성
 	 ****************************************************/

	/**
	 * 페이지 로딩 이벤트
	 */   
	scwin.onpageload = function() {
		var accntNo = (com.isEmpty(com.getParameter("accnt_no")))? WebSquare.net.getParameter("accntNo") : com.getParameter("accnt_no");		
		dma_search.set("accnt_no", accntNo);
		
		com.setEnterKeyEvent(grp_accnt_no, scwin.btn_accntSch_onclick);
		com.setEnterKeyEvent(grp_psn, scwin.btn_emp_onclick);
		
		// 공통코드
		var codeOptions = [ { code : "RHA", compID :"sbx_bsns_cd_inout" }
		                   ,{ code : "RBA", compID :"sbx_prj_state" }
		                ];
		com.setCmnCd(codeOptions, function() {
			if(dma_search.get("accnt_no").length == 8){
				scwin.search();
			}
		});		
	};
	
	scwin.onpageunload = function() {
		
	};
	
	/**
	 * 조회버튼 클릭 이벤트
	 */
	scwin.btn_prjSchList_onclick = function(e) {
		if(tabContainer.getSelectedTabID() == "tabs1" && dma_search.get("syspayno") != ""){
			scwin.searchPartPsn();
		}else{
			scwin.search();
		}
		
	};
	
	/**
	 * 조회
	 */
	scwin.search = function(){
		com.execute(sbm_selectPrjList, {
			successCallback : function(e){
				dma_search.set("accnt_no_nm", dma_main.get("prj_nm"));
				
				btn_final_attach.setDisabled(false);
				btn_down_excel.setDisabled(false);
			}
		});
	};
	
	/**
	 * 참여연구원 조회
	 */
	scwin.searchPartPsn = function(){
		com.execute(sbm_selectPartPsn, {
			successCallback : function(e){				
				dlt_pare.setJSON(e.responseJSON.resInpPareVoList);
				
			}
		});
	};
	
	/**
	 * 계정번호 검색 팝업 호출
	 */
	scwin.btn_accntSch_onclick = function(e) {
		com.pop.open(com.pop.TYPE.ACNNO, {accnt_no : dma_search.get("accnt_no")}, function(ret){
			dma_search.set("accnt_no", ret.accnt_no);
			dma_search.set("accnt_no_nm", ret.accnt_no_nm);
			
			// 계정번호가 존재하면 조회를 실행
			if (ret.accnt_no != "") {
				com.execute(sbm_selectPrjList);
			}
		});
	};
	
	/**
	 * 닫기버튼클릭 이벤트
	 */
	scwin.btn_close_onclick = function(e) {
		if(window.location.pathname == '/popup.jsp'){
			window.close();
		}else{
			com.closePopup();
		}
	};
	
	scwin.ibx_accnt_no_onchange = function(info) {
		if(info.newValue == "") {	
			dma_search.set("accnt_no_nm", "");
		}
	};
	
	scwin.btn_down_excel_onclick = function(e) {
		var tabIndex = tabContainer.getSelectedTabIndex();
		var now = com.getServerDateTime("yyyyMMdd");
		var name = "";
		var obj;
		
		if(tabIndex == 0) {
			obj = grd_PsnData;
			name = '참여연구원_' + now + '.xlsx';
		} else if(tabIndex == 1) {
			obj = grd_paco;
			name = '참여기관_' + now + '.xlsx';
		} else if(tabIndex == 2) {
			obj = grd_PordData;
			name = '연구기자재_' + now + '.xlsx';
		}
		
		var option = {
			    fileName : name
		};
		com.downloadGridExcel(obj, option);
	};
	
	/**
	 * 사업계획서버튼클릭 이벤트
	 */	
	scwin.btn_final_attach_onclick = function(e) {
		var data ={
			attach_file_no : dlt_pare.getCellData(0, "final_attach_file"),
			mode : "view",
			ref_yn : "Y"
		};		
		
		com.pop.open(com.pop.TYPE.ATTACH, data, function(ret) {			
		});
	};
	
	/**
	 * 탭변경 이벤트
	 */	
	scwin.tabContainer_ontabclick = function(tabID,idx) {
		if(tabID == "tabs1"){
			grp_psn.show("");
			idx_emp_nm.show("");
			spn_emp.show("");
		}else{
			grp_psn.hide();
			idx_emp_nm.hide();
			spn_emp.hide();
		}
	};
	
	/**
	 * 참여연구원 검색
	 */	
	scwin.btn_emp_onclick = function(e) {
		var data ={
			empno : ibx_emp_no.getValue()
		};
		
		if(ibx_emp_no.getValue() == ""){
			data.ref_yn = "Y";	
		}
		com.pop.open(com.pop.TYPE.HUM, data, function(ret) {
			dma_search.set("syspayno", ret.syspayno);	
			dma_search.set("nm", ret.nm);
			dma_search.set("empno", ret.empno);
		});
	};
	
	scwin.ibx_emp_no_onblur = function(e) {
		if(this.getValue() == ""){
			dma_search.set("syspayno", "");	
			dma_search.set("nm", "");	
		}
	};

	]]></script>
	</head>
