        <script type="text/javascript" lazy="false"><![CDATA[
    /*************************************************************************************************************
     *
     *	전역변수
     *
     *************************************************************************************************************/
    scwin.currentDate = com.getServerDateTime("yyyyMMdd");
    
    
    /*************************************************************************************************************
     *
     *	페이지 이벤트 함수
     *
     *************************************************************************************************************/
    /*
     * 페이지 열기
     */
	scwin.onpageload = function() {
		var codeOptions = [
			//저널구분
			{ code : "BCG", compID :"sbx_jurnl" }
			//간기
		,	{ code : "BCD", compID :"sbx_imprnt" }
			//구입구분
		,	{ code : "BCC", compID :"sbx_pur" }
			//출판구분
		,	{ code : "BCA", compID :"sbx_clsf" }
			//도서관구분
		,	{ code : "BAC", compID :"sbx_centr" }
			//국가구분
		,	{ code : "FDA", compID :"sbx_natn" }
			//SCI구분
		,	{ code : "RRC", compID :"sbx_sci" }
		];
		com.setCmnCd(codeOptions, function() {
			dma_search.set("jurnl_no", com.getParameter("jurnl_no"));
			dma_jurnlNo.set("syspayno", com.getLoginInfo("sn"));
			
			com.biz.checkRole({ serviceId : "K61F", roleCodes : com.getLoginInfo("roleCode") }, function(e) {
				if(!e.isRoleYn){
					btn_delete.hide();
					btn_save.hide();
				}
			});
			
			if(dma_search.get("jurnl_no")) {
				scwin.search();
			}
			else
				scwin.init();
		});
	};
	
	/*
     * 페이지 닫기
     */
	scwin.onpageunload = function() {
		
	};
	
	
	/*************************************************************************************************************
     *
     *	사용자 정의 함수
     *
     *************************************************************************************************************/
	/*
     * 초기화
     */
	scwin.init = function() {
		dma_search.set("regst_ymd", scwin.currentDate);
	};
	
	/*
     * 조회
     */
	scwin.search = function() {
		com.execute(sbm_selectKbkJurnlNoReg, {
			successCallback : function(e) {
				var data = e.responseJSON.elData
				
				//주제분야
				data.subj_fild = data.subj_fild.replaceAll(" ", ",");
				
				dma_jurnlNo.setJSON(data);
			}
		});
	};
	
	/*
     * 저장
     */
	scwin.save = function() {
		if(scwin.validation()) {
			com.confirm("message.xom.wq.017", function(ret) {
				if(ret) {
					//주제분야
					var subjFild = dma_jurnlNo.get("subj_fild");
					dma_jurnlNo.set("subj_fild", subjFild.replaceAll(",", " "));
					
					
					//수정
					if(dma_jurnlNo.get("jurnl_no")) {
						dma_search.set("regst_ymd", scwin.currentDate);
						com.execute(sbm_updateKbkJurnlNoReg, {
							successCallback : function(e) {
								com.alert("message.xom.wq.023", function() {
									com.closePopup({ research : true });
								});
							}
						});
					}
					//등록
					else {
						com.execute(sbm_insertKbkJurnlNoReg, {
							successCallback : function(e) {
								com.alert("message.xom.wq.023", function() {
									com.closePopup({ research : true });
								});
							}
						});
					}
				}
			});
		}
	};
	
	/*
     * 검수
     */
	scwin.validation = function() {
		var data = dma_jurnlNo.getJSON();
		
		if(!data.jurnl_clsf_cd) {
			alert(dma_jurnlNo.getName("jurnl_clsf_cd") + "이(가) 공란입니다");
			return false;
		}
		else if(!data.regst_ymd) {
			alert(dma_jurnlNo.getName("regst_ymd") + "이(가) 공란입니다");
			return false;
		}
		else if(!data.jurnl_subj) {
			alert(dma_jurnlNo.getName("jurnl_subj") + "이(가) 공란입니다");
			return false;
		}
		else if(!data.clsf_cd) {
			alert(dma_jurnlNo.getName("clsf_cd") + "이(가) 공란입니다");
			return false;
		}
		else if(!data.natn_cd) {
			alert(dma_jurnlNo.getName("natn_cd") + "이(가) 공란입니다");
			return false;
		}
		else if(!data.sci_clsf_cd) {
			alert(dma_jurnlNo.getName("sci_clsf_cd") + "이(가) 공란입니다");
			return false;
		}
		else if(!data.imprnt_cd) {
			alert(dma_jurnlNo.getName("imprnt_cd") + "이(가) 공란입니다");
			return false;
		}
		else if(!data.recent_wrhsg_schdl_ymd) {
			alert(dma_jurnlNo.getName("recent_wrhsg_schdl_ymd") + "이(가) 공란입니다");
			return false;
		}
		else if(!data.centr_cd) {
			alert(dma_jurnlNo.getName("centr_cd") + "이(가) 공란입니다");
			return false;
		}
		
		return true;
	};
	
	
	/*************************************************************************************************************
     *
     *	컴포넌트 이벤트 함수
     *
     *************************************************************************************************************/
	/*
     * 유예기간
     */
	scwin.inp_pspnmtTime_onchange = function(info) {
		if(info.newValue > 30) {
			alert("30일 이하로 입력가능합니다");
			dma_jurnlNo.set("pspnmt_time", "0");
		}
	};
	
	/*
     * 열기
     */
	scwin.btn_open_onclick = function(e) {
		var url = dma_jurnlNo.get("electr_jurnl_homepg");
		var httpUrl = "";
		
		if(url.length < 1) {
			return;
		}
		else if(url.length < 5) {
			alert("URL 정보가 유효하지 않습니다.\n" + url);
			return;
		}
		else if(url.substring(0, 4) != "http"){
			httpUrl = "http://" + url;
		}
		else {
			httpUrl = url;
		}
		
		window.open(httpUrl, "", "width=1024, height=768, toolbar=yes, location=yes, scrollbars=yes, resizable=yes");
	};
	
	/*
     * 닫기
     */
	scwin.btn_close_onclick = function(e) {
		com.closePopup({ research : false });
	};
	
	/*
     * 삭제
     */
	scwin.btn_delete_onclick = function(e) {
		com.confirm("message.xom.wq.022", function(ret) {
			if(ret) {
				com.execute(sbm_deleteKbkJurnlNoReg, {
					successCallback : function(e) {
						com.alert("message.xom.wq.023", function() {
							com.closePopup({ research : true });
						});
					}
				});
			}
		});
	};
	
	/*
     * 저장
     */
	scwin.btn_save_onclick = function(e) {
		scwin.save();
	};
	
	]]></script>
