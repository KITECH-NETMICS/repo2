	scwin.inp_empno_onkeypress = function(e) {
		if(event.keyCode == 13){
			scwin.selectHumBtnStu();
		}
	};

	// 지도교수 엔터키 검색 이벤트
	scwin.inp_pruse_psn_empno_onkeypress = function(e) {
		if(event.keyCode == 13){
			scwin.selectHumBtnPruse();
		}
	};	
	
	// 학생연구원 빈값일경우 이름, 시스템사번 데이터셋 초기화
	scwin.inp_empno_onchange = function(info) {
		if(info.newValue == "") {	
			dma_stuMng.set("inp_nm");
			dma_stuMng.set("inp_sn", "");
			dma_stuMng.set("inp_empno", "");
		}
	};
	
	// 지도교수 빈값일경우 이름, 시스템사번 데이터셋 초기화
	scwin.inp_pruse_psn_empno_onchange = function(info) {
		if(info.newValue == "") {	
			dma_stuMng.set("inp_pruse_psn_nm", "");
			dma_stuMng.set("inp_pruse_psn_sn", "");
			dma_stuMng.set("inp_pruse_psn_empno", "");
		}
	};
	
		
	// 엑셀 다운로드
	scwin.btn_down_excel_onclick = function(e) {
		var now = com.getServerDateTime("yyyyMMdd");
		var name = '학생연구원 관리_' + now + '.xlsx';
		
		var option = {
			    fileName : name
