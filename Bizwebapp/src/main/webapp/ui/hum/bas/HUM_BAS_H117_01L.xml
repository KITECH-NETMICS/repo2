	// 현장실습생 학교지원금 출처(거래처) 매핑  20230313
	function popupSupportVend(){
		var node = "searchYn=N&gfnYn=N&useFlag=Y";
		var url = "/kitech/com/ComPopupVenderSel.xml";

		WebSquare.uiplugin.popup.createPopup("pop6", "window", "", "top:100px;left:100px;width:750px;height:400px", "false", "false", url, null, null, node);
			
	}


    //  조회
	scwin.schStuReqList = function() {		
		com.execute(sbm_selectHumStuMngReqList, {
			successCallback : function(e){
				dlt_stuMng.setJSON(e.responseJSON.humBasStuMngListVoList);
				tbx_result.setValue(dlt_stuMng.getRowCount());
			}
		});
	};
	
	
	// 학생연구원 검색 팝업 호출
	scwin.selectHumBtnStu = function(data) {
		
		data == "img" ? data = { empno : ""} : data = { empno : inp_empno.getValue(), nm : inp_empno.getValue(), work_clsf : "HAG010" };
		
		com.pop.open( com.pop.TYPE.HUM, data, function(ret){
			if (ret) {
				dma_stuMng.set("inp_empno", ret.empno);
				dma_stuMng.set("inp_nm", ret.nm);
				dma_stuMng.set("inp_sn", ret.syspayno);
			}
		});
	};

	
	// 지도교수검색 팝업 호출
	scwin.selectHumBtnPruse = function(data) {

		data == "img" ? data = { empno : ""} : data = { empno : inp_pruse_psn_empno.getValue(), nm : inp_pruse_psn_empno.getValue(), work_clsf : "HAG010" };
				
		com.pop.open( com.pop.TYPE.HUM, data, function(ret){
			if (ret) {
				dma_stuMng.set("inp_pruse_psn_empno", ret.empno);
				dma_stuMng.set("inp_pruse_psn_nm", ret.nm);
				dma_stuMng.set("inp_pruse_psn_sn", ret.syspayno);
			}
		});
	};





