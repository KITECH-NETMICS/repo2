				var syspayno = grd_stdMng.getCellData(row,  "syspayno");
				var empno = grd_stdMng.getCellData(row,  "empno");
				var nm = grd_stdMng.getCellData(row,  "nm");
				var dept_nm = grd_stdMng.getCellData(row,  "dept_nm");
				var posi_nm = grd_stdMng.getCellData(row,  "posi_nm");
				search_accountsel(syspayno, empno, nm, dept_nm, posi_nm);
			}
		} else
*/
		if (colId == "contrct_apr_state_nm") { // 입소신청상태
			if (grd_stdMng.getCellData(row , col) == "결재완료") {
				var apyReqNo = grd_stdMng.getCellData(row,  "apy_req_no");
				//var url = "http://apr.kitech.re.kr/processparticipant/worklist/workitemHandler.jsp?reqno="+apyReqNo;
				//window.open(url,"채용(비정규직)신청서","scrollbars=yes,toolbar=no,location=no,status=no,menubar=no,resizable=yes,width=920,height=660,left=100,top=100");
				eip3Snapshot ( retReqNo );
			}
		} else if (colId == "apr_state_nm") { // 퇴소신청상태
			if (grd_stdMng.getCellData(row , col) == "결재완료") {
				var retReqNo = grd_stdMng.getCellData(row,  "ret_req_no");
				//var url = "http://apr.kitech.re.kr/processparticipant/worklist/workitemHandler.jsp?reqno="+retReqNo;
				//window.open(url,"퇴직신청서","scrollbars=yes,toolbar=no,location=no,status=no,menubar=no,resizable=yes,width=920,height=660,left=100,top=100");
				eip3Snapshot ( retReqNo );
			}
		} else if (colId == "vend_nm_bt"){
			popupSupportVend();
		}		
	};
	
	
