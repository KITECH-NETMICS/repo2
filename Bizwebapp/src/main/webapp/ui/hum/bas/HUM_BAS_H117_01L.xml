			  , hiddenVisible : true
		};
		com.downloadGridExcel(grd_stdMng, option);
	};
		
	

		
	// 조회기간 조건에 따른 날짜 입력값 처리	

	scwin.sbx_prd_stn_onchange = function(info) {
		var prdst = inputCalendarSt.getValue().trim(); // 기간시작일
		var prdcl = inputCalendarCl.getValue().trim(); // 기간종료일
		dma_stuMng.set("frs_entr_from", "");
		dma_stuMng.set("frs_entr_to", "");
		dma_stuMng.set("entr_from", "");
		dma_stuMng.set("entr_to", "");
		dma_stuMng.set("contrct_cls_from", "");
		dma_stuMng.set("contrct_cls_to", "");
		dma_stuMng.set("retire_from", "");
		dma_stuMng.set("retire_to", "");

		if (sbx_prd_stn.getValue() == '1') {
			dma_stuMng.set("frs_entr_from", prdst);
			dma_stuMng.set("frs_entr_to", prdcl);
		} else if (sbx_prd_stn.getValue() == '2') {
			dma_stuMng.set("entr_from", prdst);
			dma_stuMng.set("entr_to", prdcl);
		} else if (sbx_prd_stn.getValue() == '3') {
			dma_stuMng.set("contrct_cls_from", prdst);
			dma_stuMng.set("contrct_cls_to", prdcl);
		} else if (sbx_prd_stn.getValue() == '4') {
			dma_stuMng.set("retire_from", prdst);
			dma_stuMng.set("retire_to", prdcl);
		}
	};


	scwin.inputCalendar_onchange = function(info) {
		var prdst = inputCalendarSt.getValue().trim(); // 기간시작일
		var prdcl = inputCalendarCl.getValue().trim(); // 기간종료일
		dma_stuMng.set("frs_entr_from", "");
		dma_stuMng.set("frs_entr_to", "");
		dma_stuMng.set("entr_from", "");
		dma_stuMng.set("entr_to", "");
		dma_stuMng.set("contrct_cls_from", "");
		dma_stuMng.set("contrct_cls_to", "");
		dma_stuMng.set("retire_from", "");
		dma_stuMng.set("retire_to", "");

		if (sbx_prd_stn.getValue() == '1') {
			dma_stuMng.set("frs_entr_from", prdst);
			dma_stuMng.set("frs_entr_to", prdcl);
		} else if (sbx_prd_stn.getValue() == '2') {
			dma_stuMng.set("entr_from", prdst);
			dma_stuMng.set("entr_to", prdcl);
		} else if (sbx_prd_stn.getValue() == '3') {
			dma_stuMng.set("contrct_cls_from", prdst);
			dma_stuMng.set("contrct_cls_to", prdcl);
		} else if (sbx_prd_stn.getValue() == '4') {
			dma_stuMng.set("retire_from", prdst);
			dma_stuMng.set("retire_to", prdcl);
		}
	};
	
