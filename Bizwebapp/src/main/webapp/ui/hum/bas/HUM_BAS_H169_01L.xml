		for(var i=0; i<dlt_grd.getRowCount(); i++){
			if(dlt_grd.getCellData(i, "educ_year") == ""){
				alert("엑셀의 교육년도 필수 입력사항입니다.(" + (i + 1) + " 행)");
				return;
			}
			
			if(dlt_grd.getCellData(i, "educ_typ") == ""){
				alert("엑셀의 교육구분 필수 입력사항입니다.(" + (i + 1) + " 행)");
