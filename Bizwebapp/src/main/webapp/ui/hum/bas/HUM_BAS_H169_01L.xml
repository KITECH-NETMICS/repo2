        <script type="text/javascript" lazy="false"><![CDATA[
    /*
     *  날짜 설정
     */
	var now = new Date();
	var defalutday = gcm.date.formatDateTime(now, "yyyyMMddHHmm"); // yyyyMMddHHmm
	var ymd = gcm.date.formatDateTime(now); // yyyyMMdd 
	var year = ymd.substr(0, 4); // yyyy
    
	scwin.onpageload = function() {
		ica_educ_year.setValue(year);
		// 공통코드
		var codeOptions = [ 
							{ code : "HDI", compID : "sbx_educ_typ, grd_list:educ_typ"},
							{ code : "HDO", compID : "sbx_educ_cd, grd_list:educ_cd", isOnDB : true}
		               	  ];		
		               	  
		com.setCmnCd(codeOptions, function(){
			com.execute(sbm_selectOccutypCd, {
				successCallback : function(e) {
					dlt_occutyp_cd.setJSON(e.responseJSON.humOccutypCdVoList);
				}
			});
			
			com.execute(sbm_selectPosiCd, {
				successCallback : function(e) {
					dlt_posi_cd.setJSON(e.responseJSON.humPosiCdVoList);
				}
			});
		});
		scwin.btn_search_onclick();
	};
	
	scwin.onpageunload = function() {
		
	};
	
	scwin.btn_search_onclick = function(e) {
		com.execute(sbm_selectData, {
			successCallback : function(e){
				dlt_grd.setJSON(e.responseJSON.humBasEducMngmtVoList);
				tbx_result.setValue(dlt_grd.getRowCount());
				
				var session = com.getLoginInfo();
				
				for(var i=0;i<dlt_grd.getRowCount();i++){
					dlt_grd.setCellData(i, "target_syspayno"
										, (dlt_grd.getCellData(i ,"target_syspayno") == "00000000") ? "" : dlt_grd.getCellData(i ,"target_syspayno"));
					grd_list.setReadOnly( "cell" , i , "educ_year" , true );
					grd_list.setReadOnly( "cell" , i , "educ_typ" , true );
					grd_list.setReadOnly( "cell" , i , "educ_cd" , true );
					grd_list.setReadOnly( "cell" , i , "target_occutyp_cd" , true );
					grd_list.setReadOnly( "cell" , i , "target_posi_cd" , true );
					grd_list.setReadOnly( "cell" , i , "target_nm" , true );
					if (-1 < session.roleCode.indexOf("1003219")){
						dlt_grd.setDisabled( "cell" , i , "info" ,  false);
					}
				}
				
				if(dlt_grd.getRowCount() == 0){
					dlt_grd.insertRow(dlt_grd.getRowCount());
				}
			}
		});
	};
	
	scwin.grd_list_oncellclick = function(row,col,colId) {
		if(colId == "btn_target"){
			var data = {
				empno : dlt_grd.getCellData(row, "target_nm"),
				work_clsf : "HAG010",
				ref_yn : "Y"
			};
			
			com.pop.open(com.pop.TYPE.HUM, data, function(ret){
				if (ret) {
					dlt_grd.setCellData(row, "target_syspayno", ret.syspayno);
					dlt_grd.setCellData(row, "target_nm", ret.nm);
				}
			});
		}
		
		if(colId == "btn_charg"){
			var data = {
				empno : dlt_grd.getCellData(row, "charg_nm"),
				work_clsf : "HAG010",
				ref_yn : "Y"
			};
			
			com.pop.open(com.pop.TYPE.HUM, data, function(ret){
				if (ret) {
					dlt_grd.setCellData(row, "charg_nm", ret.nm);
					dlt_grd.setCellData(row, "charg_syspayno", ret.syspayno);
					dlt_grd.setCellData(row, "charg_dept_nm", ret.dept_nm);
					dlt_grd.setCellData(row, "charg_dept_new_ymd", ret.dept_new_ymd);
					dlt_grd.setCellData(row, "charg_dept_cd", ret.dept_cd);
				}
			});
		}
		
		if(colId == "info"){
			var yr = dlt_grd.getCellData(row, "educ_year");
			var educTyp = dlt_grd.getCellData(row, "educ_typ");
			var educCd = dlt_grd.getCellData(row, "educ_cd");
			var targetOccutypCd = dlt_grd.getCellData(row, "target_occutyp_cd");
			var targetPosiCd = dlt_grd.getCellData(row, "target_posi_cd");
			var targetSysPayNo = dlt_grd.getCellData(row, "target_syspayno");
			if (targetSysPayNo == "00000000") {
				targetSysPayNo = "";
			}
			var targetNm = dlt_grd.getCellData(row, "target_nm");
			var param = "&yr=" + yr + "&educTyp=" +educTyp + "&educCd=" +educCd + "&targetOccutypCd=" +targetOccutypCd + "&targetSysPayNo=" +targetSysPayNo + "&targetNm=" +targetNm + "&targetPosiCd=" + targetPosiCd;
			com.biz.openPopupByNative('/filter/index.jsp?serviceid=H170' + param, '개인별교육훈련확인', {width: 1600, height: 1200}, 'BIZ2');
		}
		
		if(colId == "link_btn"){
			var http_url = "";
			var url = dlt_grd.getCellData(row, "link");
			if (url.length < 5) {
				alert("URL 정보가 유효하지 않습니다.\n" + url);
				return false;
			} else if (url.substring(0,4) != "http"){
				http_url = "http://" + url;
			} else {
				http_url = url;
			}
		
			window.open(http_url, "", "width=1024, height=768, toolbar=yes, location = yes, scrollbars = yes, resizable = yes");		
		}
	};

	scwin.btn_add_onclick = function(e) {
		var row = com.addRow(grd_list);
		dlt_grd.setCellData(row, "cud_type", "C");
		grd_list.setCellChecked(row, "chk", true);
	};
	
	scwin.btn_minus_onclick = function(e) {
		var row = grd_list.getCheckedIndex("chk");
		if(row.length < 1){
			alert("삭제할 데이터를 선택해주세요.");
			return;
		}
		
		for(var i=0; i<row.length; i++){
			if(dlt_grd.getCellData(row[i], "cud_type") == "U"){
				dlt_grd.setCellData(row[i], "cud_type", "D");
				grd_list.setRowColor(row[i], "#A6A6A6");
				row.splice(i, 1);
				alert("행 삭제는 저장 후 반영됩니다.");
			} else {
				dlt_grd.removeRows(row);
			}
		}
	};
	
	scwin.btn_copy_onclick = function(e) {
		var row = grd_list.getCheckedIndex("chk");
		
		if (row.length < 1) {
			alert("복사할 행을 선택해주세요.");
			return;
		}
		
		var rowCnt = dlt_grd.getRowCount();
		for (var i = 0; i < row.length; i++) {
			var rowJson = dlt_grd.getRowJSON(row[i]);
			dlt_grd.setRowJSON(rowCnt, rowJson, "");
			dlt_grd.setCellData(row[i], "chk", "N");
			dlt_grd.setCellData(rowCnt, "cud_type", "C");
		}
	};
	
	scwin.btn_save_onclick = async function(e) {
		var targetTmp = "";
		var cmp_chk = "";
		
		if (!com.validateData(dlt_grd,[
				{ id : "educ_year", mandatory : true},
				{ id : "educ_typ", mandatory : true},
				{ id : "educ_cd", mandatory : true},
				{ id : "duty_cnt", mandatory : true},
				{ id : "duty_time", mandatory : true},
				{ id : "charg_syspayno", mandatory : true},
				{ id : "educ_enddate", mandatory : true},
			], false)
		) return;
		
		var row = grd_list.getCheckedIndex("chk");
		if (row.length < 1){
			alert("선택 후 저장 해주세요.");
			return;
		}
		
		for(var i=0; i<dlt_grd.getRowCount(); i++){
			if(grd_list.getRowColor(i) != "#A6A6A6"){
				for(var j=0; j<dlt_grd.getRowCount(); j++){
					if(grd_list.getRowColor(i) != "#A6A6A6"){
						if(i!=j){
							targetTmp = dlt_grd.getCellData(i, "target_syspayno");
							if (targetTmp == "00000000") {
								targetTmp = "";
							}
							
							if (dlt_grd.getCellData(i, "educ_year") == dlt_grd.getCellData(j, "educ_year") &&
								dlt_grd.getCellData(i, "educ_typ") == dlt_grd.getCellData(j ,"educ_typ") &&
								dlt_grd.getCellData(i, "educ_cd") == dlt_grd.getCellData(j, "educ_cd") &&
								dlt_grd.getCellData(i, "target_occutyp_cd") == dlt_grd.getCellData(j, "target_occutyp_cd") &&
								//targetTmp == dlt_grd.getCellData(j, "target_syspayno" ) && 
								dlt_grd.getCellData(i, "target_posi_cd") == dlt_grd.getCellData(j,"target_posi_cd")) {
									alert((i+1)+"행 과  "+(j+1)+"행에 중복된 데이터가 확인 됩니다.");
									return;
							}
						}
					}
				}
			}
		}
		
		
		for(var i=0; i<row.length; i++){
			if(grd_list.getRowColor(row[i]) != "#A6A6A6"){
				var educ_typ = dlt_grd.getCellData(row[i], "educ_typ");
				var educ_cd =  dlt_grd.getCellData(row[i], "educ_cd");
				
				cmp_chk = await scwin.sbm_select_cmp_cd(row[i], educ_typ, educ_cd);
			}
			
			/*if(dlt_grd.getCellData(row[i], "target_occutyp_cd").trim() == ""){
				dlt_grd.setCellData(row[i], "target_occutyp_cd", "000000");
			}
			
			if(dlt_grd.getCellData(row[i], "target_posi_cd").trim() == ""){
				dlt_grd.setCellData(row[i], "target_posi_cd", "000000");
			}*/
			
			if(dlt_grd.getCellData(row[i], "target_syspayno").trim() == ""){
				dlt_grd.setCellData(row[i], "target_syspayno", "00000000");
			}
			
			if(dlt_grd.getCellData(row[i], "charg_syspayno").length == 8){
				dlt_grd.setCellData(row[i], "charg_syspayno", dlt_grd.getCellData(row[i], "charg_syspayno").substr(2,8));
			}
			
			dlt_grd.setCellData(row[i], "regst_syspayno", com.getLoginInfo("sn"));
			dlt_grd.setCellData(row[i], "updt_syspayno", com.getLoginInfo("sn"));
		}
		
		if(cmp_chk == "Y"){
			com.confirm("message.xom.wq.017"/*저장 하시겠습니까?*/, function(ret){
				if(ret){
					com.execute(sbm_saveData, {
						successCallback : function(e) {
							com.alert("message.xom.wq.023"/*정상적으로 처리되었습니다.*/, function(){
								scwin.btn_search_onclick();
							});
						}
					});
				}
			})
		} else {
			return;
		}
	};
	
	scwin.sbm_select_cmp_cd = async function(row, educ_typ, educ_cd) {
		dma_cd_cmp_search.set("educ_typ", educ_typ);
		dma_cd_cmp_search.set("educ_cd", educ_cd);
		var check = "Y";
		
		await com.execute(sbm_selectCmpCd, {
			successCallback : function(e) {
				if(e.responseJSON.humBasEducMngmtVoList.length < 1){
