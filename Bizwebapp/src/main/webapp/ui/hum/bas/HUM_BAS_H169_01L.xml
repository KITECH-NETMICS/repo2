			}
			
			if(dlt_grd.getCellData(i, "educ_cd") == ""){
				alert("엑셀의 교육명 필수 입력사항입니다.(" + (i + 1) + " 행)");
				return;
			}
			
			grd_list.setDisabled("cell", i, "link_btn", true);
			grd_list.setDisabled("cell", i, "info", true);
			grd_list.setRowReadOnly(i, true);
		}
		
		var row = grd_list.getCheckedIndex("chk");
		
		for (var i = 0; i < row.length; i++) {
			dlt_grd.setCellData(row[i], "cud_type", "C");
		}
	};
	
