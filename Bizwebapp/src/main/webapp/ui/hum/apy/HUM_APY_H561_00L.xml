             	dma_search.set("acp_syspayno", "");
         }
         dma_search.set("dept_cd", ibx_deptCd.getValue());
         
		com.execute(sbm_searchHumApyTrainLstReq, {
				successCallback : function(e) {
					console.log("=======!!========", e.responseJSON);
					dlt_list.setJSON(e.responseJSON.humApyTrainLstReqVoList);
					tbx_result.setValue(dlt_list.getTotalRow());
				}
			});
	};	
	
	/**
	 * 조회전 체크 함수
	*/
    function preFind() {
     	  var selectYmd=sbx_ymd.getValue();
          var selectState=sbx_status.getValue();
          var req_from = ica_reqFrom.getValue();
          var req_to = ica_reqTo.getValue();
          var apr_state = sbx_aprState.getValue();
          var reqNo = ibx_reqNo.getValue();
          var empno= ibx_empNo.getValue();
          var empnm=ibx_nm.getValue();
		  var req_no_search = reqNo.trim();

           if (reqNo != "") req_no_search = req_no_search + '%';
           else var req_no_search = reqNo.trim();
     
     		if (selectYmd!="") {
					if (selectYmd=="1") {
						// 교육신청일로 조회시
						dma_search.set("req_ymd_from", req_from); 	// 신청시작일
						dma_search.set("req_ymd_to", req_to); 		// 신청종료일
					} else if (selectYmd=="2") {
						// 결과보고일로 조회시
						dma_search.set("rpt_req_ymd_from", req_from);	// 보고신청시작일
						dma_search.set("rpt_req_ymd_to", req_to);			// 보고신청종료일
					} else {
						// 교육기간으로 조회시
						dma_search.set("start_ymd_from", req_from);			// 교육시작일
						dma_search.set("cls_ymd_to", req_from);				// 교육종료일
					}
				}
				
                if (selectState != "") {
                	if (selectState=="1") {
                		// 교육훈련신청으로 조회시
                		dma_search.set("apr_state", apr_state); 			// 신청재결상태코드
                		dma_search.set("apr_opt_rpt", "");				// 신청재결상태코드
                		dma_search.set("req_no", req_no_search);		// 신청번호
                	} else {
                		// 결과보고로 조회시
                		dma_search.set("apr_state_rpt", apr_state);	// 보고재결상태코드
                		dma_search.set("apr_opt_rpt", "1");				// 신청재결상태코드
                		dma_search.set("rpt_req_no", req_no_search);	// 신청번호
                	}
                } else {
	                dma_search.set("req_no", req_no_search);			// 신청번호
                }
                
                dma_search.set("syspayno", com.getLoginInfo("sn"));	
                dma_search.set("educ_nm", ibx_educNm.getValue().trim());	
                return true;	
     }
	
