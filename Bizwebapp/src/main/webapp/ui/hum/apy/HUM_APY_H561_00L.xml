	*/
	scwin.search = function() {
		if (!preFind()) return false;
		
		if (sbx_ch.getValue() == "TUA002") {
             	dma_search.set("duc_code", "Y");
             	dma_search.set("acp_syspayno", com.getLoginInfo("sn"));
         } else {
