    /**
     * 저장 버튼 클릭 이벤트
     */		
	scwin.btn_save_onclick = function(e) {
		
		//필수값체크 실행
		if (!scwin.preSave()) {
			return false;
		}
		
		com.confirm("message.xom.wq.017"/*저장 하시겠습니까?*/, function(ret) {
			if (ret) {
				//저장 로직 실행
				scwin.save("save");		//as-is : XAD005
			}
		});
		
	};
	
    /**
     * 결재신청 버튼 클릭 이벤트
     */	
	scwin.btn_apr_onclick = function(e) {
				
		//반려상태인 경우
		if(dma_info.get("apr_state") == "XAD015"){
			com.alert("저장 후 신청바랍니다.");	
			return;
		};
		
		//필수값체크 실행
		if (!scwin.preSave()) {
			return false;
		}
		
		com.confirm("결재신청 하시겠습니까?", function(ret){
			if (ret) {
				//결재신청 로직 실행
				scwin.save("apr");		//as-is : XAD010
			}
		});
		
	};
	
    /**
     * 저장, 결재신청 전 필수값체크
     */
	scwin.preSave = function() {
		
		//필수체크
		if (!com.validateData(dma_info, [{ id : "req_ymd", mandatory : true}], false)) return false;	//신청일
		
		var cnt = grd_list_2.getTotalRow();	//반경후 학력사항 행 개수
		var apprvl_educ_yn_chk = 0;			//인정학력 체크박스 체크한 개수 확인용
		for(var i = 0; i < cnt; i++) {		//변경후 학력사항 행 개수만큼 반복
			
			var educ 			= grd_list_2.getCellData( i , "educ" );
			var entsch_yrmon 	= grd_list_2.getCellData( i , "entsch_yrmon" );
			var gradt_yrmon 	= grd_list_2.getCellData( i , "gradt_yrmon" );
			var schl_nm 		= grd_list_2.getCellData( i , "schl_nm" );
			var gradt_state 	= grd_list_2.getCellData( i , "gradt_state" );
			var apprvl_educ_yn 	= grd_list_2.getCellData( i , "apprvl_educ_yn" );
		
			// 학력
			if ( educ == "" ) {
				com.alert("학력을 입력하세요.");
				return false;
			}

			// 입학년월
			if ( entsch_yrmon == "" ) {
				com.alert("입학년월을 입력하세요.");
				return false;
			}

			// 졸업년월
			if ( gradt_yrmon == "" ) {
				com.alert("졸업년월을 입력하세요.");
				return false;
			}

			// 학교명
			if ( schl_nm == "" ) {
				com.alert("학교명을 입력하세요.");
				return false;
			}

			// 상태
			if ( gradt_state == "" ) {
				com.alert("상태를 입력하세요.");
				return false;
			}

			if (apprvl_educ_yn == "1") {
				apprvl_educ_yn_chk = apprvl_educ_yn_chk +1;
			}
		
		};
		
		if (apprvl_educ_yn_chk != 1 ) {
			com.alert("인정학력은 하나만 선택 가능합니다.");
			return false;
		}
		
		return true;

	};
		
    /**
     * 저장, 결재신청 로직
     */	
	scwin.save = function(saveType) {
		
		com.file.transport( function( attachObj){
		
			//저장정보 추가세팅
			dma_info.set("req_typ"			, "H0A");							//기본신청유형
			dma_info.set("syspayno"			, com.getLoginInfo("sn"));			//시스템개인번호
			dma_info.set("dept_cd"			, com.getLoginInfo("deptCode"));	//부서코드
			dma_info.set("dept_new_ymd"		, com.getLoginInfo("deptNewYmd"));	//부서신설일	
			dma_info.set("attach_file_no"	, attachObj.upload_A);				//첨부파일번호
			
			//결재신청시 저장정보 추가세팅
			if (saveType == "apr") {
				dma_info.set("approval", true); 	//결재여부
			}
			
			com.execute(sbm_saveData, {
				successCallback : function(e) { // 성공 콜백					
					com.alert("message.xom.wq.023"/*정상적으로 처리되었습니다.*/, function() {
						
						//값이 세팅되어있어서 재조회 위해 빈값으로 재설정
						dma_search.set("syspayno", "");
						//리턴받은 신청번호로 상세조회
						scwin.search(e.responseJSON.elData.req_no);
						
						//결재신청시 결재진행함 이동			
						if (saveType == "apr") {
							com.openTabMenu("A005", {});
						} 

					});										
				}
			});
			
		});
		
	};

    /**
     * 반려 버튼 클릭 이벤트
     */	
	scwin.btn_reject_onclick = function(e) {
		
		if(!scwin.data.isRoleYn){
			com.alert("관리자외에는 반려를 하실 수 없습니다.");
			return;
		}
		
		var options = {
		    id: "APR02P",
		    popupName: "반려의견",
		    width: 500,
		    height: 300
		};
		
		com.openPopup("/ui/apr/APR_C001_03P.xml", options, "", function(retObj) {
		    if (retObj.comments != null) {
		        dma_info.set("comments", retObj.comments);
		        com.execute(sbm_aprReject, {
		            successCallback: function(e) {
		                com.alert("반려처리 되었습니다");
		                com.openTabMenu("A004", {});
		            }
		        });
		    }
		});
		
	};
	
    /**
     * 처리 버튼 클릭 이벤트
     */	
	scwin.btn_confirm_onclick = function(e) {
	
		if(!scwin.data.isRoleYn){
			com.alert("관리자외에는 처리를 하실 수 없습니다.");
			return;
		}
		
		com.confirm("승인처리 하시겠습니까?", function(ret) { 		
			if(ret) {
				com.execute(sbm_aprAgr, {
					successCallback : function(e) {
						com.alert("승인처리 되었습니다");
						com.openTabMenu("A004", {});
					}
				});
			}
		});
		
		
	};

	]]></script>
