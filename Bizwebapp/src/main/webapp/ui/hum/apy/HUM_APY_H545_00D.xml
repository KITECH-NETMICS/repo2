         <script type="text/javascript" lazy="false"><![CDATA[
 	/****************************************************
	 * 화면 설명 : 학력사항변경신청
	 * 변경 이력 : 2023-08-31 swt 최초생성
	 * serviceId : H545
	 ****************************************************/
	scwin.data={};
    /*************************************************************************************************************
     *
     *	페이지 이벤트 함수
     *
     *************************************************************************************************************/
	/**
	 * 페이지 열기
	 */          
	scwin.onpageload = function() {
		
		//권한
		com.biz.checkRole({ serviceId : "H545", roleCodes : com.getLoginInfo('roleCode')}, function(e){
			scwin.data.isRoleYn = e.isRoleYn;
			
			//첨부파일 초기화  *필수*
			com.file.init();
			
			//변경전 학력사항 비활성화
			grd_list_1.setDisabled(true);
			
			//숨김처리
			tb_empno.hide();		//신청자 사번
			tb_nm.hide();			//신청자 이름
			tb_syspayno.hide();		//신청자 개인번호
			btn_prt_2.hide();		//반려,처리 버튼 숨김 (그룹영역)
			
			//공통코드 (selectbox)
			var codeOptions = [ 	//변경전 학력사항 grid 세팅
								{ code : "HAL", compID : "grd_list_1:educ" }			//학력
							  , { code : "HAI", compID : "grd_list_1:loc" }				//소재지
							  , { code : "HAN", compID : "grd_list_1:major" }			//전공
							  , { code : "HAO", compID : "grd_list_1:gradt_state" }		//상태
							  , { code : "HAP", compID : "grd_list_1:degr" }			//학위
							  		//변경후 학력사항 grid 세팅
							  , { code : "HAL", compID : "grd_list_2:educ" }			//학력
							  , { code : "HAI", compID : "grd_list_2:loc" }				//소재지
							  , { code : "HAN", compID : "grd_list_2:major" }			//전공
							  , { code : "HAO", compID : "grd_list_2:gradt_state" }		//상태
							  , { code : "HAP", compID : "grd_list_2:degr" }			//학위
								 ];	//포상종류
			com.setCmnCd(codeOptions, function() {	
			});
	
			//상세조회 구분 (목록 통한 상세조회 or 신규 구분 -> 신청번호 유무로 확인)
			var req_no = com.getParameter("req_no");
			
			//if : 신규작성(신청번호가 없는경우)
			if (com.isEmpty(req_no)) {
			
				//현재 결재 상태에 따라 버튼의 활성화 유무 컨트롤
				com.biz.disabledButtons();
				
				//첨부파일 셋팅
				com.file.create(upload_A.getID(), { fileKey : "H545", mode : 'edit' });
				
				//신청서 등록자 기본정보 세팅
				tb_empno.setValue(com.getLoginInfo("empno"));		//사원번호
				tb_nm.setValue(com.getLoginInfo("cn"));				//이름
				tb_syspayno.setValue(com.getLoginInfo("sn"));		//개인번호
				tb_dept_nm.setValue(com.getLoginInfo("deptName"));	//부서명
				tb_posi_nm.setValue(com.getLoginInfo("posiName"));	//직급
				cal_req_ymd.setValue(com.getServerDateTime());		//신청일
				//사번(성명) 형식
				tb_nm_empno.setValue(com.getLoginInfo("empno") + "(" + com.getLoginInfo("cn") +")");
				
				//기본 정보 조회하기
				scwin.search("");
	
				
			//else if : 신청번호 있는경우 -> 상세조회	
			} else {
				
				//상세 조회
				scwin.search(req_no);
				
			}
		});
		
	};
	
	/**
	 * 페이지 닫기
	 */     
	scwin.onpageunload = function() {
		
	};
	
    /**
     * 상세 조회
     */	
	scwin.search = function(req_no) {
		
		//if : 신청번호 없는 경우
		if(com.isEmpty(req_no)) {
			
			//신청번호 없는 경우 -> 변경전 학력사항만 조회해서 grd1, grd2에 공통으로 뿌려준다.
			dma_search.set("syspayno", tb_syspayno.getValue());
			

			//상세 조회 2 -> 변경전 학력사항
			com.execute(sbm_outHumApyInfoBefore, {
				successCallback : function(e) {
					
					//변경전 grid : 데이터 세팅
					dlt_edu_1.setJSON(e.responseJSON.humApyEduInfoVoList);
					//grd_list_1.setFocusedCell(0,0);
					
					//변경후 grid : 데이터 세팅
					dlt_edu_2.setJSON(e.responseJSON.humApyEduInfoVoList);
					grd_list_2.setFocusedCell(0,0);
					
					
				}
			});	
			
	
		//else : 신청번호 있는 경우  
		} else {
			
			//상세 조회1 조회 조건 세팅
			dma_search.set("req_no", req_no);
			
			//상세 조회 1 -> 신청서 상단 기본 정보
			com.execute(sbm_outHumApyInfo, {
				successCallback : function(e) {
					
					dma_info.setJSON(e.responseJSON.elData);
					
					//사번(성명) 형식
					tb_nm_empno.setValue(tb_empno.getValue() + "(" + tb_nm.getValue() +")");

					//상세 조회2 조회 조건 세팅
					dma_search.set("syspayno", dma_info.get("syspayno"));
					//상세 조회 2 -> 변경전 학력사항
					com.execute(sbm_outHumApyInfoBefore, {
						successCallback : function(e) {
							
							//변경전 grid : 데이터 세팅
							dlt_edu_1.setJSON(e.responseJSON.humApyEduInfoVoList);
							//grd_list_1.setFocusedCell(0,0);
							
					
							//상세 조회 3 -> 변경후 학력사항
							com.execute(sbm_outHumApyInfoAfter, {
								successCallback : function(e) {
									
									//변경전 grid : 데이터 세팅
									dlt_edu_2.setJSON(e.responseJSON.humApyEduInfoVoList);
									grd_list_2.setFocusedCell(0,0);
									

									//결재상태코드, 신청번호 보내서 버튼 컨트롤
									com.biz.disabledButtons(dma_info.get("apr_state"), req_no);									
									
									//관리자인 경우 && 결재상태 "담당자 확인중"인 경우 버튼 처리
									if(scwin.data.isRoleYn && dma_info.get("apr_state") == "XAD070") {
										btn_prt_2.show();	//반려, 처리 버튼 보임 (그룹영역)
										btn_prt_1.hide();	//삭제, 저장 버튼 숨김 (그룹영역)
									}
									
									//첨부파일 조회
									var vMode = "edit";		//첨부파일 수정모드
									var aprChk = false;		
									if (dma_info.get("apr_state") >= "XAD020") {
										//비활성화
										vMode = "view";		//첨부파일 보기모드
										aprChk = true;		//버튼 비활성화 값 세팅
									}
									//첨부파일 조회 관련
									var attachFileNo = dma_info.get("attach_file_no");
									com.file.create(upload_A.getID(), {fileKey: com.isNotEmpty(attachFileNo) ? attachFileNo : 'H545', mode: vMode});
									//입력창 활성화, 비활성화
									grd_list_2.setReadOnly(aprChk);		//변경후 학력사항
									trggier_add.setDisabled(aprChk);	//변경후 학력사항 그리드 내 "+" 버튼
									trggier_del.setDisabled(aprChk);	//변경후 학력사항 그리드 내 "-" 버튼
									inp_rmk.setReadOnly(aprChk);		//변경내용 요약
									//cal_req_ymd.setReadOnly(aprChk);	//신청일
									
								}
							});	
						
						}
					});	
					
				}
			});
		
		}
		
	};
	
    /**
     * 목록 버튼 클릭 이벤트
     */	
	scwin.btn_list_onclick = function(e) {
		scwin.backToList();
	};
	
	/**	
	 * 목록 화면 이동
	 */
	scwin.backToList = function(data) {
		com.moveList("H535", data);
	};
	
    /**
     * grid2 내 "+" 버튼 클릭 이벤트
     */	
