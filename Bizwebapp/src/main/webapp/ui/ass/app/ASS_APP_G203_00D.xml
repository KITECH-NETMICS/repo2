	scwin.preSave = function() {
		var asetTyp = rdo_asetTyp.getValue();

		if(asetTyp == '') {
			com.alert("자산구분을 선택해주세요.!");
			return false;
		}

		var transSyspayno = ibx_userSyspayNo01.getValue();

		if(transSyspayno == '') {
			com.alert("인계자를 선택해주세요.!", function() {
				ibx_userNm01.select();
			});

			return false;
		}

		var cause = txa_cause.getValue();

		if(cause == '') {
			com.alert("인수 인계 사유를 입력해 주시기 바랍니다.!", function() {			
				txa_cause.focus();
			});

			return false;
		}

		var receiptSyspayno = ibx_userSyspayNo02.getValue(); // 인수자 시스템번호

		if(receiptSyspayno == '') {
			com.alert("인수자를 선택해주세요.!", function() {
				ibx_userNm02.focus();	
			});

			return false;
		}

		if(dlt_list.getRowCount() < 1) {
			com.alert("인수 인계 대상 자산을 선택 해 주시기 바랍니다!");

			return false;
		}
		
		// 자산구분 체크
		if (!scwin.asetTypCheck()) {
			com.alert("자산구분이 같은 경우에만 신청이 가능합니다.");
			
			return false;
		}

		return true;
	}
	
	// 자산구분 체크 - 자산구분이 같은 항목만 신청가능함
	scwin.asetTypCheck = function() {
		var asetTyp = rdo_asetTyp.getValue();
		var isChk = true;

		for (var i = 0; i < dlt_list.getRowCount(); i++) {
			
			if (i > 0) {
				if (asetTyp != dlt_list.getCellData(i, "aset_typ")) {
					isChk = false;
				}
			}
			//asetTyp = dlt_list.getCellData(i, "aset_typ");
		}
		
		return isChk;
	}
	
