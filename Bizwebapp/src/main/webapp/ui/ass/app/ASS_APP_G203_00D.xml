        <script type="text/javascript" lazy="false"><![CDATA[
	/****************************************************
	 * 화면 설명 : 자산 인수인계 / 수량 인수인계
	 * 변경 이력 : 2023-09-04	이정호	최초작성
	 * ServiceId : G203
 	 ****************************************************/
	// 폼번수
	scwin.docuClsf = "";
	scwin.saveFlag = true;
	scwin.cudType = "";

	scwin.data = {

	};

	var supManagerId01 = ""; // 인계담당자
	var supManagerId02 = ""; // 인수담당자

	scwin.onpageload = function() {
		// 첨부파일 컴포넌트 초기화 - 필수
		com.file.init();
		
		var param = com.getParameter();
		var loginInfo = com.getLoginInfo();

		scwin.data = {
			// 첨부 파일 변수
			"typeCode" : loginInfo.serviceId
			, "aprState" : ""// 결재상태코드
			, "attachFileNo_A" : ""

			// 결재화면에서 넘어온 파라미터 세팅
			, "param2" : param.param2 ? param.param2 : ""		// req_no
			, "param3" : param.param3 ? param.param3 : ""	// apr_state
			, "param4" : param.param4 ? param.param4 : ""	// apr

			// 목록화면에서 넘어온 파라미터 세팅
			, "args" : param.args ? param.args : ""
			, "p_screen_id" : param.screen_id ? param.screen_id : "G201"	// 화면id
			, "p_empno" : param.empno ? param.empno : ""
			, "p_user_nm" : param.user_nm ? param.user_nm : ""
			, "reqNo" : param.req_no ? param.req_no : ""			// 인수인계 신청번호

		}

		if(scwin.data.p_screen_id == "G202") {
			ibx_reqNo.setDisabled(false);
			ibx_reqNo.setStyle("text-decoration", "underline");
			ibx_reqNo.setStyle("color", "#0000FF");
			ibx_reqNo.setStyle("cursor", "pointer");
		}

		if(scwin.data.reqNo != "") {
			ibx_reqNo.setValue(scwin.data.reqNo);
		}
		
		com.setEnterKeyEvent(group_user01, scwin.btn_srchEmpNo01_onclick);
		com.setEnterKeyEvent(group_user02, scwin.btn_srchEmpNo02_onclick);

		// 공통코드
		var codeOptions = [
			{ code : "AAA", compID : "rdo_asetTyp" }	// 자산구분
		];

		com.setCmnCd(codeOptions, function() {
			rdo_asetTyp.deleteItem(4);
			rdo_asetTyp.deleteItem(3);

			scwin.data.args.aset_typ ? rdo_asetTyp.setValue(scwin.data.args.aset_typ) : rdo_asetTyp.setSelectedIndex(0);

			if(ibx_reqNo.getValue() != "") {	// 신청번호 있을 경우
				// 조회
				scwin.menuFind("N");
			} else {	// 신청번호 없을 경우
				if(loginInfo.roleCode.indexOf("1015117") == -1) {
					ibx_userEmpNo01.setValue(loginInfo.empno);
					scwin.getUserInfo();
					// 첨부파일 셋팅
					com.file.create(upload_A.getID(), { fileKey : scwin.data.typeCode, mode : 'edit' });
				}
			}
		});
	};
	
	scwin.onpageunload = function() {
		
	};
	
	scwin.getUserInfo = function() {
		var data = { empno : ibx_userEmpNo01.getValue(), work_clsf : "HAG010" };

		com.pop.open(com.pop.TYPE.HUM, data, function(ret) {
			ibx_userEmpNo01.setValue(ret.empno);
			ibx_userNm01.setValue(ret.nm);
			ibx_userSyspayNo01.setValue(ret.syspayno);
			ibx_userExtNo01.setValue(ret.ext_no);
			ibx_userEmail01.setValue(ret.email);
			ibx_userDeptNm01.setValue(ret.dept_nm);
			ibx_transDeptNewYmd01.setValue(ret.dept_new_ymd);
			ibx_userDeptCd01.setValue(ret.dept_cd);
			ibx_fomatUnit01.setValue(ret.fomat_unit);
		});
	};

	scwin.menuFind = function(afterSave) {
		dma_search.set("req_no", ibx_reqNo.getValue());

		com.execute(sbm_select, {
			successCallback : function(e) {
				var approval = dma_detail.get("approval");
				
				dlt_list.setJSON(e.responseJSON.elData.assAppChaReqDetailVoList);
				dma_detail.setJSON(dlt_list.getRowJSON(0));
				
				rdo_asetTyp.setDisabled(true);
				
				//첨부파일 셋팅
				com.file.create(upload_A.getID(), {
					fileKey : ("" !== dma_detail.get("attach_file_no").trim()) ? dma_detail.get("attach_file_no").trim() : scwin.data.typeCode
					, mode : 'edit' 
				});

				scwin.data.aprState = dma_detail.get("apr_state");

