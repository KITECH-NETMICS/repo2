	scwin.menuSave = function(cudType) {
		if(cudType == "S") {
			dma_detail.set("approval", true);

		} else if(cudType == "C") {
			dma_detail.set("approval", false);

		} 

		if(!scwin.preSave()) {
			return;
		}
		
		var asetTyp = rdo_asetTyp.getValue();

		switch (asetTyp) {
			case 'AAA010' :	// 자산
				scwin.docuClsf = 'A11';
				break;

			case 'AAA020' :	// 관리대상시작품
				scwin.docuClsf = 'A11';
				break;

			case 'AAA030' :	// 수량
				scwin.docuClsf = 'A10';
				break;
			
			default :
				scwin.docuClsf = '';
				break;
		}

		dma_detail.set("docu_clsf", scwin.docuClsf);

		if(scwin.data.reqNo != "" && scwin.cudType != "S") {
			scwin.cudType = "U";
		}

		dma_detail.set("cud_type", scwin.cudType);

//		if(scwin.cudType == "S") {
//			if(!scwin.selectDeptManager(ibx_userSyspayNo01.getValue(), ibx_userDeptNm01.getValue())) return false;
//			if(!scwin.selectDeptManager(ibx_userSyspayNo02.getValue(), ibx_userDeptNm02.getValue())) return false;
//		}

		com.file.transport(function(attachObj) {
			dma_detail.set("attach_file_no", attachObj.upload_A);
			
			com.execute(sbm_save, {
				successCallback : function(e) {
					if(dma_detail.get("approval")) {
						if(scwin.docuClsf == "A11") {
							// 스냅샷 이동
							com.movePageMenu("A001", { "reqNo" : dma_detail.get("req_no")});
						} else {
							// 결재진행함 이동
							com.movePageMenu("A005");
						}
					} else {
						ibx_reqNo.setValue(e.responseJSON.elData.req_no);
						scwin.menuFind("Y");

					}
				}
			});
		})

	}

