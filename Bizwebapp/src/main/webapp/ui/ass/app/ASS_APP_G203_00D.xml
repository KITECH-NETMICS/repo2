	scwin.btn_delChkRow_onclick = function(e) {
		var chkArr = grd_list.getCheckedIndex("chk").reverse();

		if(chkArr.length <= 0) {			
			com.alert("삭제할 행을 선택해 주십시오");
		} else {
			chkArr.forEach(function(index) {
				var rowStatus = dlt_list.getRowStatus(index);
				if(rowStatus == "R" || rowStatus == "U") {
					dlt_list.deleteRow(index);
					grd_list.setRowVisible(index, false); 

				} else {
					dlt_list.removeRow(index);
