        <script type="text/javascript" lazy="false"><![CDATA[

	scwin.data = {};

	scwin.onpageload = function() {

		// 공통코드
		var codeOptions = [
			{ code : "ABA" }	// 자산구분
			, { code : "AGA", compID : "sel_moveClsf" }	// 자산이동구분
			, { code : "ARB" }							// 자산 관리 주체
			, { code : "XAD" }							// 진행상태구분
		];

		com.setCmnCd(codeOptions, function() {
		});

		scwin.data.hisBack = com.getParameter("his_back");
		
		if(scwin.data.hisBack == "Y") {
			
		} else {
			// 오늘 날짜 세팅
			var today = com.getServerDateTime();
			cal_reqYmdSt.setValue(com.addDate(today, -60));
			cal_reqYmdEd.setValue(today);

			// 사용자 정보 세팅
			ibx_userEmpNo.setValue(com.getLoginInfo("empno"));		// 사용자 사원번호
			ibx_userEmpNm.setValue(com.getLoginInfo("cn").trim());
			ibx_userSysPayNo.setValue(com.getLoginInfo("sn"));
		}

	};
	
	scwin.onpageunload = function() {
		
	};

	/**
	 *	조회 함수
	 */
	scwin.search = function() {
		com.execute(sbm_selectAssAppMoveReqList, {
			successCallback : function(e) {

				// 데이터 초기화
				dlt_list.removeAll();

				// 조회결과 세팅
				dlt_list.setJSON(e.responseJSON.assAppMoveReqVoList, true);

				// 조회 건수 세팅
				tbx_totalCnt.setValue(dlt_list.getTotalRow());
			}
		});
	};
	
	scwin.btn_srch_onclick = function(e) {
		scwin.search();
	};
	
	scwin.btn_down_excel_onclick = function(e) {
		var now = com.getServerDateTime();
		var name = '이동신청 내역조회' + now + '.xlsx';

		var option = {
			fileName : name
			, removeColumns : "0"
		};

		com.downloadGridExcel(grd_list, option);
	};
	
	scwin.btn_req_onclick = function(e) {
		scwin.menuMove("new");
	};

	scwin.menuMove = function(flag) {
		var param = {
			"screen_id" : "G208"
			, "args" : dma_search.getJSON()
			, "nm" : ibx_userEmpNm.getValue()
			, "empno" : ibx_userEmpNo.getValue()
		}

		if(flag == "detail") {
			var row = grd_list.getFocusedRowIndex();
			param.g_req_no = dlt_list.getCellData(row, "req_no");
		}

		/**
		 * 서비스ID 등록 전 com.openTabUrl() 사용
		 * 서비스ID 등록 후 com.openTabMenu() 사용
		 */
//		com.openTabMenu("G210", param, function() {
//			scwin.search();
//		});
		com.openTabUrl("자산이동 신청" , "/ui/ass/app/ASS_APP_G210_00D.xml", 1, param, function() {
			scwin.search();
		});
	}

	scwin.grd_list_oncellclick = function(row, col, colId) {
		if(colId == "req_no") scwin.menuMove("detail");
	};
	]]></script>
