	scwin.sendApr = function() {
		com.execute(sbm_aprConfirm, {
			successCallback : function(e) {
				com.alert("수량 인수인계가 완료되었습니다.!", function() {
					// 결재,업무 완료함
					com.openTabMenu("A006", {});
				});
			}
		});
	};

	scwin.menuSave = function() {
		var sn = com.getLoginInfo("sn");
		var today = com.getServerDateTime();
		debugger;
		for(var i = 0; i < dlt_list.getRowCount(); i++) {
			dlt_list.setCellData(i, "receipt_affir_syspayno", sn);	// 인수자 확인정보
			dlt_list.setCellData(i, "receipt_affir_ymd", today);
			dlt_list.setCellData(i, "mngmt_mbody_affir_syspayno", sn);
			dlt_list.setCellData(i, "mngmt_mbody_affir_ymd", today);
			dlt_list.setCellData(i, "updt_syspayno", sn);	// 수정자
		}

		com.execute(sbm_savePosiRegion, {
			successCallback : function(e) {
				debugger;
				scwin.sendApr(scwin.data.aprClsf);
			}
		});
	};
	
	/**
	 * 인쇄 버튼 클릭시 이벤트
	 */
	scwin.btn_print_onclick = function(e) {
		var reqNo = dlt_list.getCellData(0, "req_no");
		if(reqNo.trim() != ""){
			let node = "req_no=" + reqNo;
			com.oz.openPopup({
			    ozrCode : "A005",      
			    param: node
			});
		}else{
			com.alert("인쇄할 내용이 없습니다.");
		} 
	};
