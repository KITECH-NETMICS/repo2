        <script type="text/javascript" lazy="false"><![CDATA[
	scwin.data = {
		"reqNo" : ""
		, "asetNo" : ""
		, "posiRegion" : ""
		, "posiBuild" : ""
		, "posiFloor" : ""
		, "posiDetail" : ""
		, "receiptSyspayno" : ""
		, "aprClsf" : ""

		// 추가 결재확장변수    
		, "ext1Cd" : ""
		, "infoCd" : ""
		, "ext2Cd" : ""
		, "ext3Cd" : ""

		//반려
		, "rejectComment" : ""
	};

	scwin.onpageload = function() {
		var param = com.getParameter();

		scwin.data = {
			"reqNo" : param.req_no
		};

		// 공통코드
		var codeOptions = [
			{ code : "HBX", compID : "sbx_posiRegion" }	// 위치(지역)
		];

		com.setCmnCd(codeOptions, function() {
			grd_list.setColumnNodeSet("posi_region", "data:dlt_cmnCd_HBX", "comNm", "comCd");
		});

		if(scwin.data.req_no != "") {
			scwin.menuFind();

		} else {
			// 사용위치

		}

	};
	
	scwin.onpageunload = function() {
		
	};

	scwin.menuFind = function() {
		// 신청번호
		dma_search.set("req_no", scwin.data.reqNo);
		dma_search.set("doc_id", scwin.data.reqNo.substr(0, 3));

		com.execute(sbm_getSelectData, {
			successCallback : function(e) {
				dma_detail.setJSON(e.responseJSON.elData.assAppChaReqDetailVoList[0]);
				dlt_list.setJSON(e.responseJSON.elData.assAppChaReqDetailVoList);

				sbx_posiRegion.setValue(dma_detail.get("posi_region"));
				
				if (dma_detail.get("apr_state") == "XAD100") {
					btn_confirm.setDisabled(true);	
				}
				
			}
		});
	};

	scwin.getBuilding = function(region) {
		dlt_building.removeAll();
		dma_search.set("mngmt_item_1", region);

		com.execute(sbm_getBuilding, {
			successCallback : function(e) {
				dlt_building.setJSON(e.responseJSON.assCommVoList);
				
				sbx_posiBuild.setValue(dma_detail.get("posi_build"));

				scwin.gfnFloor(dlt_building.getMatchedJSON("cd", dma_detail.get("posi_build"))[0]);
				
				sbx_posiFloor.setValue(dma_detail.get("posi_floor"));
			}
		});
	};

	scwin.gfnFloor = function(selectData) {
		var j = 0;

		if(!selectData) selectData = dlt_building.getRowJSON(0);

		// 지상 층 정보
		var mngmtNo1 = selectData.mngmt_no_1 != "" ? parseInt(selectData.mngmt_no_1, 10) : 0;
		var mngmtNo2 = selectData.mngmt_no_2 != "" ? parseInt(selectData.mngmt_no_2, 10) : 0;

		// 지하 층 정보
		var mngmtNo3 = selectData.mngmt_no_3 != "" ? parseInt(selectData.mngmt_no_3, 10) : 0;
		var mngmtNo4 = selectData.mngmt_no_4 != "" ? parseInt(selectData.mngmt_no_4, 10) : 0;

		// 옥상 정보
		var mngmtNo5 = selectData.mngmt_no_5 != "" ? parseInt(selectData.mngmt_no_5, 10) : 0;

		sbx_posiFloor.removeAll(true);

		// 지상층
		if(mngmtNo1 != 0) {
			for(j = mngmtNo1; j <= mngmtNo2; j++) {
				sbx_posiFloor.addItem(j + "F", j + "층");
			}
		}
		
		// 지하층
		if(mngmtNo3 != 0) {
			for(j = mngmtNo3; j <= mngmtNo4; j++) {
				sbx_posiFloor.addItem("B" + j * (-1), "지하 " + j * (-1) + " 층");
			}
		}

		// 옥상
		if(mngmtNo5 != 0) {
			sbx_posiFloor.addItem("옥상", "옥상");
		}
	};

	scwin.posiBatchUpdate = function(columnId, value) {
		var chkedArr = grd_list.getCheckedIndex("chk");

		switch(columnId) {
			case "posi_region":
				chkedArr.forEach(function(index) {
					dlt_list.setCellData(index, "posi_region", value);
				})
				break;

			case "posi_build":
				chkedArr.forEach(function(index) {
					dlt_list.setCellData(index, "posi_build", value);
					dlt_list.setCellData(index, "posi_build_nm", dlt_building.getMatchedColumnData("cd", value, "cd_nm", true)[0]);
				})
				break;

			case "posi_floor":
				chkedArr.forEach(function(index) {
					dlt_list.setCellData(index, "posi_floor", value);
				})
				break;

			default:
				break;
		}
		
	};

	// 위치-지역
	scwin.sbx_posiRegion_onchange = function(info) {

		if(info.newValue) scwin.getBuilding(info.newValue);
		
		
		scwin.posiBatchUpdate("posi_region", info.newValue);
	};

	// 위치-건물
	scwin.sbx_posiBuild_onchange = function(info) {
		var selectData = dlt_building.getMatchedJSON("cd", info.newValue)[0];
		scwin.gfnFloor(selectData);
		
		
		scwin.posiBatchUpdate("posi_build", info.newValue);
	};

	// 위치-층
	scwin.sbx_posiFloor_onchange = function(info) {
		scwin.posiBatchUpdate("posi_floor", info.newValue);
	};

	/**
	 *	승인 버튼 클릭 이벤트
	 */
	scwin.btn_confirm_onclick = function(e) {
		scwin.data.aprClsf = "APP";
		scwin.menuSave();
	};

	/**
	 *	반려 버튼 클릭 이벤트
	 */
	scwin.btn_reject_onclick = function(e) {
		var options = {
			id : "APR02P"
			, popupName : "반려의견"
			, width : 500
			, height: 300
		};

		com.pop.open(com.pop.TYPE.RETRN_OPN,{ref_yn : "Y"}, function(retObj) {
			if(retObj.comments != null) {
				dma_detail.set("comments", retObj.comments);
				
				com.execute(sbm_aprReject, {
					successCallback : function(e) {
