<?xml version="1.0" encoding="UTF-8"?>
<rMateChart backgroundColor="#FFFFFF" borderStyle="none">
    <Options>
        <Caption text="Global Temperature"/>
        <Legend />
    </Options>
    <NumberFormatter id="nft" precision="2"/>
    <Line2DChart showDataTips="true" dataTipFormatter="{nft}">
        <horizontalAxis>
            <CategoryAxis id="hAxis" categoryField="Month"/>
        </horizontalAxis>
        <verticalAxis>
            <LinearAxis id="vAxis" title="2011년 상승률 (%)"/>
        </verticalAxis>
        <series>
            <Line2DSeries yField="2011" displayName="2011년">
                <showDataEffect>
                    <SeriesClip duration="1000"/>
                </showDataEffect>
            </Line2DSeries>
        </series>
        <annotationElements>
            <CrossRangeZoomer enableZooming="false" horizontalStrokeEnable="false"/>
        </annotationElements>
    </Line2DChart>
</rMateChart>
