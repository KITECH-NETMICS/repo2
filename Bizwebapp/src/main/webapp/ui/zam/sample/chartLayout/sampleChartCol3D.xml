<?xml version="1.0" encoding="UTF-8"?>
<rMateChart backgroundColor="#FFFFFF" borderStyle="none">
    <Options>
        <Caption text="Global Temperature" />
    </Options>
    <Column3DChart showDataTips="true" columnWidthRatio="0.4">
        <horizontalAxis>
            <CategoryAxis categoryField="Month"/>
        </horizontalAxis>
        <verticalAxis>
            <LinearAxis id="vAxis" title="2011년 상승률 (%)" />
        </verticalAxis>
        <series>
            <Column3DSeries yField="2011" displayName="2011년">
                <showDataEffect>
                    <SeriesInterpolate/>
                </showDataEffect>
            </Column3DSeries>
        </series>
        <annotationElements>
            <CrossRangeZoomer enableZooming="false" horizontalStrokeEnable="false"/>
        </annotationElements>
    </Column3DChart>
</rMateChart>