         <script type="text/javascript" lazy="false"><![CDATA[
        
			scwin.GLOBAL_VARS = {
				p_param: com.getParameter(),
				revParam: {}
			};
        
			scwin.onpageload = function() {
				// 첨부파일 컴포넌트 초기화
                com.file.init();
                
                eval_psn_syspaynm.setValue(com.getLoginInfo('cn'));
        		eval_daytm.setValue(com.getServerDateTime("yyyyMMdd"));
                
                WFRAME_PATBIBLIOGRAPHY.getWindow().scwin.selectBibliography(scwin.GLOBAL_VARS.p_param.mngmt_no);
                
                dma_search.set('app_id', scwin.GLOBAL_VARS.p_param.ref_no);
					
				com.execute(sbm_svcpatpapreqsr03);
				
				const uid = com.getLoginInfo('uid');
				if (uid !== "patsysdev") {
	        		bpm1.hide();
	        	}
				
			};
			
			scwin.sbm_svcpatpapreqsr03_done = function(e) {
		
				com.trimDataCollection(dma_result);
			
				let attachFileNo_A = "";
				let attachFileNo_B = "";
				let attachFileNo_C = "";
					
				attachFileNo_A = dma_result.get("com_attach_file");
			    attachFileNo_B = dma_result.get("prt_app_attach_file");
			    attachFileNo_C = dma_result.get("prt_attach_file");
			
			    scwin.GLOBAL_VARS.p_param.job_state_cd = dma_result.get("job_state_cd");
			    scwin.GLOBAL_VARS.p_param.req_claim_yn = dma_result.get("req_claim_yn");
			    scwin.GLOBAL_VARS.p_param.com_patofic_cd = dma_result.get("com_patofic_cd");
			    scwin.GLOBAL_VARS.p_param.com_patofic_charg_id = dma_result.get("com_patofic_charg_id");
			
			    eval_psn_syspaynm.setValue(com.getLoginInfo('cn'));
			    eval_daytm.setValue(com.getServerDateTime("yyyyMMdd"));
				
				com.file.create(upload_A.getID(), { fileKey: attachFileNo_A ? attachFileNo_A : com.getLoginInfo('serviceId'), mode: "view", target: "IPMS" });
				com.file.create(upload_B.getID(), { fileKey: attachFileNo_B ? attachFileNo_B : com.getLoginInfo('serviceId'), mode: "view", target: "IPMS" });
				com.file.create(upload_C.getID(), { fileKey: attachFileNo_C ? attachFileNo_C : com.getLoginInfo('serviceId'), mode: "view", target: "IPMS" });
				
			};
			
			scwin.force_save_onclick = function(){
			
				if(!com.validateData(dma_save,  [
					{ id : "eval_result_scr", mandatory : true },
					{ id : "eval_service_scr", mandatory : true },					
				], false)) return;
				
                com.confirm("강제저장 하시겠습니까?", function(ret) {
                    if (ret) {
                    	scwin.force_save();
                    }
               });
            };
			
			scwin.save_onclick = function(){
			
				if(!com.validateData(dma_save,  [
					{ id : "eval_result_scr", mandatory : true },
					{ id : "eval_service_scr", mandatory : true },					
				], false)) return;
				
                com.confirm("저장 하시겠습니까?", function(ret) {
                    if (ret) {
                    	scwin.save();
                    }
               });
            };
            
            scwin.save = function(){
            	const sn = com.getLoginInfo('sn');
				const prt_app_date = dma_result.get("prt_app_date");
				
				dma_save.set("app_id", scwin.GLOBAL_VARS.p_param.ref_no);
				dma_save.set("req_no", scwin.GLOBAL_VARS.p_param.req_no);
				dma_save.set("mngmt_no", scwin.GLOBAL_VARS.p_param.mngmt_no);
				dma_save.set("com_patofic_cd", scwin.GLOBAL_VARS.p_param.com_patofic_cd);
				dma_save.set("com_patofic_charg_id", scwin.GLOBAL_VARS.p_param.com_patofic_charg_id);
				dma_save.set("req_claim_yn", scwin.GLOBAL_VARS.p_param.req_claim_yn);
				dma_save.set("bf_state", scwin.GLOBAL_VARS.p_param.job_state_cd);
				dma_save.set("curr_state_cd", scwin.GLOBAL_VARS.p_param.job_state_cd);
				dma_save.set("ref_key", scwin.GLOBAL_VARS.p_param.ref_no);
				dma_save.set("state", "P1000");
				dma_save.set("next_state_cd", "P1000");
				dma_save.set("job_state_cd", "P1000");
				dma_save.set("psn_syspayno", sn);
				dma_save.set("eval_psn_syspayno", sn);
				dma_save.set("updt_syspayno", sn);
				dma_save.set("syspayno", sn);
				
				if (prt_app_date && typeof prt_app_date === 'string' && prt_app_date.length >= 6) {
				    dma_save.set("smitapp_reg_ymd", prt_app_date.substr(0, 6));
				}
				
				com.execute(sbm_svcpatpapreqsi03, {
					successCallback : function(e) {
						com.alert("저장되었습니다.");
						$p.parent().scwin.trigger_list_onclick();
					}
				});
            };
            
            scwin.force_save = function(){
				const sn = com.getLoginInfo('sn');
				const prt_app_date = dma_result.get("prt_app_date");
				
				dma_save.set("app_id", scwin.GLOBAL_VARS.p_param.ref_no);
				dma_save.set("req_no", scwin.GLOBAL_VARS.p_param.req_no);
				dma_save.set("mngmt_no", scwin.GLOBAL_VARS.p_param.mngmt_no);
				dma_save.set("com_patofic_cd", scwin.GLOBAL_VARS.p_param.com_patofic_cd);
				dma_save.set("com_patofic_charg_id", scwin.GLOBAL_VARS.p_param.com_patofic_charg_id);
				dma_save.set("req_claim_yn", scwin.GLOBAL_VARS.p_param.req_claim_yn);
				dma_save.set("bf_state", scwin.GLOBAL_VARS.p_param.job_state_cd);
				dma_save.set("curr_state_cd", scwin.GLOBAL_VARS.p_param.job_state_cd);
				dma_save.set("ref_key", scwin.GLOBAL_VARS.p_param.ref_no);
				dma_save.set("state", "P1000");
				dma_save.set("next_state_cd", "P1000");
				dma_save.set("job_state_cd", "P1000");
				dma_save.set("psn_syspayno", "20150653");
				dma_save.set("eval_psn_syspayno", "20150653");
				dma_save.set("updt_syspayno", "20150653");
				dma_save.set("syspayno", "20150653");
				
				if (prt_app_date && typeof prt_app_date === 'string' && prt_app_date.length >= 6) {
				    dma_save.set("smitapp_reg_ymd", prt_app_date.substr(0, 6));
				}
				
				com.execute(sbm_svcpatpapreqsi04, {
					successCallback : function(e) {
						com.alert("저장되었습니다.");
						$p.parent().scwin.trigger_list_onclick();
					}
				});
            };
			
			scwin.onpageunload = function() {
				
			};
		
		]]></script>
