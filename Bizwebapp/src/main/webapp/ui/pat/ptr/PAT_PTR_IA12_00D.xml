         <script type="text/javascript" lazy="false"><![CDATA[
        
			scwin.GLOBAL_VARS = {
				p_param: com.getParameter(),
				revParam: {}
			};
			
			scwin.onpageload = function() {
				// 첨부파일 컴포넌트 초기화
                com.file.init();
                
	        	eval_psn_syspaynm.setValue(com.getLoginInfo('cn'));
	        	eval_daytm.setValue(com.getServerDateTime("yyyyMMdd"));
	        	
	        	complement.hide();
	        	$(".evaluation").wq("setStyle", "display", "none");
	        	
	        	eval_clsf.setValue('0');
	        	
                WFRAME_PATBIBLIOGRAPHY.getWindow().scwin.selectBibliography(scwin.GLOBAL_VARS.p_param.req_no);
                
                dma_search.set("req_no", scwin.GLOBAL_VARS.p_param.req_no);
                dma_search.set("mngmt_no", scwin.GLOBAL_VARS.p_param.mngmt_no);
                dma_search.set("srh_id", scwin.GLOBAL_VARS.p_param.ref_no);
                
                com.execute(sbm_svcpatptrrevsr02);
			};
			
			scwin.sbm_svcpatptrrevsr02_done = function() {
						
			      const cn = com.getLoginInfo('cn');
			      const toDay = com.getServerDateTime("yyyyMMdd");
			
			      scwin.GLOBAL_VARS.revParam.job_state_cd = dma_result.get("job_state_cd"); //상태코드
			      scwin.GLOBAL_VARS.revParam.srhres_id = dma_result.get("srhres_id"); //선행기술 결과번호
			
			      eval_psn_syspaynm.setValue(cn);
			      eval_daytm.setValue(toDay);
			
			      const attachFileNo_A = dma_result.get("req_attach_file_no");
			      com.file.create(upload_A.getID(), {fileKey: attachFileNo_A ? attachFileNo_A : com.getLoginInfo('serviceId'), mode: "view" , target:"IPMS"});
			
			      const attachFileNo_B = dma_result.get("rpt_srh_attach_file");
			      com.file.create(upload_B.getID(), {fileKey: attachFileNo_B ? attachFileNo_B : com.getLoginInfo('serviceId'), mode: "view" , target:"IPMS"});
			
			      const attachFileNo_C = dma_result.get("rpt_announce_attach_file");
			      com.file.create(upload_C.getID(), {fileKey: attachFileNo_C ? attachFileNo_C : com.getLoginInfo('serviceId'), mode: "view" , target:"IPMS"});
			
			      const attachFileNo_D = dma_result.get("rpt_attach_file");
			      com.file.create(upload_D.getID(), {fileKey: attachFileNo_D ? attachFileNo_D : com.getLoginInfo('serviceId'), mode: "view" , target:"IPMS"});
			      
			};
			scwin.save_onclick = function(){
			
				if(eval_clsf.getValue() === '1'){
					if(!com.validateData(dma_save,  [
						{ id : "rereq_comment", mandatory : true }
					], false)) return;
				}else{
					if(!com.validateData(dma_save,  [
						{ id : "eval_clsf", mandatory : true },
						{ id : "eval_result_scr", mandatory : true },
						{ id : "eval_service_scr", mandatory : true },
						{ id : "eval_comment", mandatory : true }
					], false)) return;
				}
				
                com.confirm("message.xom.wq.017"/*저장 하시겠습니까?*/, function(ret) {
                    if (ret) {
                    	scwin.save();
                    }
               });
            };
			
			scwin.save = function(){
			
				dma_save.set("req_no", scwin.GLOBAL_VARS.p_param.req_no);
				dma_save.set("mngmt_no", scwin.GLOBAL_VARS.p_param.mngmt_no);
				dma_save.set("syspayno", com.getLoginInfo('sn'));
				dma_save.set("curr_state_cd", scwin.GLOBAL_VARS.revParam.job_state_cd);
				dma_save.set("ref_key", scwin.GLOBAL_VARS.p_param.ref_no);
				dma_save.set("srh_id", scwin.GLOBAL_VARS.p_param.ref_no);
				dma_save.set("srhres_id", scwin.GLOBAL_VARS.revParam.srhres_id);
				dma_save.set("psn_syspayno", com.getLoginInfo('sn'));
				dma_save.set("bf_state", "P0213");
				                
				if (eval_clsf.getValue() === '0') { //완료/평가 P0219
					dma_save.set("state", "P0219");
					dma_save.set("next_state_cd", "P0219");
					dma_save.set("job_state_cd", "P0219");
					dma_save.set("eval_psn_syspayno", com.getLoginInfo('sn'));
				    
				} else { // 보완요청 P0214
					dma_save.set("state", "P0214");
					dma_save.set("next_state_cd", "P0214");
					dma_save.set("job_state_cd", "P0214");
				    dma_save.set("rereq_psn_syspayno", com.getLoginInfo('sn'));    
				}
				
				com.execute(sbm_svcpatptrrevsi01, {
					successCallback : function(e) {
						com.alert("저장되었습니다.");
			        	$p.parent().scwin.trigger_list_onclick();
					}
				});
			};
			
			scwin.eval_clsf_onchange = function(info){
				if(info.newValue === '0'){
					complement.hide();
		        	$(".evaluation").wq("setStyle", "display", "table-row");
				}else{
					complement.show("table-row");
		        	$(".evaluation").wq("setStyle", "display", "none");
				}
			};
			
			scwin.onpageunload = function() {
				
			};
		
		]]></script>
