         <script type="text/javascript" lazy="false"><![CDATA[
	        scwin.GLOBAL_VARS = {};
			
			scwin.onpageload = function() {
				// 첨부파일 컴포넌트 초기화
                com.file.init();
                
                scwin.GLOBAL_VARS = {
				    docuClsf: "I05",
				    prityEvalReqNo: com.getParameter().prity_eval_req_no || "", // If prity_eval_req_no is undefined, set it to an empty string
				    mngmtNo: com.getParameter().mngmt_no || "",
				    prvReqNo: com.getParameter().req_no || "",
				    aprState: com.getParameter().apr_state || "",
				    prvScrnId: com.getParameter().screen_id || "",
				    prityStartYmd: com.getParameter().prity_start_ymd || "",
				    prityEndYmd: com.getParameter().prity_end_ymd || "",
				    noType: com.getParameter().no_type || "",
				    refNo: com.getParameter().ref_no || "",
				    pritySyspaynm: com.getParameter().prity_syspaynm || "",
				    prityEmpno: com.getParameter().prity_empno || "",
				    pritySyspayno: com.getParameter().prity_syspayno || "",
				    cudType: "C",
				    toDay: com.getServerDateTime("yyyyMMdd"),
				    aprYn: "",
				    param2: com.getParameter().param2 || "",
				    param4: com.getParameter().param4 || "",
				    attachFileNo_A: ""
				};
                
                com.biz.disabledButtons(scwin.GLOBAL_VARS.aprState, dma_search.get("prity_eval_req_no"));
                
                if(scwin.GLOBAL_VARS.param4 === "apr"){
					if(scwin.GLOBAL_VARS.param2 !== ""){
						outputPrityEvalReqNo.setValue(scwin.GLOBAL_VARS.param2);
						scwin.GLOBAL_VARS.prityEvalReqNo = scwin.GLOBAL_VARS.param2;
					}				
					scwin.GLOBAL_VARS.aprState = com.getParameter().param3;
				}
				debugger;
				if(scwin.GLOBAL_VARS.prityEvalReqNo !== "") outputPrityEvalReqNo.setValue(scwin.GLOBAL_VARS.prityEvalReqNo);			
				if(scwin.GLOBAL_VARS.mngmtNo !== "") outputMngmtNo.setValue(scwin.GLOBAL_VARS.mngmtNo);
				
				scwin.init();
				
			};
			
			scwin.approval = function() {
			    const sn = com.getLoginInfo('sn');
			    const uid = com.getLoginInfo('uid');
			    const serviceId = com.getLoginInfo('serviceId');
			    let invntPsn = "";
			    let invntPsnNm = "";
			
			    let invntPsnClsfNm = "";
			    let blongClsfNm = "";
			    let nm = "";
			    let dataList = [];
			
			    let aYmd = inputCalendarReqYmd.getValue();
			    if (aYmd !== '') aYmd = (aYmd).substr(0, 4) + '-' + (aYmd).substr(4, 2) + '-' + (aYmd).substr(6, 2);
			
			    dlt_list01.getAllJSON().forEach(item => {
			        if (item.invnt_psn_clsf === "Y") {
			            invntPsn = item.syspayno;
			            invntPsnNm = item.nm_krn;
			            invntPsnClsfNm = "V";
			        } else {
			            invntPsnClsfNm = "";
			        }
			
			        if (item.blong_clsf === "PDT010") blongClsfNm = "내부";
			        if (item.blong_clsf === "PDT020") blongClsfNm = "외부";
			        nm = item.nm_krn + "(" + item.empno + ")";
			
			        const data = {
			            invnt_psn_clsf_nm: invntPsnClsfNm,
			            blong_clsf_nm: blongClsfNm,
			            nm: nm,
			            stake_rt: item.stake_rt,
						contac_loc: item.contac_loc,
						blong_nm: item.blong_nm
			        };
			
			        dataList.push(data);
			    });
			
			    dlt_approval.setJSON(dataList);
			
			    // 전문 데이터
			    dma_approval.set("login_user_id", uid);
			    dma_approval.set("req_no", outputPrityEvalReqNo.getValue());
			    dma_approval.set("reporter_id", sn);
			    dma_approval.set("ui_id", serviceId);
			    dma_approval.set("apr_state", "XAD010");
			    dma_approval.set("manager_id", invntPsn);
			    dma_approval.set("researcher_id", "");
			    dma_approval.set("result", "");
			    dma_approval.set("info_cd", outputInvntKrnNm.getValue());
			    dma_approval.set("ext2_cd", invntPsnNm);
			    dma_approval.set("ext1_cd", "");
			    dma_approval.set("main_key", scwin.GLOBAL_VARS.prityEvalReqNo);
			    dma_approval.set("researcher_id", invntPsn);
			    dma_approval.set("endpoint", uid);			    
			
			    // 스냅샷 데이터
			
			    dma_approval.set("mngmt_no", outputMngmtNo.getValue());
			    dma_approval.set("invnt_krn_nm", outputInvntKrnNm.getValue());
			    dma_approval.set("smitapp_no", inputSmitappNo.getValue());
			    dma_approval.set("smitapp_ymd", outputSmitappYmd.getValue());
			    dma_approval.set("req_ymd", aYmd);
			    dma_approval.set("req_nm", inputReqNm.getValue() + "(" + inputReqEmpno.getValue() + ")");
			    dma_approval.set("req_dept_nm", dma_result.get("dept_nm"));
			    dma_approval.set("req_resn", textareaReqResn.getValue());
			    dma_approval.set("rmk", inputRmk.getValue());
			    dma_approval.set("attach_file_no", dma_result.get("attach_file_no"));
			    
			    com.execute(sbm_svcpatapppriapr);
			};
			
			scwin.sbm_svcpatapppriapr_done = function(e){
				const reqNo = e.responseJSON.elData.req_no;
				com.movePageMenu("A001", { "reqNo" : reqNo});
			};
			
			scwin.init = function() {

			    inputCalendarReqYmd.setValue(scwin.GLOBAL_VARS.toDay);
			
				// 공통코드 세팅
			    var codeOptions = [ 
					 { code : "RFA", compID :"gridResult01:tech_fee_clsf"}
					,{ code : "PDT", compID :"gridResult02:blong_clsf"}
				];
			
				com.setCmnCd(codeOptions);
			
			    if (scwin.GLOBAL_VARS.prityEvalReqNo !== '' || scwin.GLOBAL_VARS.mngmtNo !== '') {
			        triggerMngmt.visible(false);
			        scwin.menu_find();
			    } else {
			        com.file.create(upload_A.getID(), { fileKey: com.getLoginInfo('serviceId'), mode: "edit", target: "IPMS" });
			    }
			    scwin.get_pay_info();
			};
			
			
			scwin.get_pay_info = function() {
				inputReqNm.setValue(com.getLoginInfo('cn'));
				inputReqEmpno.setValue(com.getLoginInfo('empno'));
			};
			
			scwin.menu_find = function() {
			    // 신청번호
			    if (scwin.GLOBAL_VARS.prityEvalReqNo === "" && scwin.GLOBAL_VARS.mngmtNo === "") {
			        com.alert("우선심사청구번호 또는 관리번호가 존재해야합니다.");
			        return;
			    }
			    //dma_search.set("prity_eval_req_no", outputPrityEvalReqNo.getValue());
			    //dma_search.set("mngmt_no", outputMngmtNo.getValue());
			    dma_search.set("doc_id", scwin.GLOBAL_VARS.docuClsf);
			
			    com.execute(sbm_svcpatappprir02); // getSelectData
			};
			
			scwin.sbm_svcpatappprir02_done = function(e) {
				
			    com.trimDataCollection(dma_result);
			    com.trimDataCollection(dlt_list01);
			    com.trimDataCollection(dlt_list02);
			
			    inputSmitappNo.setValue(dma_result.get("smitapp_no"));
			    scwin.GLOBAL_VARS.prityEvalReqNo = dma_result.get("prity_eval_req_no");
			    if (scwin.GLOBAL_VARS.prityEvalReqNo !== "") scwin.GLOBAL_VARS.cudType = "U";
