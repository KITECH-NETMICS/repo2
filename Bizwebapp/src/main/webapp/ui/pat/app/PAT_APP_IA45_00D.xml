				const req_dept_cd = dma_result.get("req_dept_cd") ? dma_result.get("req_dept_cd") : dept_cd;
				const req_dept_new_ymd = dma_result.get("req_dept_new_ymd") ? dma_result.get("req_dept_new_ymd") : dept_new_ymd;
			
			    dma_save.set("docu_clsf", scwin.GLOBAL_VARS.docuClsf);
