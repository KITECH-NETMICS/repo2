			scwin.param = {};
        
			scwin.onpageload = function() {
				// 첨부파일 컴포넌트 초기화
                com.file.init();
                scwin.param = com.getParameter();
                dma_search.setJSON(scwin.param);
                
                //dma_search.set("req_no", "I01202100175");
                //dma_search.set("mngmt_no", "KR20210295");               		
                
				com.execute(sbm_svcpatpsrinqr02);
				
			};
			
			scwin.sbm_svcpatpsrinqr02_done = function(e) {

				com.trimDataCollection(dma_patBasicInfo);
				com.trimDataCollection(dma_patManagerInfo);
				com.trimDataCollection(dlt_patInventInfo);
				com.trimDataCollection(dlt_patCoVendInfo);
				com.trimDataCollection(dlt_patInventorInfo);
				com.trimDataCollection(dlt_patPrjInfo);
				com.trimDataCollection(dlt_patPriorInfo);
				com.trimDataCollection(dlt_patDivdeInfo);
				
				const attachFileNo_A = dma_patBasicInfo.get("open_attach_file_no");				
				com.file.create(upload_A.getID(), {fileKey: attachFileNo_A ? attachFileNo_A : com.getLoginInfo('serviceId'), mode: "view" , target:"IPMS"});
				
				prityrgt_main.setValue(dma_patBasicInfo.get("prityrgt_main_rspns_no") + "/" + dma_patBasicInfo.get("prityrgt_main_rspns_ymd"));

				const t_invnt_nm = dma_patManagerInfo.get("invnt_nm") + "(" + dma_patManagerInfo.get("invnt_syspayno") + ")";
				const t_mandate_psn_nm = dma_patManagerInfo.get("mandate_psn_nm") + "(" + dma_patManagerInfo.get("mandate_psn_syspayno") + ")"
				invnt_nm.setValue(t_invnt_nm);
				mandate_psn_nm.setValue(t_mandate_psn_nm);
			};
			
			scwin.grid1_onclick = function(row, col, colId){
			      if(colId === "req_no"){
			            const req_no = dlt_patInventInfo.getRowJSON(row).req_no
			            com.openTabMenu("I110", { req_no: req_no });
			      }
			};
			
			scwin.onpageunload = function() {
				
			};
		
		]]></script>
