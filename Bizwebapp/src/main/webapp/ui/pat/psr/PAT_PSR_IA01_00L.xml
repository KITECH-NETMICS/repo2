			scwin.onpageload = function() {
			
				var codeOptions = [
				 { code : "PAT", compID :"pat_clsf"},
				 { code : "HCF", compID :"dept_typ"},
				];
				com.setCmnCd(codeOptions);
				
				scwin.search();
				
			};
			
			scwin.search = function() {
				com.execute(sbm_svcpatpsrinqr01, {
					successCallback : function(e) {
						tbx_result.setValue(dlt_list.getTotalRow());
					}
				});
			};
			
			scwin.onpageunload = function() {
				
			};
			
			scwin.menu_move = function(row, col, colId) {
				
				if(colId === "req_no" || colId === "mngmt_no"){
					const data = dlt_list.getRowJSON(row);
					com.openTabMenu("IA73", data);
				}
				/*com.openTabUrl("출원업무", "/ui/pat/ptd/PAT_PCM_IF99_00D.xml", 1, data, function(retObj) {
					// 콜백
					if(retObj){
					}
				});*/
			};
			
			scwin.btn_down_excel_onclick = function() {
				if (dlt_list.getTotalRow() > 6000)
				{
					com.alert("6000건 이상");
				} else {
					var now = com.getServerDateTime("yyyyMMdd");
					var name = '지식재산권_' + now + '.xlsx';
					
					var option = {
						    fileName : name,
						    removeColumns : ""
					};
					com.downloadGridExcel(grid, option);
				}
			};
			
						
			/*
			 *	주 발명자 검색
			 */		
			scwin.main_hum_onkeypress = function(e) {
			    if (event.keyCode == 13) {
			        scwin.selectHumBtn("main");
			    }
			};
			
			// 주 발명자 초기화
			scwin.main_hum_onchange = function(info) {
			    if (info.newValue == "") {
			        dma_search.set("main_invnt_psn_empno", "");
			        dma_search.set("main_invnt_psn_syspayno", "");
			        dma_search.set("main_invnt_psn_nm", "");
			    }
			};
			
			// 주 발명자 검색 버튼
			scwin.main_hum_onclick = function(e) {
			    scwin.selectHumBtn("main", "img");
			};
			
			/*
			 *	공동 발명자 검색
			 */		
			scwin.joint_hum_onkeypress = function(e) {
			    if (event.keyCode == 13) {
			        scwin.selectHumBtn("joint");
			    }
			};
			
			// 공동 발명자 초기화
			scwin.joint_hum_onchange = function(info) {
			    if (info.newValue == "") {
			        dma_search.set("invnt_psn_empno", "");
			        dma_search.set("invnt_psn_syspayno", "");
			        dma_search.set("invnt_psn_nm", "");
			    }
			};
			
			// 공동 발명자 검색 버튼
			scwin.joint_hum_onclick = function(e) {
			    scwin.selectHumBtn("main", "img");
			};
			
			
			scwin.selectHumBtn = function(type, data) {
				let empno = joint_empno.getValue();
				if(type === "main") empno = main_empno.getValue();
				
			    // 버튼(img)으로 팝업함수 바로 호출 처음 변수에 아무것도 넣지 않으면 최초 서브미션도 실행 안함. 
			    data == "img" ? data = { empno: "" } : data = { empno: empno, nm: empno, work_clsf: "HAG010" };
			
			    com.pop.open(com.pop.TYPE.HUM, data, function(ret) {
				    if(type === "main"){
					    dma_search.set("main_invnt_psn_empno", ret.empno);
					    dma_search.set("main_invnt_psn_syspayno", ret.syspayno);
			        	dma_search.set("main_invnt_psn_nm", ret.nm);
			        	
				    }   
				    if(type === "joint"){
				    	dma_search.set("invnt_psn_empno", ret.empno);
				    	dma_search.set("invnt_psn_syspayno", ret.syspayno);
			        	dma_search.set("invnt_psn_nm", ret.nm);
				    }
			    });
			};
			
			
			// 계정번호 검색
			scwin.accnt_onkeydown = function(event) {
			    if(event.keyCode == 13){
			        scwin.accnt_onclick();
			    }
			};
			
			// 계정번호 초기화
			scwin.accnt_onchange = function(info) {
			    if (info.newValue == "") {
			        dma_search.set("accnt_no", "");
					dma_search.set("accnt_respn_psn", "");
			    }
			};
			
			scwin.accnt_onclick = function() {
				var data = {
					accnt_no : input_accnt_no.getValue(),
					ref_yn : "Y"
				};
				
				com.pop.open(com.pop.TYPE.ACNNO, data, function(ret) {
					dma_search.set("accnt_no", ret.accnt_no);
					dma_search.set("accnt_respn_psn", ret.accnt_rspns_nm);
				});
			};
		
		]]></script>
