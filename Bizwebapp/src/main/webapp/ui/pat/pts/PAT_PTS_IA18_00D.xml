         <script type="text/javascript" lazy="false"><![CDATA[
        
			scwin.GLOBAL_VARS = {
				p_param: {},
				revParam: {}
			};
			
			scwin.onpageload = function() {
				
				scwin.GLOBAL_VARS.p_param = com.getParameter();
			
				$(".complement").wq("setStyle", "display", "none");
				$(".evaluation").wq("setStyle", "display", "none");
				
				// 첨부파일 컴포넌트 초기화
                com.file.init();
                
				dma_sel_search.set("spec_id", scwin.GLOBAL_VARS.p_param.ref_no);
			
				WFRAME_PATBIBLIOGRAPHY.getWindow().scwin.selectBibliography(scwin.GLOBAL_VARS.p_param.mngmt_no);

				// 조회
				com.execute(sbm_svcpatptsreqr06);
			};
			
			scwin.sbm_svcpatptsreqr06_done = function(e) {
				dma_search.setEmpty();
				dma_result.setEmpty();
				const result = dlt_list.getDataLength();

				if (result > 0) {
				    srh_sn_sb.setSelectedIndex(srh_sn_sb.getItemCount() - 1);
				
				    if (srh_sn_sb.getValue() == "0") {
				        dma_search.set("spec_id", scwin.GLOBAL_VARS.p_param.ref_no);
				        com.execute(sbm_svcpatptsreqr02);
				    } else {
				        dma_search.set("spec_id", scwin.GLOBAL_VARS.p_param.ref_no);
				        dma_search.set("spec_sn", srh_sn_sb.getValue());
				        com.execute(sbm_svcpatptsreqr03);
				    }
				}
				
			};
			
			scwin.search_done = function(e) {
			
				com.trimDataCollection(dma_result);
				
				scwin.GLOBAL_VARS.revParam.job_state_cd = dma_result.get("job_state_cd"); //상태코드
				scwin.GLOBAL_VARS.p_param.srhres_id = dma_result.get("srhres_id"); //선행기술 결과번호
				scwin.GLOBAL_VARS.p_param.spec_sn = dma_result.get("spec_sn");
				
				srh_sn_sb.setValue(scwin.GLOBAL_VARS.p_param.spec_sn);
				
				if (scwin.GLOBAL_VARS.p_param.spec_sn === '1') {
				    group2.hide();
				} else {
				    group2.show();
				}
				
				if (dma_result.get("eval_clsf") === '0') {
				    $(".complement").wq("setStyle", "display", "none");
				    $(".evaluation").wq("setStyle", "display", "table-row");
				} else {
				    $(".evaluation").wq("setStyle", "display", "none");
				    $(".complement").wq("setStyle", "display", "table-row");
				}
				
				const attachFileNo_A = dma_result.get("req_attach_file_no");
				const attachFileNo_B = dma_result.get("rpt_spec_attach_file");
				const attachFileNo_C = dma_result.get("rpt_announce_attach_file");
				const attachFileNo_D = dma_result.get("rereq_attach_file");
				
				com.file.create(upload_A.getID(), { fileKey: attachFileNo_A ? attachFileNo_A : com.getLoginInfo('serviceId'), mode: 'view', target: "IPMS" });
				com.file.create(upload_B.getID(), { fileKey: attachFileNo_B ? attachFileNo_B : com.getLoginInfo('serviceId'), mode: 'view', target: "IPMS" });
				com.file.create(upload_C.getID(), { fileKey: attachFileNo_C ? attachFileNo_C : com.getLoginInfo('serviceId'), mode: 'view', target: "IPMS" });
				com.file.create(upload_D.getID(), { fileKey: attachFileNo_D ? attachFileNo_D : com.getLoginInfo('serviceId'), mode: 'view', target: "IPMS" });
				
			};
			
			scwin.srh_sn_sb_onchange = function(info) {
				if(info.newValue !== ""){
					dma_search.set("spec_id", scwin.GLOBAL_VARS.p_param.ref_no);
					dma_search.set("spec_sn", info.newValue);
					com.execute(sbm_svcpatptsreqr03);
				}
			};
			
			scwin.onpageunload = function() {
				
			};
		
		]]></script>
