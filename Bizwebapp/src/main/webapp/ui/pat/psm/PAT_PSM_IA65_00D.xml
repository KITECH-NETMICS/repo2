         <script src="/cm/js/biz/pat.js" cache="false" />
         <script type="text/javascript" lazy="false"><![CDATA[
			scwin.param = {};
        
			scwin.onpageload = function() {
				// 첨부파일 컴포넌트 초기화
                com.file.init();
                
                scwin.param = com.getParameter();
                dma_search.setJSON(scwin.param);
                
                btn_save.visible(false);
                btn_close.visible(false);
                
                if(scwin.param.isPopup === "Y"){
	                btn_close.visible(true);
	                searchSection.visible(false);
                	wfm_title.visible(false);	                
                }
                
                scwin.param.modeType = "view";
                scwin.param.scrnClsf = "NEW";
                
                var codeOptions = [{ code : "PME", compID :"gridResult01:rvew_rst_cd, gridResult02:rvew_rst_cd, gridResult03:rvew_rst_cd, gridResult04:rvew_rst_cd" }];
				com.setCmnCd(codeOptions);
                
                const roleCode = com.getLoginInfo("roleCode");
                if (roleCode.indexOf("10162") > 0) {
					if(scwin.param.scrnClsf === "OLD") {
						scwin.param.modeType = "edit";
						btn_save.visible(true);
					}
				}                
				com.execute(sbm_svcpatpsmdtlr01);
				
			};
			
			scwin.sbm_svcpatpsmdtlr01_done = function(e) {

				com.trimDataCollection(dma_patPsmExam);
				com.trimDataCollection(dlt_patPsmExamArea);
				com.trimDataCollection(dlt_krExam);
				com.trimDataCollection(dlt_freeExam);
				com.trimDataCollection(dlt_etcExam);
				com.trimDataCollection(dlt_programExam);
				
				area_nm.setValue(dlt_patPsmExamArea.getRowJSON(0).area_nm || '');
				
				const attachFileNo_A = dma_patPsmExam.get("attach_file_no");
				com.file.create(upload_A.getID(), {fileKey: attachFileNo_A ? attachFileNo_A : com.getLoginInfo('serviceId'), mode: scwin.param.modeType});
				
			};
			
			scwin.grid01_cell_click = function(row, col, colId) {
				if(colId === "attach_file_no_img"){
					scwin.file_download(row, col);
				}				
			};
			
			scwin.grid02_cell_click = function(row, col, colId) {
				if(colId === "attach_file_no_img"){
					const dataObj = dlt_freeExam.getRowJSON(row);
					scwin.grid_oz_print(dataObj);
				}				
			};
			scwin.grid03_cell_click = function(row, col, colId) {
				if(colId === "attach_file_no_img"){
					const dataObj = dlt_etcExam.getRowJSON(row);
					scwin.grid_oz_print(dataObj);
				}
			};
			scwin.grid04_cell_click = function(row, col, colId) {
				if(colId === "attach_file_no_img"){
					const dataObj = dlt_programExam.getRowJSON(row);
					scwin.grid_oz_print(dataObj);
				}
			};
			
			scwin.grid_oz_print = function(dataObj) {
				
				const roleCode = com.getLoginInfo("roleCode");
				const patClsf = dataObj.pat_clsf;
				
				let ozrName = "";
				let node = "req_no=" + dataObj.key_1;
				
				if(roleCode.indexOf("1016102") > 0){ //지식재산권-관리자
					node += "&manager_yn=Y";
				}else{
					node += "&manager_yn=N";
				}
				// 특허, 실용신안
				if(patClsf == "PAT070" || patClsf == "PAT020"){
					ozrName = "I007";
				}else if(patClsf == "PAT030"){// 상표
					ozrName = "I008";
				}else if(patClsf == "PAT040"){// 디자인
					ozrName = "I009";
				}else if(patClsf == "PAT050"){// 프로그램
					ozrName = "I010";
				}
				
				com.oz.openPopup({
					ozrCode: ozrName,
					param: node
				});
					
			};
			
			scwin.save = function() {
												
				com.confirm("첨부파일 저장하시겠습니까?", function(ret) {
			        if (ret) {
			            com.file.transport(function(attachObj) {
			            	dma_save.set("attach_file_no", attachObj.upload_A);
							dma_save.set("updt_syspayno", com.getLoginInfo("sn"));
							dma_save.set("exam_id", scwin.param.exam_id);
                            
                            com.execute(sbm_svcpatpsmdtlu01, {
								successCallback : function(e) {
									com.alert("수정되었습니다.");
								}
							});
							
                        });
			        }
			    });
			};
			
			scwin.file_download = function(row, col) {
				const data = {
					req_no: dlt_krExam.getRowJSON(row).key_1,
					mngmt_no: dlt_krExam.getRowJSON(row).key_2,
					roleCode: com.getLoginInfo("roleCode"),
					searchGbn: "PSM01"
				}
				com.pop.open(biz.pat.pop.TYPE.PAT_FILEDOWNLOAD, data, function(ret) {
			    });
			};
			
			/**
			 * 닫기 버튼 클릭 이벤트
			 */
			scwin.btn_close_onclick = function(e) {
				com.closePopup();
			};
			
			/**
			 * 목록 버튼 클릭시 이벤트
			 */
			scwin.trigger_list_onclick = function(e) {
			    let menuIdToMove = "IA61";
			    com.moveList(menuIdToMove);
			};
			
			scwin.onpageunload = function() {
				
			};
		
		]]></script>
