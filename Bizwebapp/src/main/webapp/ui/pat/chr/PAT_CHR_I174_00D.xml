         <script type="text/javascript" lazy="false"><![CDATA[
         
             scwin.GLOBAL_VARS = {
                reqNo: "",       // 신청번호
                mngmtNo: "",     // 관리번호
                pStatus: "",       // 문서상태
                pRegionCentr: "",  // 지역센터
                pRoleChk: "",      // 관리자여부
                attachFileNo_A: "", // 요청내용 첨부파일번호
                attachFileNo_B: "", // 평가내역 첨부파일번호
                typeCode: com.getLoginInfo('serviceId'), // 첨부파일 유형코드
                gridRow: -1, // 그리드클릭여부
                toDay: com.getServerDateTime("yyyyMMdd"),
                patState: "", // 지재권상태
                CU_flag: "",
                patClsf: "",
                domClsf: "",
                mainParam: "",
                reqNoParam: "",
                patClsfParam: "",
                domClsfParam: "",
                cudType: "C"
            };
        
            scwin.onpageload = function() {
                
                // 첨부파일 컴포넌트 초기화
                com.file.init();
                let viewMode = "edit";            
                let $param = com.getParameter();
    
                // 20150630 지재권 질평가 마감일자 기준에 따라 질평가 수정가능여부 컨트롤     
                var codeOptions = [ { code : "FCD", compID :""} ];        
                com.setCmnCd(codeOptions, function() {  
                    var ret = com.getComponent(gcm.CMNCD_INFO.DATA_PREFIX + "FCD");
                    for(let i = 0 ; i < ret.getDataLength() ; i++){
                        if(ret.getCellData(i, gcm.CMNCD_INFO.VALUE) === 'FCD060'){
                            const ResultClsYmd = ret.getCellData(i, gcm.CMNCD_INFO.FILED_ARR[9]);
                            inputClsYmd.setValue( ResultClsYmd );
                        }
                    }
                });
            
                
                // 기술창업IP경영실 만 질평가 반영여부 수정권한 있음
                // 질평가는 다음해 2월1일 이전까지 수정가능함(공통코드: FCD, mngmt_item_4)
                if (com.getLoginInfo('deptCode') == "H363" || com.getLoginInfo('roleCode').indexOf("1016118") > 0 || com.getLoginInfo('sn') == "20130576") {
                    $(".group4").wq("setStyle", "display", "table-cell"); //질평가반영여부
                    if (Number(inputClsYmd.getValue()) > Number(scwin.GLOBAL_VARS.toDay)) {
                        radioExamReflectClsf.setDisabled(false);
                    } else {
                        radioExamReflectClsf.setDisabled(true);
                    }
                } else {
                    radioExamReflectClsf.setValue("");
                    $(".group4").wq("setStyle", "display", "none"); //질평가반영여부
                }
                $l("통프에서 넘어옴==>" + $param.param2);
                if ($param.param2 != "") {
                    dma_search.set("req_no", $param.param2);
                    com.execute(sbm_svcpatchrcasmr02); // selectData
                } else {	                
                    scwin.GLOBAL_VARS.reqNo = $p.parent().dma_param.get("reqNo");
                    scwin.GLOBAL_VARS.mngmtNo = $p.parent().dma_param.get("mngmtNo");
                    scwin.GLOBAL_VARS.pStatus = $p.parent().dma_param.get("pStatus");
                    scwin.GLOBAL_VARS.pRegionCentr = $p.parent().dma_param.get("pRegionCentr");
                    scwin.GLOBAL_VARS.pRoleChk = $p.parent().dma_param.get("pRoleChk");
                    
                    // scwin.GLOBAL_VARS.reqNo = parent.document.getElementById("inputReqNo").value;
                    // scwin.GLOBAL_VARS.mngmtNo = parent.document.getElementById("inputMngmtNo").value;
                    // scwin.GLOBAL_VARS.pStatus = parent.document.getElementById("inputStatus").value;
                    // scwin.GLOBAL_VARS.pRegionCentr = parent.document.getElementById("inputRegionCentr").value;
                    // scwin.GLOBAL_VARS.pRoleChk = parent.document.getElementById("inputRoleChk").value;
                
                    if (scwin.GLOBAL_VARS.pRoleChk != "true") {
                        // 관리자 가 아닌 경우 저장버튼 숨기기
                        btn_save.setDisabled(true);
                        // 첨부파일은 다운로드만 가능하게
                        viewMode = "view";
                    }
                    if ((scwin.GLOBAL_VARS.reqNo != '') && (scwin.GLOBAL_VARS.mngmtNo != '')) {
                        scwin.menu_find();
                    } else {
                        // 우수특허구분 기본값으로 N 선택
                        radioExcPatYn.setValue("N");
                    }
                }
                com.file.create(upload_A.getID(), {fileKey: com.getLoginInfo('serviceId'), mode: viewMode});
                com.file.create(upload_B.getID(), {fileKey: com.getLoginInfo('serviceId'), mode: viewMode});
                
            };
            
            scwin.sbm_svcpatchrcasmr02_done = function(e){
                // msgPatSmitappSS02_02, PatSmitappVo
                // 출원지시 정보
                scwin.GLOBAL_VARS.reqNo = dma_rnecmt.get("req_no");
                scwin.GLOBAL_VARS.mngmtNo = dma_rnecmt.get("mngmt_no");
                scwin.GLOBAL_VARS.patClsf = dma_rnecmt.get("pat_clsf");
                scwin.GLOBAL_VARS.domClsf = dma_rnecmt.get("dom_oversea_clsf"); 
                scwin.GLOBAL_VARS.mainParam = scwin.GLOBAL_VARS.reqNo;
                scwin.GLOBAL_VARS.reqNoParam = scwin.GLOBAL_VARS.reqNo;
                scwin.GLOBAL_VARS.patClsfParam = scwin.GLOBAL_VARS.patClsf;
                scwin.GLOBAL_VARS.domClsfParam = scwin.GLOBAL_VARS.domClsf;
                
                scwin.menu_find();
            };
            
            scwin.sbm_svcpatchrcassr01_done = function(e) {
                const totalCount = e.responseJSON.elData.totalCount;
                if(totalCount > 0){
                    // 등록완료보고 정보                
                    scwin.GLOBAL_VARS.CU_flag = "U";
                                      
                    if(dma_result.get("regst_decsn_ymd") != ""){
                        inputCalendarRegstDecsnYmd.setValue(dma_result.get("regst_decsn_ymd"));  // 등록결정일자
                    }
                    
                    inputRegstNo.setValue(dma_result.get("regst_no")); // 등록번호
                    
                    if(dma_result.get("regst_ymd") != "" ){
                        inputCalendarRegstYmd.setValue(dma_result.get("regst_ymd")); // 등록일자
                    }                    
                    if(dma_result.get("regst_reg_ymd") != "" ){
                        inputCalendarRegstRegYmd.setValue(dma_result.get("regst_reg_ymd")); // 등록실적년월
                    }                   
                    
                    textareaCompltRptRmk.setValue(dma_result.get("complt_rpt_rmk"));  // 사건내역

                    if(dma_result.get("exc_pat_yn") != "" ){
                        var excPatYn = dma_result.get("exc_pat_yn");
                        radioExcPatYn.setValue(excPatYn);      // 우수특허구분
                    }
                    
                    if(dma_result.get("eval_grd") != "" ){
                        selectboxEvalGrd.setValue(dma_result.get("eval_grd"));// 평가등급
                    }
                                        
                    if(dma_result.get("eval_ymd") != "" ){
                        inputCalendarEvalYmd.setValue(dma_result.get("eval_ymd"));  // 평가일자
                    }
                    
                    scwin.GLOBAL_VARS.patState = dma_result.get("state"); // 상태값
                    
                    com.file.init();
                      
                    // 첨부 파일
                    scwin.GLOBAL_VARS.attachFileNo_A = dma_result.get("attach_file_no");
                    if(scwin.GLOBAL_VARS.attachFileNo_A != ""){
                        com.file.create(upload_A.getID(), {fileKey: scwin.GLOBAL_VARS.attachFileNo_A ? scwin.GLOBAL_VARS.attachFileNo_A : com.getLoginInfo('serviceId'), mode: 'edit'});
                    }
                    
                    // 평가내역 첨부
                    scwin.GLOBAL_VARS.attachFileNo_B = dma_result.get("attach_file_no2");
                    if(scwin.GLOBAL_VARS.attachFileNo_B != ""){
                        com.file.create(upload_B.getID(), {fileKey: scwin.GLOBAL_VARS.attachFileNo_B ? scwin.GLOBAL_VARS.attachFileNo_B : com.getLoginInfo('serviceId'), mode: 'edit'});
                    }
                    // 등록비용발생금액
                    inputRegstDecsnAmt.setValue(dma_result.get("regst_decsn_amt"));
                    // 등록비용지급일
                    if(dma_result.get("regst_decsn_amt_ymd") != ""){
                       inputCalendarRegstDecsnAmtYmd.setValue(dma_result.get("regst_decsn_amt_ymd"));
                    } 
                    
                    radioExamReflectClsf.setValue(dma_result.get("exam_reflect_clsf"));  // 20150630 질평가반영여부               
                }else{
                    scwin.GLOBAL_VARS.CU_flag = "I";
                }
            };
            
            scwin.menu_find = function() {
                dma_search.set("req_no", scwin.GLOBAL_VARS.reqNo);
                dma_search.set("mngmt_no", scwin.GLOBAL_VARS.mngmtNo);
                com.execute(sbm_svcpatchrcassr01); // selectData
            };
            
            scwin.menu_delete = function() {
                com.confirm("message.xom.wq.022" /*삭제 하시겠습니까?*/ , function(ret) {
                    if (ret) {
                        dma_delete.set("req_no", scwin.GLOBAL_VARS.reqNo);
                        dma_delete.set("mngmt_no", scwin.GLOBAL_VARS.mngmtNo);
                        dma_delete.set("state_code", "30500");
                        dma_delete.set("updt_syspayno", com.getLoginInfo('sn'));
                        dma_delete.set("prcs_clsf", "delete");          
                        
                        com.execute(sbm_svcpatchrcasid02, {
                            successCallback : function(e) {
                                com.alert("등록내역을 삭제하였습니다.");
                                scwin.GLOBAL_VARS.CU_flag = "U";
                                scwin.menu_find();
                            }
                        });
                    }
               });
            };
            
            scwin.pre_save = function() {
                // 등록결정일자
                var regstDecsnYmd = inputCalendarRegstDecsnYmd.getValue();
                if(regstDecsnYmd == ''){
                    com.alert("등록결정일자를 입력해 주시기 바랍니다.");                 
                    return false;   
                } 
                // 등록일자
                var regstYmd = inputCalendarRegstYmd.getValue();
                if(regstYmd == ''){
                    com.alert("등록일자를 입력해 주시기 바랍니다.");
                    return false;   
                }   
                
                // 등록번호
                var regstNo = inputRegstNo.getValue();
                if(regstNo == ''){
                    com.alert("등록번호를 입력해 주시기 바랍니다.");
                    return false;
                }
                
                // 등록실적년월
                var regstRegYmd = inputCalendarRegstRegYmd.getValue();
                if(regstYmd != ''){
                    if(regstRegYmd == ''){
                        com.alert("등록실적년월을 입력해 주시기 바랍니다.");
                        return false;   
                    }
                }
                return true;
            };
            
            scwin.menu_save_onclick = function(){
                if (!scwin.pre_save()) return;
                com.confirm("message.xom.wq.017" /*저장 하시겠습니까?*/ , function(ret) {
                    if (ret) {
                        com.file.transport(function(attachObj) {
                            scwin.menu_save(attachObj);
                        });
                    }
               });
            };
            
            scwin.menu_save = function(attachObj) {
                if (!scwin.pre_save()) return;
               
                // 현재 상태값이 "등록완료보고"보다 아래인 경우, 상태값 변경      
                let state = "";      
                if(patState > '30500'){
                    if(scwin.GLOBAL_VARS.patState != ""){
                        state = scwin.GLOBAL_VARS.patState;
                    }else{
                        state = scwin.GLOBAL_VARS.pStatus;
                    }                
                }else{
                    state = '30500';
                }           
                
                if((scwin.GLOBAL_VARS.reqNo != '') && (scwin.GLOBAL_VARS.mngmtNo != '')) scwin.GLOBAL_VARS.cudType = "U";
                
                dma_save.set("cud_type", scwin.GLOBAL_VARS.cudType);
                dma_save.set("req_no", scwin.GLOBAL_VARS.reqNo);
                dma_save.set("mngmt_no", scwin.GLOBAL_VARS.mngmtNo);
                dma_save.set("state", state);
                dma_save.set("regst_decsn_ymd", inputCalendarRegstDecsnYmd.getValue()); // 등록결정일자
                dma_save.set("exc_pat_yn", radioExcPatYn.getValue()); // 우수특허구분
                dma_save.set("eval_grd", selectboxEvalGrd.getValue()); // 평가등급
                dma_save.set("eval_ymd", inputCalendarEvalYmd.getValue()); // 평가일자
                dma_save.set("complt_rpt_ymd", scwin.GLOBAL_VARS.toDay); // 등록완료보고
                
                // patmastr update에도 사용됨
                dma_save.set("regst_no", inputRegstNo.getValue()); // 등록번호
                dma_save.set("regst_ymd", inputCalendarRegstYmd.getValue()); // 등록일자
                dma_save.set("regst_reg_ymd", inputCalendarRegstRegYmd.getValue()); // 등록실적년월
                
                // 전체사건내역 등록 노드
                dma_save.set("state_code", "30500"); // 전체사건내역(상태코드)
                dma_save.set("proc_ymd", inputCalendarRegstYmd.getValue()); // 전체사건내역(처리일자)
                dma_save.set("state_nm", "등록완료보고"); // 전체사건내역(발생구분명)
                dma_save.set("rmk", textareaCompltRptRmk.getValue()); // 전체사건내역(내역)                        
            
                dma_save.set("attach_file_no", attachObj.upload_A);
                dma_save.set("attach_file_no2", attachObj.upload_A);
                dma_save.set("regst_decsn_amt", inputRegstDecsnAmt.getValue()); // 등록비용지급금액
                
                dma_save.set("regst_decsn_amt_ymd", inputCalendarRegstDecsnAmtYmd.getValue()); // 등록비용지급일자
                dma_save.set("complt_rpt_rmk", textareaCompltRptRmk.getValue()); // 사건내역
                dma_save.set("regst_syspayno", com.getLoginInfo('sn'));
                dma_save.set("updt_syspayno", com.getLoginInfo('sn'));
                dma_save.set("exam_reflect_clsf", radioExamReflectClsf.getValue()); // 질평가반영여부

                com.execute(sbm_svcpatchrcasid01, {
                    successCallback : function(e) {
                        if(scwin.GLOBAL_VARS.CU_flag == "I"){
                            scwin.send_apr();
                        }   
                        com.alert ("등록완료하였습니다.");
                        scwin.GLOBAL_VARS.CU_flag = "U";
                    }
                });
            };
            
            
            scwin.send_apr = function() {
                                
                dma_apr_send.set("login_user_id", com.getLoginInfo('uid'));
                dma_apr_send.set("main_key", scwin.GLOBAL_VARS.mainParam);
                dma_apr_send.set("req_no", scwin.GLOBAL_VARS.reqNoParam);
                dma_apr_send.set("pat_clsf_cd", scwin.GLOBAL_VARS.patClsfParam);
                dma_apr_send.set("dom_oversea_clsf_cd", scwin.GLOBAL_VARS.domClsfParam);
                dma_apr_send.set("reporter_id", com.getLoginInfo('sn'));
                dma_apr_send.set("ui_id", "I174");
                
                com.execute(sbm_svcpatchrcasapr1);
                
            }
            
            
            scwin.onpageunload = function() {
                
            };
        
]]></script>
