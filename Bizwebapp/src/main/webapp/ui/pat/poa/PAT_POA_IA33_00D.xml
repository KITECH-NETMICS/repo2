         <script type="text/javascript" lazy="false"><![CDATA[
        
			scwin.param = {};
			
			scwin.onpageload = function() {
			
				// 첨부파일 컴포넌트 초기화
                com.file.init();
                
                triggerUpdate.hide();

				com.biz.checkRole({
				    serviceId: "IA33",
				    roleCodes: com.getLoginInfo('roleCode')
				}, function(e) {
				    e.isRoleYn ? triggerUpdate.show() : triggerUpdate.hide();
				});
				
                scwin.param = com.getParameter();
                dma_sel_search.set("oa_id", scwin.param.ref_no);
        		WFRAME_PATBIBLIOGRAPHY.getWindow().scwin.selectBibliography(scwin.param.mngmt_no);
        		
        		com.execute(sbm_svcpatpoapatr01);
			};
			
			scwin.sbm_svcpatpoapatr01_done = function(e) {
			
				const result = dlt_list.getDataLength();
				
				scwin.menu_find();

				if (result > 0) {
				    oa_sn_sb.setSelectedIndex(oa_sn_sb.getItemCount() - 1);
				    scwin.menu_find();
				}
								
			};
			
			scwin.menu_find = function() {
				dma_search.set("oa_id", scwin.param.ref_no);
				const oasnsb = oa_sn_sb.getValue();
				if (isNaN(Number(oasnsb))) {
					console.log("파라미터가 없습니다.");
					return;
				}else{
					dma_search.set("oares_sn", oasnsb);
				}
				
				com.execute(sbm_svcpatpoapatr03);
			};
			
			scwin.sbm_svcpatpoapatr03_done = function(e) {
				com.trimDataCollection(dma_result);
				
				var job_state_cd = dma_result.get("job_state_cd").replace(/ /gi, "");
				
				const attachFileNo_A = dma_result.get("rcpt_attach_file");
				const attachFileNo_B = dma_result.get("res_file");
				const attachFileNo_C = dma_result.get("res_attach_file");
				const attachFileNo_D = dma_result.get("rpn_attach_file");
				
				if (dma_result.get("eval_res_clsf") === '8') {
				    complement.hide();
				    $(".evaluation").wq("setStyle", "display", "table-row");
				} else {
				    $(".evaluation").wq("setStyle", "display", "none");
				    $("#complement").wq("setStyle", "display", "table-row");
				}
				
				var oares_sn = dma_result.get("oares_sn").replace(/ /gi, "");
				oa_sn_sb.setValue(oares_sn);
				
				var stateGbn = ["P1501", "P1502", "P1503", "P1504"].includes(job_state_cd);
				
				if (oares_sn === "1") {
				    if (oa_sn_sb.getItemCount() === 1) { // 보완요청X
				        stateGbn ? 
				        scwin.showGroups([false, true, true, true, true]) : 
				        scwin.showGroups([false, true, true, true, false]);
				    } else { // 보완요청O
				        scwin.showGroups([false, true, true, false, false]);
				    }
				} else {
				    if (oa_sn_sb.getItemCount() === oares_sn) { // 마지막건
				        stateGbn ? 
				        scwin.showGroups([true, true, true, true, true]) : 
				        scwin.showGroups([true, true, true, true, false]);
				    } else {
				        scwin.showGroups([true, true, true, false, false]);
				    }
				}
				
				com.file.create(upload_A.getID(), {fileKey: attachFileNo_A ? attachFileNo_A : com.getLoginInfo('serviceId'), mode: "view" , target:"IPMS"});
				com.file.create(upload_B.getID(), {fileKey: attachFileNo_B ? attachFileNo_B : com.getLoginInfo('serviceId'), mode: "view" , target:"IPMS"});
				com.file.create(upload_C.getID(), {fileKey: attachFileNo_C ? attachFileNo_C : com.getLoginInfo('serviceId'), mode: "view" , target:"IPMS"});
				com.file.create(upload_D.getID(), {fileKey: attachFileNo_D ? attachFileNo_D : com.getLoginInfo('serviceId'), mode: "view" , target:"IPMS"});
				
			};
			
			scwin.showGroups = function (states) {
			  [group3, group4, group5, group6, group7].forEach(function (group, index) {
			    states[index] ? group.show() : group.hide();
			  });
			};
			
			scwin.update_attach_file_no = function() {
			
				const srhId =  scwin.param.ref_no;
				const attachFileNo_A = dma_result.get("rcpt_attach_file");
				const attachFileNo_B = dma_result.get("res_file");
				const attachFileNo_C = dma_result.get("res_attach_file");
				const attachFileNo_D = dma_result.get("rpn_attach_file");
				
				const data = {
				    screen_id: 'I18',
				    srh_id: srhId,
				    attach_file_no_a: attachFileNo_A,
				    attach_file_no_b: attachFileNo_B,
				    attach_file_no_c: attachFileNo_C,
				    attach_file_no_d: attachFileNo_D
				}
			    
			    const PAT_ATTACH = { OPTION : { width: 750, height: 600, id: "PatAttachMngPop", popupName: "지재권 첨부파일 수정"},  URL : "/ui/pat/pop/PAT_POP_I110_07P.xml"};
			    com.pop.open(PAT_ATTACH, data, function(ret) {
			    				    
			    });
			};
			
			scwin.save_onclick = function(){		            
				
                com.confirm("저장 하시겠습니까?", function(ret) {
                    if (ret) {
                    	com.file.transport(function(attachObj) {
                            scwin.save(attachObj);
                        });
                    }
               });
            };
            
            scwin.save = function(attachObj) {
            	dma_save.set("rcpt_attach_file", attachObj.upload_A );
				dma_save.set("res_file", attachObj.upload_B );
				dma_save.set("res_attach_file", attachObj.upload_C );
				dma_save.set("rpn_attach_file", attachObj.upload_D );
				dma_save.set("oa_id", scwin.param.ref_no);
				dma_save.set("mngmt_no", scwin.param.mngmt_no);
				dma_save.set("oares_sn", oa_sn_sb.getValue());
				
				com.execute(sbm_svcpatpoapatm02, {
					successCallback : function(e) {
						com.alert("저장되었습니다.");
						$p.parent().scwin.trigger_list_onclick();
					}
				});
				
            };
			
			scwin.onpageunload = function() {
				
			};
		
		]]></script>
