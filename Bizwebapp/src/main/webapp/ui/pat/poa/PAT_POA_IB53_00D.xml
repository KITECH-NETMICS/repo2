         <script src="/cm/js/biz/pat.js" cache="false" />
         <script type="text/javascript" lazy="false"><![CDATA[
        
			scwin.GLOBAL_VARS = {};
			
			scwin.onpageload = function() {
			
				// 첨부파일 컴포넌트 초기화
                com.file.init();
			
				scwin.GLOBAL_VARS = {
					reqNo: com.getParameter().req_no || "",	// 신청번호			
					paramXml: com.getParameter().paramXml || "", // 이전 화면에서 넘어온 조회조건 데이터
					p_xml_id: com.getParameter().xml_id || "", // 이전화면ID
					aprState: "", // 결재상태코드	
					param2: com.getParameter().param2 || "",
					param4: com.getParameter().param4 || "",
					oaId: "",
					oaSn: "",
					invntSysPayNo: ""
				}
				
				if(scwin.GLOBAL_VARS.param4 === "apr"){
					if(scwin.GLOBAL_VARS.param2 !== ""){
						scwin.GLOBAL_VARS.reqNo = param2;
					}				
					scwin.GLOBAL_VARS.aprState = com.getParameter().param3;
				}
				
				// PDT 소속구분 공통코드 세팅
				var codeOptions = [ 
					{ code : "PDT", compID :""}
				];
				com.setCmnCd(codeOptions);
				
				const empno = com.getLoginInfo("empno");
				const sn = com.getLoginInfo("sn");
				const cn = com.getLoginInfo("cn");
				
				if (scwin.GLOBAL_VARS.reqNo == "") {
				    inputReqEmpno.setValue(empno);
				    inputReqNm.setValue(cn);
				    // inputReqPsn.setValue(sn);
				} else {
				    inputReqNo.setValue(reqNo);
				    scwin.menu_find();
				}
			};
			
			scwin.menu_find = function(){
				dma_search.set("oa_id", scwin.GLOBAL_VARS.oaId);
				com.execute(sbm_svcpatpoaprdr02);
			};
			
			scwin.sbm_svcpatpoaprdr02_done = function(e) {
			
				com.trimDataCollection(dma_result);
			
				scwin.GLOBAL_VARS.invntSysPayNo = dma_result.get("invnt_syspayno");
				scwin.GLOBAL_VARS.oaId = dma_result.get("oa_id");
			    scwin.GLOBAL_VARS.oaSn = dma_result.get("oa_sn");
			  
			    outputOaSn.setValue(scwin.GLOBAL_VARS.oaSn + "차");
			
			    if (scwin.GLOBAL_VARS.reqNo !== "") {
			        triggerMngmt.hide();
			        inputSmitappNo.setReadOnly(true);
			        inputReqNo.setValue(dma_result.get("req_no"));
			    }
				// TO-DO
			    com.biz.disabledButtons(scwin.GLOBAL_VARS.aprState, dma_result.get("req_no"));
			    var attachFileNo_A = dma_result.get("attach_file_no");
			
			    var vMode = "edit";
			    if (scwin.GLOBAL_VARS.aprState >= "XAD020") {
			        vMode = "view";
			    }
			    com.file.create(upload_A.getID(), { fileKey: attachFileNo_A ? attachFileNo_A : com.getLoginInfo('serviceId'), mode: vMode, target: "IPMS" });
			};
			
			/**
			 * 신청서 저장 클릭
			 */
			scwin.save_onclick = function() {
				dma_save.set("oa_id", scwin.GLOBAL_VARS.oaId);
			    dma_save.set("long_term", radioLongTerm.getValue());
			    dma_save.set("req_psn", com.getLoginInfo("sn"));
			    dma_save.set("chng_rsn", textareaReqResn.getValue());
			    dma_save.set("bf_chng_ymd", inputCalendarBfChngYmd.getValue());
			    dma_save.set("chng_ymd", inputCalendarChngYmd.getValue());
			    dma_save.set("rmk", inputRmk.getValue());
			    dma_save.set("req_no", inputReqNo.getValue());
			    dma_save.set("syspayno", com.getLoginInfo("sn"));
			    
			    // interface
			    dma_save.set("ui_id", com.getLoginInfo("serviceId"));
			    dma_save.set("endpoints", com.getLoginInfo("uid"));
			    dma_save.set("apr_state", "");
			    dma_save.set("mngmt_item_7", "");
				
				if (!com.validateData(dma_save, [
		            { id: "long_term", mandatory: true },
		            { id: "chng_rsn", mandatory: true }
		        ], false)) return;
				
				
			    com.confirm("저장 하시겠습니까?", function(ret) {
			        if (ret) {
			            com.file.transport(function(attachObj) {
			            	dma_save.set("attach_file_no", attachObj.upload_A);
			            	com.execute(sbm_svcpatpoaprdm01);
			            });
			        }
			    });
			};
			
			scwin.sbm_svcpatpoaprdm01_done = function(e) {
			    const reqNo = e.responseJSON.elData.req_no;
			    com.alert("저장하였습니다.");
			
			    inputReqNo.setValue(reqNo);
			    scwin.menu_find();
			};
			

			/**
			 * 신청서 결재 클릭
			 */
			scwin.approval_onclick = function() {
			    com.confirm("결재 하시겠습니까?", function(ret) {
			        if (ret) {
			            scwin.approval();
			        }
			    });
			};
			
			
			/**
			 * 신청서 결재
			 */
			scwin.approval = function() {
			
			    dma_approval.set("login_user_id", com.getLoginInfo('uid'));
			    dma_approval.set("req_no", inputReqNo.getValue());
			    dma_approval.set("reporter_id", com.getLoginInfo('sn'));
			    dma_approval.set("ui_id", com.getLoginInfo('serviceId'));
			    dma_approval.set("researcher_id", scwin.GLOBAL_VARS.invntSysPayNo);
			    dma_approval.set("apr_state", scwin.GLOBAL_VARS.aprState);
				dma_approval.set("info_cd", outputInvntKrnNm.getValue());
				dma_approval.set("ext1_cd", "");
				dma_approval.set("ext2_cd", "");
				dma_approval.set("ext3_cd", "");
			    dma_approval.set("result", "");
			
			    dma_approval.set("smitapp_no", inputSmitappNo.getValue());
			    dma_approval.set("req_sn", inputReqSn.getValue());
			    dma_approval.set("invnt_krn_nm", outputInvntKrnNm.getValue());
			    dma_approval.set("smitapp_ymd", outputSmitappYmd.getFormatValue());
			    dma_approval.set("mngmt_no", outputMngmtNo.getValue());
			    dma_approval.set("bf_chng_ymd", inputCalendarBfChngYmd.getFormattedValue());
			    dma_approval.set("long_term", radioLongTerm.getValue());
			    dma_approval.set("nm", inputReqNm.getValue());
			    dma_approval.set("dept_nm", dma_result.get("dept_nm"));
			    dma_approval.set("chng_rsn", textareaReqResn.getValue());
			    dma_approval.set("rmk", inputRmk.getValue());
			    dma_approval.set("oa_sn", scwin.GLOBAL_VARS.oaSn);
			    dma_approval.set("rcpt_oa_date", outputRcptOaDate.getFormatValue());
			    dma_approval.set("rcpt_clsf_nm", outputRcptClsfNm.getValue());
			    dma_approval.set("office_nm", outputPatOficNm.getValue());
			    dma_approval.set("doc_no", outputReqNo.getValue()); // 발명신고서번호
			    dma_approval.set("smitapp_natn_nm", outputSmitappNatnNm.getValue()); // 대상국가
			    dma_approval.set("pat_clsf_nm", outputPatClsfNm.getValue()); // 권리구분
			    dma_approval.set("smitapp_occr_clsf_nm", outputSmitappOccrClsfNm.getValue()); // 출원구분
			    dma_approval.set("rcpt_due_date", outputRcptDueDate.getFormatValue()); //OA대응마감일
			    dma_approval.set("req_ymd", outputReqYmd.getFormatValue());
			
			    dma_approval.set("attach_file_no", dma_result.get("attach_file_no"));
			
			    com.execute(sbm_svcpatpoaprdapr);
			};
			
			scwin.sbm_svcpatpoaprdapr_done = function(e) {
			    const reqNo = e.responseJSON.elData.req_no;
			    com.movePageMenu("A001", { "reqNo" : reqNo});
			};
			
			/**
			 * 신청서 삭제 클릭
			 */
			scwin.del_onclick = function() {
			    com.confirm("삭제 하시겠습니까?", function(ret) {
			        if (ret) {
			            scwin.delete_proc();
			        }
			    });
			};
			
			
			/**
			 * 신청서 삭제
			 */
			scwin.delete_proc = function() {
			    dma_delete.set("req_no", inputReqNo.getValue());
			    com.execute(sbm_svcpatpoaprdd01);
			};
			
			scwin.sbm_svcpatpoaprdd01_done = function(e) {
			    const reqNo = e.responseJSON.elData.req_no;
			    com.alert(reqNo + " 삭제 하였습니다");
			    if (scwin.GLOBAL_VARS.param4 === "apr") {
			        // 결재 진행화면
			        com.movePageMenu("A005");
			    } else {
			        // 업무화면								
			        scwin.trigger_list_onclick();
			    }
			
			};
			
			scwin.smitapp_popup_open = function(){

				const data ={
				  flag : "prdChng"
				};
			
				com.pop.open(biz.pat.pop.TYPE.RELATED_SMITAPP_NO, data, function(ret) {
					outputMngmtNo.setValue(ret.mngmt_no);
					inputSmitappNo.setValue(ret.smitapp_no);
					scwin.GLOBAL_VARS.oaId = ret.oa_id;
					outputOaSn.setValue(ret.oa_sn + "차");
					inputCalendarBfChngYmd.setValue(ret.rcpt_due_date);
					dma_result.setEmptyValue();
					scwin.menu_find();
				});
			
			};
			
			scwin.radio_longTerm_onchange = function(info){
			  var bfChngYmd = inputCalendarBfChngYmd.getValue();
			  if (bfChngYmd !== "") {
			      // bfChngYmd = 20210411일 때 한달뒤는 20210511이 나오도록 날짜에 +1을 해줌
			      var now = new Date(bfChngYmd.substring(0, 4), bfChngYmd.substring(4, 6) - 1, Number(bfChngYmd.substring(6, 8)) + 1);
			      now.setMonth(now.getMonth() + Number(this.getValue()));
			
			      var strDate = now.toISOString().substring(0, 10).replace(/\-/g, "");
			      inputCalendarChngYmd.setValue(strDate);
			  }
			};
			
						
			/**
			 * 목록 버튼 클릭시 이벤트
			 */
			scwin.trigger_list_onclick = function(e) {
			    let menuIdToMove = "IB52";
			    com.moveList(menuIdToMove);
			};
			
			
			/*
		     * 인쇄 버튼 클릭 이벤트
		     */
			scwin.btn_print_onclick = function(e) {
				var node = "req_no=";
				com.oz.openPopup({
					ozrCode : "IB52",
					param: node
		        });
			};
			
			scwin.onpageunload = function() {
				
			};
		
		]]></script>
    </head>
    <body ev:onpageload="scwin.onpageload" ev:onpageunload="scwin.onpageunload">
    	<w2:wframe id="wfm_title" src="/cm/xml/contentTitle.xml" style=""></w2:wframe>
    	<xf:group class="pageWrap" id="" style="">
