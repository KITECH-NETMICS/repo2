         <script type="text/javascript" lazy="false"><![CDATA[
        
			scwin.GLOBAL_VARS = {
				p_param: com.getParameter(),
				revParam: {}
			};
        
			scwin.onpageload = function() {
				// 첨부파일 컴포넌트 초기화
                com.file.init();
                
				$(".evaluation").wq("setStyle", "display", "none");				
				complement.hide();
                               
                dma_search.set("oa_id", scwin.GLOBAL_VARS.p_param.ref_no);
        		WFRAME_PATBIBLIOGRAPHY.getWindow().scwin.selectBibliography(scwin.GLOBAL_VARS.p_param.mngmt_no);
        		
        		com.execute(sbm_svcpatpoapatr05);
			};
			
			scwin.sbm_svcpatpoapatr05_done = function(e) {
				
				com.trimDataCollection(dma_result);
			
				scwin.GLOBAL_VARS.revParam.job_state_cd = dma_result.get("job_state_cd");
				scwin.GLOBAL_VARS.revParam.oares_sn = dma_result.get("oares_sn");
				scwin.GLOBAL_VARS.revParam.oares_id = dma_result.get("oares_id");
				scwin.GLOBAL_VARS.revParam.oa_sn = dma_result.get("oa_sn");
				scwin.GLOBAL_VARS.revParam.res_clsf = dma_result.get("res_clsf");
				
				eval_res_clsf.addItem('9', '보완요청', 0);
				
				if (dma_result.get("res_clsf") === '1') { // 의견제출통지
				    eval_res_clsf.addItem('0', '진행', 1);
				    eval_res_clsf.addItem('8', '포기', 2);
				} else if (dma_result.get("res_clsf") === '2') { // 거절결정통지
				    eval_res_clsf.addItem('2', '진행', 1);
				    eval_res_clsf.addItem('8', '포기', 2);
				} else { // 국외
				    eval_res_clsf.addItem('4', '대응진행', 1);
				    eval_res_clsf.addItem('8', '포기', 2);
				}
				
				eval_res_clsf.setValue('9');
				
				const attachFileNo_A = dma_result.get("res_file");
				const attachFileNo_B = dma_result.get("res_attach_file");
				com.file.create(upload_A.getID(), {fileKey: attachFileNo_A ? attachFileNo_A : com.getLoginInfo('serviceId'), mode: "view" , target:"IPMS"});
				com.file.create(upload_B.getID(), {fileKey: attachFileNo_B ? attachFileNo_B : com.getLoginInfo('serviceId'), mode: "view" , target:"IPMS"});
				
				
				eval_psn_syspaynm.setValue(com.getLoginInfo('cn'));
				eval_daytm.setValue(com.getServerDateTime("yyyyMMdd"));
				
			};
			
			scwin.save_onclick = function() {
			
				const sn = com.getLoginInfo('sn');
			
				dma_save.set("req_no", scwin.GLOBAL_VARS.p_param.req_no);
				dma_save.set("mngmt_no", scwin.GLOBAL_VARS.p_param.mngmt_no);
				dma_save.set("oa_id", scwin.GLOBAL_VARS.p_param.ref_no);
				dma_save.set("oares_id", scwin.GLOBAL_VARS.revParam.oares_id);
				dma_save.set("oares_sn", scwin.GLOBAL_VARS.revParam.oares_sn);
				dma_save.set("oa_sn", scwin.GLOBAL_VARS.revParam.oa_sn);
				dma_save.set("psn_syspayno", sn);
				if (eval_res_clsf.getValue() === '0' || eval_res_clsf.getValue() === '2') {
				    dma_save.set("eval_res_clsf", scwin.GLOBAL_VARS.revParam.res_clsf);
				} else {
				    dma_save.set("eval_res_clsf", eval_res_clsf.getValue());
				}
				dma_save.set("act_blong_clsf", "0");
				dma_save.set("act_sn", scwin.GLOBAL_VARS.revParam.oa_sn);
			
				dma_save.set("ref_key", scwin.GLOBAL_VARS.p_param.ref_no);
				dma_save.set("curr_state_cd", scwin.GLOBAL_VARS.revParam.job_state_cd);
				dma_save.set("syspayno", sn);
				dma_save.set("bf_state", scwin.GLOBAL_VARS.revParam.job_state_cd);
			
				if (eval_res_clsf.getValue() != '8') {
				    if (eval_res_clsf.getValue() == '9') { // 보완요청
				        dma_save.set("state", "P1412");
				        dma_save.set("state", "P1412");
				        dma_save.set("next_state_cd", "P1412");
				        dma_save.set("job_state_cd", "P1412");
				    } else {
				        dma_save.set("state", "P1415");
				        dma_save.set("state", "P1415");
				        dma_save.set("next_state_cd", "P1415");
				        dma_save.set("job_state_cd", "P1415");
				    }
				    dma_save.set("rereq_psn_syspayno", sn);
				    dma_save.set("rereq_daytm", eval_daytm.getValue());
				    dma_save.set("rereq_comment", rereq_comment.getValue());
			
				} else { // 포기
			
				    dma_save.set("state", "P9000");
				    dma_save.set("state", "P9000");
				    dma_save.set("next_state_cd", "");
				    dma_save.set("job_state_cd", "P9000");
			
				    dma_save.set("eval_psn_syspayno", sn);
				    dma_save.set("eval_daytm", eval_daytm.getValue());
				    dma_save.set("eval_result_scr", eval_result_scr.getValue());
				    dma_save.set("eval_service_scr", eval_service_scr.getValue());
				    dma_save.set("eval_comment", eval_comment.getValue());
			
				}
			
			
				if (eval_res_clsf.getValue() === '9') {
			
				    if (!com.validateData(dma_save, [
				            { id: "eval_res_clsf", mandatory: true },
				            { id: "rereq_comment", mandatory: true }
				        ], false)) return;
			
				}
			
				if (eval_res_clsf.getValue() === '8') {
			
				    if (!com.validateData(dma_save, [
				            { id: "eval_res_clsf", mandatory: true },
				            { id: "eval_comment", mandatory: true },
				            { id: "eval_result_scr", mandatory: true },
				            { id: "eval_service_scr", mandatory: true },
				        ], false)) return;
			
				}
			
			    com.confirm("저장 하시겠습니까?", function(ret) {
			        if (ret) {
			            scwin.save();
			        }
			    });
			};
			
			
			scwin.save = function() {
				com.execute(sbm_svcpatpoapatm04, {
					successCallback : function(e) {
						com.alert("저장되었습니다.");
						$p.parent().scwin.trigger_list_onclick();
					}
				});
			};
			

			
			scwin.eval_res_clsf_onchange = function(info) {
			    if (info.newValue === '8') {
			    	complement.hide();
			        $(".evaluation").wq("setStyle", "display", "table-row");
			    } else {
			        $(".evaluation").wq("setStyle", "display", "none");
					complement.show("table-row");
			    }
			};
			
			scwin.onpageunload = function() {
				
			};
		
		]]></script>
