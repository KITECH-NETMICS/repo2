         <script type="text/javascript" lazy="false"><![CDATA[
        
			scwin.GLOBAL_VARS = {
				p_param: com.getParameter(),
				revParam: {}
			};
			
			scwin.onpageload = function() {
				
				// 첨부파일 컴포넌트 초기화
                com.file.init();
                
				dma_search.set("reg_id", scwin.GLOBAL_VARS.p_param.ref_no);
				WFRAME_PATBIBLIOGRAPHY.getWindow().scwin.selectBibliography(scwin.GLOBAL_VARS.p_param.mngmt_no);
				
				// 조회
				com.execute(sbm_svcpatprerersr03);
				
			};
			
			scwin.sbm_svcpatprerersr03_done = function(e) {
			
			dma_result.get("patofic_nm");
			
			    scwin.GLOBAL_VARS.revParam.patofic_cd = dma_result.get("patofic_cd");
			    scwin.GLOBAL_VARS.revParam.job_state_cd = dma_result.get("job_state_cd"); 
			    			
			    com_psn_syspaynm.setValue(com.getLoginInfo('cn'));
			    com_daytm.setValue(com.getServerDateTime("yyyyMMdd"));
			    		    
			    //첨부파일 조회
			    const attachFileNo_A = dma_result.get("req_attach_file");
			    const attachFileNo_B = "";
				
				com.file.create(upload_A.getID(), {fileKey: attachFileNo_A ? attachFileNo_A : com.getLoginInfo('serviceId'), mode: "view" , target:"IPMS"});
				com.file.create(upload_B.getID(), {fileKey: attachFileNo_B ? attachFileNo_B : com.getLoginInfo('serviceId'), mode: "edit" , target:"IPMS"});
					
			};
			
			scwin.save_onclick = function(){
                com.confirm("의뢰 하시겠습니까?", function(ret) {
                    if (ret) {
                        com.file.transport(function(attachObj) {
                            scwin.save(attachObj);
                        });
                    }
               });
            };
			
			scwin.save = function(attachObj) {
			
				if(!com.validateData(dma_save,  [
					{ id : "com_patofic_charg_id", mandatory : true },
					{ id : "com_due_date", mandatory : true }					
				], false)) return;
			
				const sn = com.getLoginInfo('sn');
				
				dma_save.set("reg_id", scwin.GLOBAL_VARS.p_param.ref_no);
				dma_save.set("req_no", scwin.GLOBAL_VARS.p_param.req_no);
				dma_save.set("mngmt_no", scwin.GLOBAL_VARS.p_param.mngmt_no);
				dma_save.set("com_psn_syspayno", sn);
				dma_save.set("com_daytm", com_daytm.getValue());
				dma_save.set("com_patofic_cd", scwin.GLOBAL_VARS.revParam.patofic_cd);
				dma_save.set("com_attach_file", attachObj.upload_B);
				dma_save.set("updt_syspayno", sn);
				dma_save.set("psn_syspayno", sn);
				dma_save.set("state", "P1611");
				dma_save.set("job_state_cd", "P1611");
				dma_save.set("bf_state", scwin.GLOBAL_VARS.revParam.job_state_cd);
				dma_save.set("syspayno", sn);
				dma_save.set("curr_state_cd", scwin.GLOBAL_VARS.revParam.job_state_cd);
				dma_save.set("next_state_cd", "P1611");
				dma_save.set("ref_key", scwin.GLOBAL_VARS.p_param.ref_no);
							
				com.execute(sbm_svcpatprerersu03, {
					successCallback : function(e) {
						com.alert("저장되었습니다.");
			        	$p.parent().scwin.trigger_list_onclick();
					}
				});
				
			};
			
			scwin.onpageunload = function() {
				
			};
			
			/*
			 *	사용자 검색
			 */		
			scwin.tb_empno_onkeypress = function(e) {
			    if (event.keyCode == 13) {
			        scwin.selectHumBtn();
			    }
			};
			
			// 빈값일경우 이름, 시스템사번 데이터셋 초기화
			scwin.tb_empno_onchange = function(info) {
			    if (info.newValue == "") {
			        // 주담당자 ID
			        dma_save.set("com_patofic_charg_id", "");
			        // 주담당자 이름
			        dma_save.set("com_patofic_charg_nm", "");
			    }
			};
			
			scwin.btn_search_hum_onclick = function(e) {
			    scwin.selectHumBtn("img");
			};
			
			
			scwin.selectHumBtn = function(data) {
			    data == "img" ? data = { "src_clsf": "tab2", "charg_nm": tb_empno.getValue() } : data = { "src_clsf": "tab2", "charg_nm": tb_empno.getValue() };
			    com.pop.open(biz.pat.pop.TYPE.PAT_OFFICE, data, function(ret) {
			        // 주담당자 ID
			        dma_save.set("com_patofic_charg_id", ret.charg_id);
			        // 주담당자 이름
			        dma_save.set("com_patofic_charg_nm", ret.charg_nm);
			    });
			};
		
		]]></script>
