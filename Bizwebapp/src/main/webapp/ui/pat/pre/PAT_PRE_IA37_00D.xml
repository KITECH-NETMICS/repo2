				// 조회
				com.execute(sbm_svcpatprerersr02);
			};
			
			scwin.sbm_svcpatprerersr02_done = function(e) {
			
				scwin.GLOBAL_VARS.revParam.job_state_cd = dma_result.get("job_state_cd"); // 상태코드
							
				//첨부파일 조회
				const attachFileNo_A = dma_result.get("req_attach_file");
				const attachFileNo_B = dma_result.get("req_attach_file");
				const attachFileNo_C = dma_result.get("prt_regist_attach_file");
				const attachFileNo_D = dma_result.get("prt_reg_org_attach_file");
				const attachFileNo_E = dma_result.get("prt_reg_attach_file");
				const attachFileNo_F = dma_result.get("prt_attach_file");
				
				com.file.create(upload_A.getID(), {fileKey: attachFileNo_A ? attachFileNo_A : com.getLoginInfo('serviceId'), mode: "view" , target:"IPMS"});
				com.file.create(upload_B.getID(), {fileKey: attachFileNo_B ? attachFileNo_B : com.getLoginInfo('serviceId'), mode: "view" , target:"IPMS"});
				com.file.create(upload_C.getID(), {fileKey: attachFileNo_C ? attachFileNo_C : com.getLoginInfo('serviceId'), mode: "view" , target:"IPMS"});
				com.file.create(upload_D.getID(), {fileKey: attachFileNo_D ? attachFileNo_D : com.getLoginInfo('serviceId'), mode: "view" , target:"IPMS"});
				com.file.create(upload_E.getID(), {fileKey: attachFileNo_E ? attachFileNo_E : com.getLoginInfo('serviceId'), mode: "view" , target:"IPMS"});
				com.file.create(upload_F.getID(), {fileKey: attachFileNo_F ? attachFileNo_F : com.getLoginInfo('serviceId'), mode: "view" , target:"IPMS"});
				
				eval_annual_patofic_cd.addItem(dma_result.get("patofic_cd") , dma_result.get("patofic_nm") , 1 );
				eval_annual_patofic_cd.setSelectedIndex(1);
				eval_psn_syspaynm.setValue(com.getLoginInfo('cn'));
				eval_daytm.setValue(com.getServerDateTime("yyyyMMdd"));
				
			};
			
			scwin.onpageunload = function() {
				
			};
		
		]]></script>
