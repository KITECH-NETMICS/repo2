    		</xf:submission>
    		<xf:submission id="sbm_saveSystemReq" ref="data:json,dma_reqUserInfo" action="SvcPATPBDCADM05.pwkjson" method="post"
    			mediatype="application/json" encoding="UTF-8" mode="asynchronous">
