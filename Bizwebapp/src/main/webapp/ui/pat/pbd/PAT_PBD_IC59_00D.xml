    	<script type="text/javascript" lazy="false"><![CDATA[
	/**************************************************************
	*						
	*						정의함수
	*
	***************************************************************/
	
	//	페이지 로드
	scwin.onpageload = function() {
		com.file.init();
		com.setEnterKeyEvent(grpSchHum, scwin.selectHumBtn);
		var codeOptions = [ { code : "FEB", compID : "sbx_issuCause"} ];
		com.initDataMap(dma_qna);
		com.setCmnCd(codeOptions, function() {
			com.setEnterKeyEvent(grpSchHum, scwin.selectHumBtn);
			scwin.initForm();
		});
	};
	
	scwin.onpageunload = function() {
		
	};	
	
	// 페이지 초기화
	scwin.initForm = function() {
		var params      = com.getParameter();
		
		if (com.isEmpty(params.qna_id)) {
			com.initDataMap(dma_qna);
			com.file.create(upload_A.getID(), {fileKey: 'I999', mode: 'edit'});
			com.file.create(upload_B.getID(), {fileKey: 'I999', mode: 'edit'});
			wri_daytm.setValue(com.getServerDateTime());
			//dma_qna.set("empno", com.getLoginInfo("empno"));
			//dma_qna.set("wri_psn_syspayno", com.getLoginInfo("sn"));
			//scwin.schHum();
		} else {
			scwin.selectData(params.qna_id);
		}
	};
	
	// 데이터 조회
	scwin.selectData = function(qna_id) {
		dma_qna.set("rowStatus", "U"); // 상태 : 수정
		dma_qna.set("qna_id", qna_id); // 키 셋팅 및 조회
		com.execute(sbm_selectQna, {
			successCallback : function(e) { 
				//scwin.schHum();
				var quest_fileKey  = e.responseJSON.elData.quest_attach_file;
				var answer_fileKey = e.responseJSON.elData.answer_attach_file;
				
				quest_fileKey = (null == quest_fileKey || '' == quest_fileKey.trim()) ? 'I999' : quest_fileKey;
				com.file.create(upload_A.getID(), {fileKey: quest_fileKey, mode: 'read'});
				
				answer_fileKey = (null == answer_fileKey || '' == answer_fileKey.trim()) ? 'I999' : answer_fileKey;
				com.file.create(upload_B.getID(), {fileKey: answer_fileKey, mode: 'read'});
				if (e.responseJSON.elData.send_clsf == 'Y') {
					//btn_reqAuto.hide();
				}				
				
				//scwin.editor.SetTextFile(dma_qna.get("qna_comment"));
			}
		});
	};
	
	// 저장
	scwin.save = function() {
		
		com.confirm("message.xom.wq.017", function(ret) {
			if (ret) {
				dma_qna.set("updt_syspayno", com.getLoginInfo("sn"));
				com.file.transport( function(attachObj) {
					dma_qna.set("quest_attach_file", attachObj.upload_A);
					com.execute(sbm_saveQna, {
						successCallback : function(e) { // 성공 콜백
							com.alert("message.xom.wq.023" /*정상적으로 처리되었습니다.*/, function () {
										scwin.selectData(e.responseJSON.elData.qna_id);
							});
						}
					});
				});	
			}
		});
	};
	
	// 삭제
	scwin.del = function() {
		com.confirm("message.xom.wq.022", function(ret) {
			if (ret) {
				com.file.deleteAll( function(){
					com.execute(sbm_deleteQna, {
						successCallback : function(e) { // 성공 콜백
							com.alert("message.xom.wq.023"/*정상적으로 처리되었습니다.*/, function() {
								scwin.checkList = [];
								com.moveList("IC58", {});
							});
						}
					});
				});
			}
		});
	};
	
	// 사원검색 돋보기 아이콘 클릭 이벤트
	scwin.btn_search_hum_onclick = function(e) {
		scwin.selectHumBtn("img");
	};
	
	// 직원검색 팝업 호출
	scwin.selectHumBtn = function(data) {
		// 버튼(img)으로 팝업함수 바로 호출 처음 변수에 아무것도 넣지 않으면 최초 서브미션도 실행 안함.
		data == "img" ? data = { empno : ""} : data = { empno : answer_psn_syspayno.getValue(), nm : answer_psn_syspaynm.getValue(), work_clsf : "HAG010" };
		
		com.pop.open( com.pop.TYPE.HUM, data, function(ret){
			if (ret) {
				answer_psn_syspayno.setValue(ret.empno);
				answer_psn_syspaynm.setValue(ret.nm);
				dma_qna.set("answer_psn_syspayno", ret.syspayno);
			}
		});
	};
	
	// 사원검색
	scwin.schHum = function() {
		var data = { empno : dma_qna.get("empno"), work_clsf : "HAG010" };
		com.pop.open( com.pop.TYPE.HUM, data, function(ret) {
			if (ret) {
				let residNo = ret.resid_no.split('-');
				let residNoFr = residNo[0];
				let residNoBk = residNo[1];
				
				dma_qna.set("wri_psn_syspaynm", ret.nm);
			}
		});
	};
	
	// 유효성 체크
	scwin.validateData = function() {
		if(!com.validateData(dma_qna,[
			{ id : "quest_title", mandatory : true},
		], false)) return false;
		
		return true;		
	};
	
	
	/**************************************************************
	*						
	*						컴포넌트 이벤트
	*
	***************************************************************/
	
	// 목록버튼 이벤트
	scwin.btn_list_onclick = function(e) {
		com.moveList("IC58", {});
	};
	
	// 저장버튼 이벤트
	scwin.btn_save_onclick = function(e) {
		// 저장
		if(!scwin.validateData()) return;
		scwin.save();
		
	};
	
	// 직원찾기 팝업 버튼 이벤트
	scwin.btn_search_hum_onclick = function(e) {
		scwin.schHum();
	};
	
	// 삭제 버튼 클릭 이벤트
	scwin.btn_del_onclick = function(e) {
		scwin.del();
	};
	
	scwin.inp_empno_onchange = function(info) {
		if(info.newValue == "") {	
			tb_nm.setValue("");
			dma_qna.set("wri_psn_syspayno", "");
		}
	};
	
	// 시스템 자동문의 버튼
	scwin.btn_reqAuto_onclick = function(e) {
		if (confirm("시스템 자동문의 신청 하시겠습니까?")) {
			scwin.reqSystem();
		}		
	};
	
	scwin.reqSystem = function() {
		dma_qna.set("syspayno", com.getLoginInfo("sn"));
		com.execute(sbm_reqUserInfo, {
			successCallback : function(e) { // 성공 콜백
				var data = e.responseJSON.elData;
				dma_reqUserInfo.set("demnd_item_mngmt_no", "new");
				dma_reqUserInfo.set("req_sys", "XRA010");
				dma_reqUserInfo.set("req_psn_syspayno", com.getLoginInfo("sn"));
				dma_reqUserInfo.set("req_psn_nm", com.getLoginInfo("cn"));
				dma_reqUserInfo.set("req_dept", data.dept_cd);
				dma_reqUserInfo.set("dept_nm", data.dept_nm);
				dma_reqUserInfo.set("req_ymd", data.req_ymd);
				dma_reqUserInfo.set("rcpt_psn_empno", "");
				dma_reqUserInfo.set("req_subj", "[특허사무소 문의]" + dma_qna.get("quest_title"));
				dma_reqUserInfo.set("progrss_state", "XRC010");
				dma_reqUserInfo.set("complt_demnd_ymd", data.complt_demnd_ymd);
				dma_reqUserInfo.set("req_time", data.req_time);
				dma_reqUserInfo.set("complt_demnd_time", data.complt_demnd_time);
				dma_reqUserInfo.set("CUMode", "insert");
				dma_reqUserInfo.set("regst_psn_syspayno", com.getLoginInfo("sn"));
				dma_reqUserInfo.set("updt_psn_syspayno", com.getLoginInfo("sn"));
				dma_reqUserInfo.set("relat_menu_nm", "지식재산권 시스템 문의");
				dma_reqUserInfo.set("req_kind", "PAT");
				dma_reqUserInfo.set("qna_id", com.getParameter().qna_id);
				dma_reqUserInfo.set("req_contnt", dma_qna.get("quest_comment"));
				dma_reqUserInfo.set("relat_menu_id", "IC59");
				dma_reqUserInfo.set("regst_psn_syspayno", com.getLoginInfo("sn"));
				dma_reqUserInfo.set("updt_psn_syspayno", com.getLoginInfo("sn"));
				com.execute(sbm_saveSystemReq, {
					successCallback : function(e) { // 성공 콜백
						btn_reqAuto.hide();
						alert("시스템문의 신청되었습니다.");
					}
				});			
			}
		});		
	};	
	]]></script>
	</head>
	
