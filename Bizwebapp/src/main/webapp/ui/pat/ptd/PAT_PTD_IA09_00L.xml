            <!-- 출원업무대기함 업무공통탭 조회 -->
			<xf:submission id="sbm_svcpatptdpatmr01" 
						   ref='data:json,dma_search' 
						   target='' 
						   action="SvcPATPTDPATMR01.pwkjson" 
