         <script type="text/javascript" lazy="false"><![CDATA[
    /****************************************************
 	 * 화면 설명 : 대외협력조회 wframe
 	 * 변경 이력 : 2023-11-23	김준하	최초 작성
 	 ****************************************************/
 	scwin.data = {}; 	
	/**
	 * 페이지 로딩 이벤트
	 */ 
	scwin.onpageload = function() {
		scwin.mouCheck = true;
		scwin.researchCheck = true;
		scwin.tripCheck = true;
		scwin.vipCheck = true;
		scwin.eventCheck = true;
	};	
	scwin.onpageunload = function() {		
	};
	
	scwin.searchBaseInfo = function(agencyCd) {
		scwin.mouCheck = false;
		scwin.researchCheck = false;
		scwin.tripCheck = false;
		scwin.vipCheck = false;
		scwin.eventCheck = false;		
		scwin.search(agencyCd);
		
	};
	
	/*
     * 조회
     */
	scwin.search = function(agencyCd){
		dma_search.set("agncy_cd", agencyCd);
		com.execute(sbm_corp, {
			successCallback : function(e){
				var data = e.responseJSON.elData.lcpAgncyMstFormVo;
			    var tabLabels = {
			        'tab01': "협약(" + data.moucnt + ")",
			        'tab02': "공동연구(" + data.rescnt + ")",
			        'tab03': "출장(" + data.tripcnt + ")",
			        'tab04': "귀빈접견(" + data.vipcnt + ")",
			        'tab05': "행사(" + data.eventcnt + ")"
			    };
								
			    // 각 탭의 라벨을 동적으로 설정
			    for (var tabId in tabLabels) {
			        if (tabLabels.hasOwnProperty(tabId)) {
			            var tabIdx = tabContainer.getTabIndex(tabId);
			            tabContainer.setLabelText(tabIdx, tabLabels[tabId]);
			        }
			    }
				switch (tabContainer.getSelectedTabID()) {
					case "tab01": scwin.searchMou(); break;
					case "tab02": scwin.researchSearch(); break;
					case "tab03": scwin.tripSearch(); break;
					case "tab04": scwin.vipSearch(); break;
					case "tab05": scwin.eventSearch(); break;
				}			    
			}
		});
	};
	
	// 탭 변경 이벤트	
	scwin.tabContainer_onchange = function(tabID,idx,userTabID) {
		switch (userTabID) {
			case "tab01": scwin.searchMou(); break;
			case "tab02": scwin.researchSearch(); break;
			case "tab03": scwin.tripSearch(); break;
			case "tab04": scwin.vipSearch(); break;
			case "tab05": scwin.eventSearch(); break;
		}		
	};
	
	//협약
	scwin.searchMou = function() {
		dma_search.set("tab_type", "Mou");	
		scwin.tab_Search();
	
	}
	//공동연구
	scwin.researchSearch = function() {
		dma_search.set("tab_type", "Res");
		scwin.tab_Search();

	}
	//출장
	scwin.tripSearch = function() {
		dma_search.set("tab_type", "Trip")
		scwin.tab_Search();
	}
	//귀빈접견
	scwin.vipSearch = function() {
		dma_search.set("tab_type", "Vip");
		scwin.tab_Search();
	}
	//행사
	scwin.eventSearch = function() {
		dma_search.set("tab_type", "Eve");
		scwin.tab_Search();
	}
	
	//탭 조회
	scwin.tab_Search = function() {
		com.execute(sbm_tabList, {
			successCallback : function(e){
			}
		});
	}					
	
	scwin.grd_mou_oncellclick = async function(row,col,colId) {
		if (colId == "mou_no") {
			var mouNo = dlt_mou_list.getCellData(row, "mou_no");
			var serviceid = dlt_mou_list.getCellData(row, "type") == "국내협약" ? "L40D" : "L453";
			const munuObj = await com.getMenuObj(serviceid);
		    const data = {
		        mouNo: mouNo,
		        flag: "Search"
		    };			
	    	const popUp = { OPTION: { width: 1300, height: 900, id: munuObj.serviceID, popupName: munuObj.cn }, URL: munuObj.serviceNewUrl };
	    	com.pop.open(popUp, data);										
		}		
	};
	
	scwin.grd_res_oncellclick = function(row,col,colId) {
		if (colId == "prj_no") {
			var data = {};
			data.prj_no = dlt_res_list.getCellData(row, "prj_no");
			biz.lpl.pop.open(biz.lpl.pop.TYPE.PROJEC, data, function() {});										
		}		
	};
	
	scwin.grd_trip_oncellclick = function(row,col,colId) {
		if (colId == "req_no") {
			var repReqNo = dlt_trip_list.getCellData(row, "rep_req_no");
			if (repReqNo.trim() == "") {
				alert("[알림] 신청번호 " + reqNo + " 에 대한 복명서가 작성되지 않았습니다.");
				return;
			}
			var reqNo = dlt_trip_list.getCellData(row, "req_no");
			
			let menuObj = {};
			menuObj[gcm.MENU_INFO.SERVICE_ID] = "C303";
			menuObj[gcm.MENU_INFO.MENU_ID] = "C303";
			menuObj[gcm.MENU_INFO.MENU_NM] = "해외출장복명서";
			menuObj[gcm.MENU_INFO.SRC_PATH] = "/kitech/ccs/ctr/CtrOverTripRepReq.xml";
			
			const paramObj = {
			    reqNo: repReqNo,
			    biztripReqNo: reqNo,
			    flag: "AgncySearch"
			};			
			gcm.win.openMenu(menuObj, paramObj);										
		}		
	};
	
	scwin.grd_vip_oncellclick = async function(row,col,colId) {
		if (colId == "visit_term") {
			var audencNo = dlt_vip_list.getCellData(row, "audenc_no");
			
			var serviceid = "L457";
			const munuObj = await com.getMenuObj(serviceid);
		    const data = {
		        audencNo: audencNo,
		        flag: "Search"
		    };			
	    	const popUp = { OPTION: { width: 1300, height: 900, id: munuObj.serviceID, popupName: munuObj.cn }, URL: munuObj.serviceNewUrl };
	    	com.pop.open(popUp, data);				
		}		
	};
	
	scwin.grd_eve_oncellclick = async function(row,col,colId) {
		if (colId == "event_nm") {
			var eventno = dlt_eve_list.getCellData(row, "event_no");
			
			var serviceid = "L450";
			const munuObj = await com.getMenuObj(serviceid);
		    const data = {
		        eventno: eventno,
		        flag: "Search"
		    };			
	    	const popUp = { OPTION: { width: 1500, height: 900, id: munuObj.serviceID, popupName: munuObj.cn }, URL: munuObj.serviceNewUrl };
	    	com.pop.open(popUp, data);				
		}		
	};
	]]></script>
