 	 ****************************************************/  
 	
 	scwin.data = {};
 	
 	/**
	 * 페이지 로딩 이벤트
	 */  	            
	scwin.onpageload = function() {
		scwin.data.service_id = "L52C";
		
		// dataMap 초기화
		com.initDataMap(dma_detail);	
		com.initDataMap(dma_info);		
		
		var param = com.getParameter();	
		if (param != undefined) {
			dma_info.setJSON(param);
		}
		
		com.biz.checkRole({serviceId : scwin.data.service_id, roleCodes : com.getLoginInfo('roleCode')}, function(e) {
			scwin.data.isRoleYn = e.isRoleYn;
		});
	
		if (!scwin.data.isRoleYn || dma_info.get("flag") =="reqSel") {
			btn_save.hide();
		}

		if (dma_info.get("req_no") !="") {
			if (dma_info.get("flag") =="reqSel") {
				dma_search.set("req_no", dma_info.get("req_no"));
			} else {
				dma_search.set("req_no", dma_info.get("req_no"));
				dma_search.set("prgds_nm", dma_info.get("prgds_nm"));				
			}
			scwin.search();
		}		
	};
	
	scwin.onpageunload = function() {
		
	};
	
 	/**
	 * 조회 실행
	 */	
	scwin.search = function() {
		com.execute(sbm_selectLplLprPrGoodsDeliv, {
			successCallback : function(e) {
				dlt_pr_gd_deliv.setJSON(e.responseJSON.lplLprPrGoodsReqInfoVoList, false);
	           	var tCount = dlt_pr_gd_deliv.getRowCount();
	           	var totalSum = 0;				
	            for (var i=0; i<tCount; i++) {
              		var temp1 = dlt_pr_gd_deliv.getCellData( i, "unit_price");
    				var temp2 = dlt_pr_gd_deliv.getCellData( i, "distr_qty");
    			
            		dlt_pr_gd_deliv.setCellData( i, "did_distr_qty", temp2);
            		dlt_pr_gd_deliv.setCellData( i, "distr_qty", 0);

    				totalSum = temp1 * temp2;
    				dlt_pr_gd_deliv.setCellData( i, "sum_cost", totalSum);
    				
    				if (dma_info.get("flag") == "reqSel") {
    					grd_gd_distr.setCellDisabled(i, "user_pop", true);
    					grd_gd_distr.setCellDisabled(i, "distr_qty", true);
    					grd_gd_distr.setCellDisabled(i, "distr_ymd", true);
	    			}
           		}			
			}
		});		
	};
	
 	/**
	 * 수령자 조회 돋보기버튼 클릭 이벤트
	 */	
	scwin.grd_gd_distr_oncellclick = function(row,col,colId) {
		if (colId =="") {
			scwin.searchEmp(row);
		}
	};

	/**
	* 사용자 조회 팝업 오픈
	*/
	scwin.searchEmp = function(row) {
		var data = { empno : '', 
					 work_clsf : "HAG010", 
					 ref_yn : "Y"
					}
		com.pop.open( com.pop.TYPE.HUM, data, function(ret) {
			dlt_pr_gd_deliv.setCellData(row, "recv_psn_nm", ret.nm);
			dlt_pr_gd_deliv.setCellData(row, "recv_psn_syspayno", ret.sn);								
		});
	};	

	/**
	* 출고수량 수정 입력 이벤트
	*/	
	scwin.grd_gd_distr_onviewchange = function(info) {
		if (colIndex == grd_gd_distr.getColumnIndex("distr_qty")) {
			var invVol = Number(dlt_pr_gd_deliv.getCellData( row, "inv_vol"));
			
			if (Number(info.newData) > invVol) {
				com.alert("재고 수량을 확인해주세요");
				dlt_pr_gd_deliv.setCellData( info.rowIndex, "distr_qty", "");
			} else {
				//배부수량이 입력되면 총금액을 보여준다. 
				var totalSum = 0;
				var temp1 = dlt_pr_gd_deliv.getCellData( info.rowIndex, "unit_price");
				var temp2 = newData;
				totalSum = temp1 * temp2;
				dlt_pr_gd_deliv.setCellData( info.rowIndex, "sum_cost", totalSum);
			}
		}		
	};
	
	/**
	 * 닫기 버튼 클릭 이벤트
	 */				
	scwin.btn_close_onclick = function(e) {
		com.closePopup({}); // 팝업 닫기 (콜백 수행을 위해 빈 객체 넘김)				
	};
	
	/**
	 * 저장 버튼 클릭 이벤트
	 */			
	scwin.btn_save_onclick = function(e) {
		if (!scwin.preSave()) {
			return;
		}
		com.confirm("message.xom.wq.017" /*저장 하시겠습니까?*/, function(ret) {
			if (ret) {
		   		var tCount = dlt_pr_gd_deliv.getRowCount();
		   		for (var i=0; i<tCount; i++) {
			   		com.initDataMap(dma_detail);
			   		
		       		dma_detail.setJSON(dlt_pr_gd_deliv.getRowJSON(i)); 
		       		
		      		var distrQty = dlt_pr_gd_deliv.getCellAllData(i, "distr_qty");
		      		var doQty = dlt_pr_gd_deliv.getCellAllData(i, "did_distr_qty");
		      		var sumQty = Number(distrQty)  +  Number(doQty);       
		       		var invVol = dlt_pr_gd_deliv.getCellAllData(i, "inv_vol");
		       		var newInvVol = Number(invVol) - Number(distrQty);   
		       		
		       		dma_detail.set("inv_vol", newInvVol);
		       		dma_detail.set("distr_qty", sumQty);	
		       		dma_detail.set("flag", "D");  		
		       		dma_detail.set("io_clsf", "O");  		
		       		dma_detail.set("ymd1", dlt_pr_gd_deliv.getCellAllData(i, "distr_ymd"));  		
		       		dma_detail.set("ymd2", dlt_pr_gd_deliv.getCellAllData(i, "distr_ymd"));  		 
		       		dma_detail.set("qty", distrQty);       	
		       		dma_detail.set("regst_psn_syspayno", com.getLoginInfo("sn")); 
		       		dma_detail.set("updt_psn_syspayno", com.getLoginInfo("sn"));       		 	       			       		       			
					
					scwin.save();
		    	}					
			} 
		});			
	
	};
	
	/**
	 * 저장 실행
	 */	
	scwin.save = function() {
		com.execute(sbm_selectLplLprPrGoodsDeliv, {
			successCallback : function(e) {
				com.alert("저장되었습니다.");
			    opener.goSearch();
			    com.closePopup({});
			}
		});		
	};
	
	/**
	 * 저장전 확인
	 */		
	scwin.preSave = function() {
		if (!scwin.validate()) {
			return false;
		}
		return true;
	};
	
	/**
     * 검증
     */
	scwin.validate = function() {
		if (!com.validateData(dlt_pr_gd_deliv,[
			{ id : "distr_ymd", mandatory : true},
			{ id : "recv_psn_syspayno", mandatory : true},
			{ id : "distr_qty", mandatory : true},
		], false)) return false;
		return true;	
	};
]]></script>
