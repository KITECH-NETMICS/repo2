        <script type="text/javascript" lazy="false"><![CDATA[
	/****************************************************
 	 * 화면 설명 : 기관찾기
 	 * 변경 이력 : 2023-11-08	김준하	최초 작성
 	 ****************************************************/
	scwin.data={
		flag : false
	};
	/**
	 * 페이지 로딩 이벤트
	 */
	scwin.onpageload = function() {
		// 검색조건 영역내의 컴포넌트에 엔터키 이벤트 발생시 조회 수행
		com.setEnterKeyEvent(grp_search, scwin.btn_search_onclick);
		var param = com.getParameter();
		scwin.flag = param.flag
		scwin.custCd = param.custCd
		scwin.agncyFlag = param.agncyFlag
		scwin.useFlag = param.useFlag // Y or N or '' : 고객사용유무플래그(Y or '' - 사용가능, N - 사용중지)
				
       	if (scwin.custCd != "") {
       		ibx_krchar_agncy_nm.setValue(scwin.custCd);
       	}
        var codeOptions = [{ code: "RFK", compID: "sbx_dom_excpt_clsf"}, { code: "FDA", compID: "sbx_natn_cd" }];
        com.setCmnCd(codeOptions, function() {	    
	    	if (scwin.flag == "Dom" || scwin.flag == "DomMou") {
				domExcptClsf = "RFK001";
				searchNatnCd = "FDA001";
				sbx_natn_cd.setValue("FDA001");
				sbx_dom_excpt_clsf.setValue("RFK001");  //국내		    
		    } else if (scwin.flag == "Over" || scwin.flag == "OverRep" || scwin.flag == "OverEvent" || scwin.flag == "OverVip" || scwin.flag == "OverMou") {
		    	domExcptClsf = "RFK002";
				searchNatnCd = "";
				sbx_dom_excpt_clsf.setValue("RFK002"); //해외			   
			} else {
		    	domExcptClsf = "";
		    	searchNatnCd = "";			
			}		
			com.execute(sbm_select_tree, {
				successCallback : function(e){
					var elements = $(`#${trv_natn.getID()} .w2treeview_open_child`);				
					for ( var i = 0; i < elements.length; i++ ) {
					    elements[i].classList.remove("w2treeview_open_child");
					}							
				}
			});	    
	    });       	
	};
	
	//트리 클릭
	scwin.trv_natn_onlabelclick = function(value,nodeElement,index) {
		var executeMoad = true;
		for (var i=0; i < nodeElement.childNodes.length; i++) {
			if (nodeElement.childNodes[i].nodeName == "map") {
				executeMoad = false;	 
			}
		}
		if (executeMoad) {		
			dma_search.set("useFlag", scwin.useFlag);
			dma_search.set("natn_cd", value);		
			com.execute(sbm_selectAgencyList, {
				successCallback : function(e){
									
				}
			})			
		}		
	};	
		
	/**
	 * 조회버튼 클릭
	 */
	scwin.btn_search_onclick = function() {
		if (sbx_dom_excpt_clsf.getValue() == "") {
			alert("국내외 구분을 선택해주세요");	
			return false;
		}	
		var agnecyNm = ibx_krchar_agncy_nm.getValue().trim();
   		if (agnecyNm != "") {
   			agnecyNm = agnecyNm.toUpperCase();
   		}
   		dma_search.set("cmpy_typ", "RZB100");
   		dma_search.set("dom_excpt_clsf", sbx_dom_excpt_clsf.getValue());
   		dma_search.set("natn_cd", sbx_natn_cd.getValue());
   		dma_search.set("cmpy_nm_krchar", agnecyNm==''?'%':'%'+agnecyNm+'%');
   		dma_search.set("cmpy_nm_eng", agnecyNm==''?'%':'%'+agnecyNm+'%'); 		
		scwin.search();
	};

	/**
	 * 조회
	 */
	scwin.search = function() {
		// 목록 조회
		com.execute(sbm_selectSearchList, {
			successCallback : function(e){				
				dlt_ag_list.setJSON(e.responseJSON.elData.lcpVipAudencVoList);			
			}
		})
	};
	
	/**
	 * 선택한 행의 데이터 넘겨주기
	 */
	scwin.setAgncyInfo = function(row){
		com.closePopup(grd_lcp_agncy_master.getRowJSON(row));
	}
	
	scwin.btn_close_onclick = function(e) {
		com.closePopup();
	};

	scwin.grd_user_info_oncelldblclick = function(row,col,colId) {
		scwin.data.flag = true;
		scwin.setAgncyInfo(row);
	};
	
	/**
	 * 신규
	 */	
	scwin.btn_new_onclick = function(e) {
		//기관등록창 사이즈 조절은 기관기본정보 등록창에서 한다.
		if (scwin.flag == "Over") { //해외출장에서 들어온 경우
			scwin.openPopup("UserInsert", scwin.agncyFlag, scwin.flag)
		} else if(scwin.flag == "OverRep") {
			scwin.openPopup("UserInsert", scwin.agncyFlag, scwin.flag)
		} else {
			scwin.openPopup("UserInsert", scwin.agncyFlag)
		}		
	};
	
	scwin.openPopup = function(flag, agncyFlag, where) {
		var data = {};
		data.flag = flag;
		data.agncyFlag = agncyFlag;
		data.where = where;
		biz.lpl.pop.open(biz.lpl.pop.TYPE.ORGAN, data, function(ret) {
			if(ret.research)
				scwin.search();		
		});		
	}	
	scwin.sbx_natn_cd_onchange = function(info) {
		if (sbx_dom_excpt_clsf.getValue() == "RFK002") {   			
			if (sbx_natn_cd.getValue() == "FDA001") {
				sbx_natn_cd.setValue("FDA002");
			}
		}			
	};
	scwin.sbx_dom_excpt_clsf_onchange = function(info) {
		if (sbx_dom_excpt_clsf.getValue() == "RFK001") { 
			sbx_natn_cd.setValue("FDA001");
			sbx_natn_cd.setDisabled(true);
		} else {
			sbx_natn_cd.setDisabled(false);
			sbx_natn_cd.setValue("");
		}		
	};
	]]></script>
