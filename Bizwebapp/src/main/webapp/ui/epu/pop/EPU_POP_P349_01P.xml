         <script type="text/javascript" lazy="false"><![CDATA[
	/****************************************************
 	 * 화면 설명 : 품목내역 검색 팝업
 	 * 변경 이력 : 2022-11-29	이영희	최초 작성
 	 ****************************************************/
 	/**
	 * 페이지 로딩 이벤트
	 */         
	scwin.onpageload = function() {
		// parameter
		var param = com.getParameter();
		dma_search.setJSON(param);	
		
		if(param.type == "R"){
			grp_pur_info.setDisabled(true);
		}
		
		if (param.req_clsf == "H") { // 결재스냅샷에서 호출한 경우
			btn_confirm.hide();	
		}
		
		var codeOptions = [ { code : "PFA", compID :"sbx_unit_cd" } // 단위
			];
		com.setCmnCd(codeOptions, function() {
			debugger;
			// 품목 트리 조회
			com.execute(sbm_prodItem);
			
			// 구매요구번호가 있을 경우
			if (dma_search.get("pur_req_no") != "") {
				com.execute(sbm_purItem , {
					successCallback : function(e) {		
						dma_purItem.set("addtax_calc_rts", dma_search.get("addtax_calc_rts"));
					}
				});
			}else{
				if(dma_search.get("item_cd") != ""){
					sbx_src_clsf.setValue("3");
					ibx_con.setValue(param.item_cd);
					btn_sch.trigger("click");
				}
			}	
		});			
	};
	
	scwin.onpageunload = function() {
		
	};
	
	/**
	 * 트리 클릭 이벤트
	 */ 
	scwin.trv_proditem_onlabelclick = function(value,nodeElement,index) {
		var strOuCode = value;

		if (strOuCode == "0") {
			strOuCode = "root";
		}
				
		if (dlt_prodTree.getRowJSON(index-1).item_level == "4") {
			if (dlt_prodTree.getRowJSON(index-1).item_dept_end == "Y") {
				dma_search.set("item_cd", strOuCode);
				dma_search.set("item_upper_cd", "");				
			} else {
				dma_search.set("item_cd", "");
				dma_search.set("item_upper_cd", strOuCode);
			}
			
			com.execute(sbm_prodItemCell, {
				successCallback:function(e){
					grd_proditem.setFocusedCell(0,0);
				}
			});
		}
	};
	
	/**
	 * 조회버튼 클릭 이벤트
	 */
	scwin.btn_sch_onclick = function(e) {
		var SearchType = sbx_src_clsf.getValue();

		if (ibx_con.getValue() == "") {
			alert("검색어를 입력해 주시기 바랍니다.");
			return;
		}
		
		dma_schItem.setEmptyValue();	// 검색조건 초기화

		switch(SearchType) {
			case "1": 
				dma_schItem.set("item_nm", ibx_con.getValue());
				break;
			case "2": 
				dma_schItem.set("item_eng_nm", ibx_con.getValue());
				break;
			case "3": 
				dma_schItem.set("item_cd", ibx_con.getValue());
				break;
			case "4": 
				dma_schItem.set("rmk", ibx_con.getValue());
				break;
		}
		
		dma_schItem.set("item_clsf", sbx_item_clsf.getValue());
		
		com.execute(sbm_schItem, {
			successCallback:function(e){
				grd_proditem.setFocusedCell(0,0);
			}
		});
	};
	
	/**
	 * 수량 클리어 이벤트
	 */
	scwin.inputQty_onblur = function(e) {
		if (inputQty.getValue() != null && inputReqUnitPrice.getValue() != "") {
			scwin.setReqUnitPriceRelation();
		}
	};
	
	/**
	 * 추정단가 클리어 이벤트
	 */
	scwin.inputReqUnitPrice_onblur = function(e) {
		if (inputQty.getValue() != null && inputReqUnitPrice.getValue() != "") {
			scwin.setReqUnitPriceRelation();
		}
	};
	
	/**
	 * 추정가격, 부대비용, 부가세 계산
	 */
	scwin.setReqUnitPriceRelation = function() {
		//부가세 계산방법 변경 : 원단위 5이하 절삭, 5이상 반올림(2012.03.27)]
		var reqAmt = parseInt(Number(inputQty.getValue())*Number(inputReqUnitPrice.getValue())); //수량 * 단가

		inputReqAmt.setValue(reqAmt); //추정가격

		var addTaxCalcRts = Number(inputAddtaxCalcRts.getValue()); //부가세율
		var addTax = Math.round(reqAmt * addTaxCalcRts / 1000) * 10;

		inputReqTimeAddtaxExtexpns.setValue(addTax); //해당품목의 부가세

		//부대비용 자동 셋팅
		var tmpReqAmt = (reqAmt + addTax)/10000; // 합계금액(만단위)

		if (tmpReqAmt>300 && tmpReqAmt <= 2200) {//300만초과 2200만 이하
			inputIncidntlExpns.setValue(360000);
		} else if (tmpReqAmt>2200 && tmpReqAmt <= 5500) { //2200만 초과 5500만 이하
			inputIncidntlExpns.setValue(690000);
		} else if (tmpReqAmt>5500 && tmpReqAmt <= 10000) { //5500만 초과 1억 이하
			inputIncidntlExpns.setValue(800000);
		} else if (tmpReqAmt>10000 && tmpReqAmt <= 30000) { //1억 초과 3억 이하
			inputIncidntlExpns.setValue(1680000);
		} else if (tmpReqAmt>30000 && tmpReqAmt <= 50000) {//3억 초과 5억 이하
			inputIncidntlExpns.setValue(3330000);
		} else if (tmpReqAmt>50000 && tmpReqAmt <= 70000) {//5억초과 7억 이하
			inputIncidntlExpns.setValue(4430000);
		} else if (tmpReqAmt>70000 && tmpReqAmt <= 100000) { //7억초과 10억 이하
			inputIncidntlExpns.setValue(5530000);
		} else if (tmpReqAmt> 100000) { //10억 초과
			inputIncidntlExpns.setValue(6630000);
		} else {
			inputIncidntlExpns.setValue(0);
		}

		var reqAmtSum = reqAmt + addTax + WebSquare.util.getNumber(inputIncidntlExpns.getValue()); //추정금액+부가세 + 부대비용

		inputReqAmtSum.setValue(reqAmtSum);
	};
	
	/**
	 * 적용버튼 클릭 이벤트
	 */
	scwin.btn_confirm_onclick = function(e) {
		if (!scwin.presave()) {
			return;
		}

		com.closePopup(dma_purItem.getJSON());
	};
	
	/**
	 * 적용전 체크
	 */
	scwin.presave = function(e) {
		var inPutKrcharGdnm =  inputKrcharGdnm.getValue();
		if (inPutKrcharGdnm == "") {
			alert("한글 품명은 필수 입니다");
			inputKrcharGdnm.focus();
			return false;
		}

		if (sbx_unit_cd.getValue() == "") {
			alert("단위는 필수 입니다");
			sbx_unit_cd.focus();
			return false;
		}

		if (inputQty.getValue() == "") {
			alert("수량은 필수 입니다");
			inputQty.focus();
			return false;
		}

		if (inputReqUnitPrice.getValue() == "") {
			alert("추정가격은 필수 입니다");
			inputReqUnitPrice.focus();
			return false;
		}

		if (inputItemCd.getValue() == "") {
			alert("코드번호는 필수 입니다 화면의 트리를 사용하여 입력해주세요");
			return false;
		}
		
		// 품목코드 10자리만 선택가능
		var inputItemCdGv = inputItemCd.getValue(); 
		if (inputItemCdGv.length != 10) {
			alert("품목코드 10자리가 아닌경우 선택할 수 없습니다.");
			return false;
		}
		
		// 한글품명 특수문자 제외 예외 ( ) , . - /
		var checkSpeChar = /[~!@#$%^&*_+|<>?:;`{}\]\[\'\"\\\'\·\・]/gi;
		if (checkSpeChar.test(inPutKrcharGdnm)) {
			alert("( ) , . - / 를 제외한 특수문자는 사용할 수 없습니다.");
			inputKrcharGdnm.focus();
			return false;
		}
		
		return true;
	}
	
	/**
	 * 부대비용변경 이벤트
	 */
	scwin.inputIncidntlExpns_onchange = function(info) {
		var AddtaxVal = Number(inputReqTimeAddtaxExtexpns.getValue());
		var ReqAmtVal = Number(inputReqAmt.getValue());
		var incidntlExpns = WebSquare.util.getNumber(inputIncidntlExpns.getValue());
		inputReqAmtSum.setValue(AddtaxVal + ReqAmtVal + incidntlExpns); //구매요구 합계(추정금액+부가세+부대비용)
	};
	
	/**
	 * 부가세 변경이벤트
	 */
	scwin.inputReqTimeAddtaxExtexpns_onchange = function(info) {
		var AddtaxVal = Number(inputReqTimeAddtaxExtexpns.getValue());
		var ReqAmtVal = Number(inputReqAmt.getValue());
		var incidntlExpns = WebSquare.util.getNumber(inputIncidntlExpns.getValue());
		inputReqAmtSum.setValue(AddtaxVal + ReqAmtVal + incidntlExpns); //구매요구 합계(추정금액+부가세+부대비용)
	};
	
	/**
	 * 닫기 버튼 클릭 이벤트
	 */
	scwin.btn_close_onclick = function(e) {
		com.closePopup();
	};
	
	/**
	 * 품목그리드 row index 변경 이벤트
	 */
	scwin.grd_proditem_onrowindexchange = function(row,oldRow) {
		dma_purItem.setEmptyValue();
		
		dma_purItem.setJSON(dlt_prodItem.getRowJSON(row));
		dma_purItem.set("krchar_gdnm", dlt_prodItem.getCellData(row, "item_nm"));
		dma_purItem.set("eng_gdnm", dlt_prodItem.getCellData(row, "item_eng_nm"));
	};
	]]></script>
    </head>
    <body ev:onpageload="scwin.onpageload">
    <xf:group id="" class="pop_contents">
    	<xf:group class="lybox " id="" style="">
