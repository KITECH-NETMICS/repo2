            			<w2:key id="item_rmk" name="상세설명" dataType="text"></w2:key>
            			<w2:key id="pur_req_no" name="구매요구번호" dataType="text"></w2:key>
            			<w2:key id="addtax_calc_rts" name="부가세율" dataType="text"></w2:key>
