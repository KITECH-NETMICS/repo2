         <script type="text/javascript" lazy="false"><![CDATA[
	/****************************************************
 	 * 화면 설명 : 품목내역 검색 팝업
 	 * 변경 이력 : 2022-11-29	이영희	최초 작성
 	 ****************************************************/
 	/**
	 * 페이지 로딩 이벤트
	 */         
	scwin.onpageload = function() {
		// parameter
		var param = com.getParameter();
		dma_search.setJSON(param);	
		
		if(param.type == "R"){
			grp_pur_info.setDisabled(true);
		}
		
		if (param.req_clsf == "H") { // 결재스냅샷에서 호출한 경우
			btn_confirm.hide();	
		}
		
		var codeOptions = [ { code : "PFA", compID :"sbx_unit_cd" }, // 단위
							{ code : "FAM", compID :"sbx_curncy_unit_cd"}
			];
		com.setCmnCd(codeOptions, function() {
			// 품목 트리 조회
			com.execute(sbm_prodItem);
			
			// 구매요구번호가 있을 경우
			if (dma_search.get("pur_req_no") != "") {
				com.execute(sbm_purItem , {
					successCallback : function(e) {		
						dma_purItem.set("addtax_calc_rts", dma_search.get("addtax_calc_rts"));
						dma_purItem.set("curncy_unit_cd", param.curncy_unit_cd);
						dma_purItem.set("local_cd", param.local_cd);
						dma_purItem.set("req_time_exchgr", param.req_time_exchgr);
					}
				});
			}else{
				if(dma_search.get("item_cd") != ""){
					sbx_src_clsf.setValue("3");
					ibx_con.setValue(param.item_cd);
					btn_sch.trigger("click");
				}
			}	
		});			
	};
	
	scwin.onpageunload = function() {
		
	};
	
	/**
	 * 트리 클릭 이벤트
	 */ 
	scwin.trv_proditem_onlabelclick = function(value,nodeElement,index) {
		var strOuCode = value;

		if (strOuCode == "0") {
			strOuCode = "root";
		}
				
		if (dlt_prodTree.getRowJSON(index-1).item_level == "4") {
			if (dlt_prodTree.getRowJSON(index-1).item_dept_end == "Y") {
				dma_search.set("item_cd", strOuCode);
				dma_search.set("item_upper_cd", "");				
			} else {
				dma_search.set("item_cd", "");
				dma_search.set("item_upper_cd", strOuCode);
			}
			
			com.execute(sbm_prodItemCell, {
				successCallback:function(e){
					grd_proditem.setFocusedCell(0,0);
				}
			});
		}
	};
	
	/**
	 * 조회버튼 클릭 이벤트
	 */
	scwin.btn_sch_onclick = function(e) {
		var SearchType = sbx_src_clsf.getValue();

		if (ibx_con.getValue() == "") {
			alert("검색어를 입력해 주시기 바랍니다.");
			return;
		}
		
		dma_schItem.setEmptyValue();	// 검색조건 초기화

		switch(SearchType) {
			case "1": 
				dma_schItem.set("item_nm", ibx_con.getValue());
				break;
			case "2": 
				dma_schItem.set("item_eng_nm", ibx_con.getValue());
				break;
			case "3": 
				dma_schItem.set("item_cd", ibx_con.getValue());
				break;
			case "4": 
				dma_schItem.set("rmk", ibx_con.getValue());
				break;
		}
		
		dma_schItem.set("item_clsf", sbx_item_clsf.getValue());
		
		com.execute(sbm_schItem, {
			successCallback:function(e){
				grd_proditem.setFocusedCell(0,0);
			}
		});
	};
	
	/**
	 * 수량 viewchange 이벤트
	 */
	scwin.ibx_qty_onviewchange = function(info) {
		if (ibx_qty.getValue() != "" && ibx_req_unit_price.getValue() != "") {
			scwin.setReqAmt();
		}
	};
	
	/**
	 * 추정단가 viewchange 이벤트
	 */
	scwin.ibx_req_unit_price_onviewchange = function(info) {
		if (ibx_qty.getValue() != "" && ibx_req_unit_price.getValue() != "") {
			scwin.setReqAmt();
		}
	};
	
	/**
	 * 추정가격, 부대비용, 부가세 계산
	 */
	scwin.setReqAmt = function() {
		var taxRate = 0;
		//국내가 체크일 경우 부가세비율을 10%만 적용한다.
		if(cbx_local_cd.getValue() == "N"){
			taxRate = 10;
		}else{
			taxRate = com.parseInt(dma_search.get("addtax_calc_rts"));
		}
		
		var reqTimeExchgr = Number(ipt_curncy_exchgr.getValue());
		if(sbx_curncy_unit_cd.getValue() == "FAM031"){
			reqTimeExchgr = reqTimeExchgr/100;
		}
		var reqAmt = com.parseInt(ibx_qty.getValue()) * Number(ibx_req_unit_price.getValue()) * reqTimeExchgr;
		ibx_req_amt.setValue(reqAmt); // 추정가격
		var addTax = com.parseInt(reqAmt * taxRate /100);
		ibx_req_time_addtax_extexpns.setValue(addTax);
		ibx_req_amt_sum.setValue(reqAmt + addTax);
	};
	
	/**
	 * 적용버튼 클릭 이벤트
	 */
	scwin.btn_confirm_onclick = function(e) {
		if (!scwin.presave()) {
			return;
		}

		com.closePopup(dma_purItem.getJSON());
	};
	
	/**
	 * 적용전 체크
	 */
	scwin.presave = function(e) {
		if(!com.validateData(dma_purItem,[
			{ id : "krchar_gdnm", mandatory : true},
			{ id : "eng_gdnm", mandatory : true},
			{ id : "unit_cd", mandatory : true},
			{ id : "qty", mandatory : true},
			{ id : "curncy_unit_cd", mandatory : true},
			{ id : "hs_no", mandatory : true},
			{ id : "req_unit_price", mandatory : true},
			{ id : "item_cd", mandatory : true},
			
			], false)) return false;	
		
		// 품목코드 10자리만 선택가능
		if (ibx_item_cd.getValue().length != 10) {
			alert("품목코드 10자리가 아닌경우 선택할 수 없습니다.");
			return false;
		}
		
		// 한글품명 특수문자 제외 예외 ( ) , . - /
		var checkSpeChar = /[~!@#$%^&*_+|<>?:;`{}\]\[\'\"\\\'\·\・]/gi;
		if (checkSpeChar.test(ibx_krchar_gdnm.getValue())) {
			alert("( ) , . - / 를 제외한 특수문자는 사용할 수 없습니다.");
			ibx_krchar_gdnm.focus();
			return false;
		}
		
		var hsCode = ibx_hs_no.getValue();
		///(\d)\1\1\1/.test(hsCode) : 연속된 숫자 4자리 이상사용이 안되도록 하기 위하여
		//20160316 HS Code가 없는 것 있어서 기타용으로 9999900000 허용(박종미요청)
		//20160408  hs번호 00000있는 것이 존재하여 같은 숫자 6이상 반복 막음
		if((/(\d)\1\1\1\1\1/.test(hsCode)) && hsCode != "9999900000"){
			alert("HS번호를 확인해주세요");
			return false;
		}
		
		return true;
	}
	
	/**
	 * 부대비 변경 이벤트
	 */
	scwin.ibx_req_time_addtax_extexpns_onviewchange = function(info) {
		var AddtaxVal = com.parseInt(ibx_req_time_addtax_extexpns.getValue());
		var ReqAmtVal = com.parseInt(ibx_req_amt.getValue());
		ibx_req_amt_sum.setValue(AddtaxVal + ReqAmtVal); //구매요구 합계(추정금액+부가세)
	};
	
	/**
	 * 닫기 버튼 클릭 이벤트
	 */
	scwin.btn_close_onclick = function(e) {
		com.closePopup();
	};
	
	/**
	 * 품목그리드 row index 변경 이벤트
	 */
	scwin.grd_proditem_onrowindexchange = function(row,oldRow) {
		dma_purItem.setEmptyValue();
		
		dma_purItem.setJSON(dlt_prodItem.getRowJSON(row));
		dma_purItem.set("krchar_gdnm", dlt_prodItem.getCellData(row, "item_nm"));
		dma_purItem.set("eng_gdnm", dlt_prodItem.getCellData(row, "item_eng_nm"));
	};
	
	/**
	 * 화폐단위 변경 이벤트
	 */
	scwin.sbx_curncy_unit_cd_onviewchange = function(info) {
		if(info.newValue !== ""){
		    com.executeDynamic({ id: "sbm_getCurncy", action: "SvcEPUREQPOPR12.pwkjson" }, {
		        requestData: { cd: info.newValue }
		        ,processMsg: ""
		        ,successCallback: function(ret) {
		        	ipt_curncy_exchgr.setValue(ret.responseJSON.elData.cash_pur_rt);
		        	scwin.setReqAmt();
		        }
		    });
	    }
	};
	]]></script>
    </head>
    <body ev:onpageload="scwin.onpageload">
