    /*
     * 페이지 열기
     */  
	scwin.onpageload = function() {
		// 기간 역전 체크
		com.checkPeriod(ica_from, ica_to);
		
		// 초기화
		var today = com.getServerDateTime();
		//이순영 요청사항 (조회시작일자를 회계연도 시작일로)
		var fromDate = today.substring(0,4) + "0101";
		ica_from.setValue(fromDate);
		ica_to.setValue(today);
		
		//enter key event
		com.setEnterKeyEvent(grp_reqPsn, scwin.btn_req_psn_onclick);
		com.setEnterKeyEvent(grp_accntRspns, scwin.btn_accnt_rspns_onclick);
		com.setEnterKeyEvent(grp_chargPsn, scwin.btn_charg_onclick);
		
		if(com.getLoginInfo("deptCode") != "H750" && !/1004107/.test(com.getLoginInfo("roleCode")) && com.getLoginInfo("sn") != "20130576"){ // 구매시스템 관리자
			grd_purList.setColumnVisible("enfrc_amt_proc", false);
			grd_purList.setColumnVisible("chng_ord_proc", false);
		}
	};
	
	scwin.onpageunload = function() {
		
	};
	
	/*
     * 조회버튼 클릭 이벤트
     */
	scwin.btn_search_onclick = function(e){
		scwin.search();
	};
	
	/*
     * 조회실행
     */
	scwin.search = function(){
	
		dma_search.set("excel_yn","");
		
		if(!com.validateData(dma_search, [
			{id:"from_ymd", mandatory : true},
			{id:"to_ymd", mandatory : true},
		], false)) return;
		
		com.execute(sbm_selectData, {
			successCallback:function(e){
				dlt_pur.setJSON(e.responseJSON.purReqMastrMngVoList);
				tbx_cnt.setValue(dlt_pur.getRowCount());
			}
		});
	};
	
	/*
     * 인사정보 셋팅
     */
	scwin.setEmpPsn = function(clsf){
		var data = { work_clsf : "HAG010"};
		switch(clsf){
			case "ACCNT" : 	data.empno = ibx_accnt_rspns_empno.getValue(); break;
			case "REQ" : 	data.empno = ibx_req_psn_empno.getValue(); break;
			case "CHARG" : 	data.empno = ibx_charg_empno.getValue(); break;
		}
		
		data.ref_yn = (data.empno == "")? "Y" : "N";	
		
		com.pop.open(com.pop.TYPE.HUM, data, function(ret) {
			if(clsf == "ACCNT"){
				dma_search.set("accnt_respn_psn_syspayno", ret.syspayno);
				ibx_accnt_rspns_empno.setValue(ret.empno);
				ibx_accnt_rspns_nm.setValue(ret.nm);
				
			}else if(clsf == "REQ"){ // 구매요구자
				dma_search.set("req_psn_syspayno", ret.syspayno);
				ibx_req_psn_empno.setValue(ret.empno);
				ibx_req_psn_nm.setValue(ret.nm);
				
			}else if(clsf == "CHARG"){ // 구매담당자
				dma_search.set("charg_syspayno", ret.syspayno);
				ibx_charg_empno.setValue(ret.empno);
				ibx_charg_nm.setValue(ret.nm);
			}
		});
	};
	
	/*
     * 계정책임자 검색버튼 클릭 이벤트
     */
	scwin.btn_accnt_rspns_onclick = function(e) {
		scwin.setEmpPsn("ACCNT");
	};
	
	/*
     * 메뉴이동
     */
	scwin.menuMove = function(data){
		data.clsf = "MNT";
		data.openerId = "P352";
		
		if(data.inout_clsf == "1"){
//			com.openTabUrl("내자구매요구", "/ui/epu/req/EPU_REQ_P022_00D.xml", 1, data, function(ret){
//				if(ret) scwin.search();
//			});
			
			com.openTabMenu("P022", data, function(ret){	
				if(ret){
					scwin.search();	
				}		
			});
		}else{
			com.openTabMenu("P023", data, function(ret){	
				if(ret){
					scwin.search();	
				}				
			});
		}
	};
	
	/*
     * grid 클릭 이벤트
     */
	scwin.grd_list_oncellclick = function(row,col,colId) {
		if(colId == "pur_req_no"){
			var data={
				inout_clsf : dlt_pur.getCellData(row, "inout_psn_clsf_cd"),
				pur_req_no : dlt_pur.getCellData(row, "pur_req_no")
			};			
			scwin.menuMove(data);
			
		}else if(colId == "progress_state"){
			biz.epu.pop.open(biz.epu.pop.TYPE.PUR_PROGRS, {pur_req_no :dlt_pur.getCellData(row, "pur_req_no")} , function(ret) {				
			});			
			
		}else if(colId == "enfrc_amt_proc"){ // 원인행위 금액 정리
			if(dlt_pur.getCellData(row, "contrct_no").trim() != ""){
				alert("구매계약이 존재합니다. \n구매계약매뉴에서 통제금액을 수정하시기 바랍니다.");
				return;
			}
			
			if(dlt_pur.getCellData(row, "cud_type").trim() == "D"){
				alert("원인행위금액이 이미 정리되었습니다.");
				return;
			}
			
			var data ={
				pur_req_no : dlt_pur.getCellData(row, "pur_req_no"),
				bid_ord_no : dlt_pur.getCellData(row, "bid_ord_no")
			};
			
			biz.epu.pop.open(biz.epu.pop.TYPE.PUR_CTRL, data, function(ret) {				
			});
		}else if(colId == "chng_ord_proc"){
			if(dlt_pur.getCellData(row, "bid_ord_no").trim() == ""){
				alert("발주번호가 존재하지 않습니다.");
				return;
			}
			
			if(dlt_pur.getCellData(row, "progress_status") == "유찰" || dlt_pur.getCellData(row, "progress_status") == "취소공고"){
				if (!confirm("현재 발주건은 진행이 불가능 하며 신규 발주만 가능하게 됩니다. 진행하시겠습니까?")) return;
				
				dma_apr.set("pur_req_no", dlt_pur.getCellData(row, "pur_req_no"));
				dma_apr.set("bid_ord_no", dlt_pur.getCellData(row, "bid_ord_no"));
				
				com.execute(sbm_processApr, {
					successCallback : function(e){
						var param={
							flag : "reg",
							frompur : "Y",
							pur_req_no : dlt_pur.getCellData(row, "pur_req_no"),
							sn : com.getLoginInfo("sn")
						};
						
						var svrId = "";
						switch(dlt_pur.getCellData(row, "pur_grp_cd")){
							case "EGF010" : svrId = "P302"; break;
							case "EGF020" : svrId = "P345"; break;
							case "EGF030" : svrId = "P303"; break;
							default : break;
						}
					
						com.openTabMenu(svrId, param);
					}
				});
			}else{
				alert("유찰 및 취소공고 된 건만 신규 재공고가 가능합니다.");
			}
		}
	};
	
	/*
     * 구매요구자 클리어 이벤트
