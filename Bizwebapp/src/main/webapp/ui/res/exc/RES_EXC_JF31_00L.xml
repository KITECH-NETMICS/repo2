        <script type="text/javascript" lazy="false"><![CDATA[
 
    /****************************************************
 	 * 화면 설명 : 실행예산편성 목록
 	 * 변경 이력 : 2023-10-16	김상미	최초 작성
 	 * 서비스아이디 : JF31
 	 ****************************************************/  
 	
 	scwin.data = {};
 	
 	/**
	 * 페이지 로딩 이벤트
	 */       
	scwin.onpageload = function() {
		var param = com.getParameter();
		scwin.data.service_id = "JF31";
		console.log(param);
		dma_search.setJSON(param);
		
		//콤보박스세팅
		var codeOptions = [{ code : "RAK", compID : "sbx_accnt_state"}, //계정상태
						   { code : "RBA", compID : "sbx_prj_state"}  	// 과제상태
						   ];		
		
		com.setCmnCd(codeOptions, function() {
			if (dma_search.get("prj_state") == "") {
				sbx_prj_state.setValue("RBA001");
			}		
		});	
		
		scwin.data.roleCode = com.getLoginInfo("roleCode");
		
		//관리자 권한 여부에 따른 초기제어 추가
		com.biz.checkRole({serviceId : scwin.data.service_id, roleCodes : com.getLoginInfo("roleCode")}, function(e) {
			scwin.data.isRoleYn = e.isRoleYn;
		});

		// enter key event
		com.setEnterKeyEvent(grp_search, scwin.btn_search_emp_onclick);		
		
		//과제년도 세팅
		scwin.data.today = com.getServerDateTime();
		
		ibx_prj_orign.setValue(scwin.data.today.substring(0,4)); 
		
		if (com.getParameter("view_tag") == "Re") {
			scwin.search();
		}else{
			if (scwin.data.roleCode.indexOf("1001114") == -1){
				ibx_empno.setValue(com.getLoginInfo("empno"));
				ibx_accnt_rspns_nm.setValue(com.getLoginInfo("cn"));
				dma_search.set("accnt_rspns", com.getLoginInfo("sn"));
				scwin.search();
			}
		}		
	};
	
	scwin.onpageunload = function() {
		
	};

	/**
	 * 계정번호 키 이벤트 
	 */
	scwin.ibx_accnt_no_onkeypress = function(e) {
		if (e.keyCode == "13") {
			dma_search.set("accnt_no", ibx_accnt_no.getValue());
			scwin.search();	
		}		
	};
	
	/**
	 * 계정책임자 변경시 이벤트
	 */		
	scwin.ibx_empno_onviewchange = function(info) {
		if (this.getValue() == "") {
			ibx_accnt_rspns_nm.setValue("");
			dma_search.set("accnt_rspns", "");						
		}			
	};	
	
	/**
	 * 사용자조회 돋보기 버튼 클릭 이벤트 
	 */	
	scwin.btn_search_emp_onclick = function(e) {
		var data = { empno : ibx_empno.getValue(), 
					 work_clsf : "HAG010", 
					 ref_yn : (ibx_empno.getValue()=="") ? "Y" :"N"
					};
		if (!scwin.data.isRoleYn) {
			data.dept_cd = com.getLoginInfo("deptCode");
		}
					
		com.pop.open( com.pop.TYPE.HUM, data, function(ret) {
			ibx_accnt_rspns_nm.setValue(ret.nm);
			ibx_empno.setValue(ret.empno);
			dma_search.set("accnt_rspns", ret.syspayno);
		});				
	};
	
	/**
	 * 조회 버튼 클릭 이벤트 
	 */		
	scwin.btn_search_onclick = function(e) {
		scwin.search();			
	};

	/**
	 * 조회 실행 전 확인
	 */		
	scwin.preSearch = function() {
		var accntNo = ibx_accnt_no.getValue().trim();     // 계정번호
		if (accntNo == "" && dma_search.get("accnt_rspns") == "") {
			alert("계정번호 또는 계정책임자중 적어도 하나는 입력해주시기 바랍니다.");
			return false;
		}
		return true;		
	};

	/**
	 * 조회 실행
	 */		
	scwin.search = function() {
		if (!scwin.preSearch()) {
			return;
		}
		com.execute(sbm_selectResExcPrjExeBdgetList, {
			successCallback : function(e) {
				console.log(e.responseJSON);
				dlt_list.setJSON(e.responseJSON.resExcExeBdgetInfoVoList);
				tbx_result.setValue(dlt_list.getTotalRow());								
			}
		});		
	};

	/**
	 * 엑셀다운로드 버튼 클릭 이벤트 
	 */		
	scwin.btn_down_excel_onclick = function(e) {
		if (dlt_list.getRowCount() == 0) {
			alert("조회 결과가 없습니다.");
			return;
		}
		
		var option = {
		    fileName : "실행예산편성.xls"
		  , hiddenVisible : false		    
		  , useDataFormat : true
		};
		com.downloadGridExcel(grd_accnt_list, option);				
	};

	/**
	 * 그리드 계정번호 버튼 클릭 이벤트 
	 */			
	scwin.grd_accnt_list_oncellclick = function(row,col,colId) {
		if (colId == "accnt_no") {
			var data = {};
	
			data.usr_adm_flag = "USER";
			data.crud_tag ="U";
			//조회 조건 세팅해서 상세화면 호출
			data.accnt_rspns_empno = ibx_empno.getValue();
			data.accnt_rspns_nm = ibx_accnt_rspns_nm.getValue();																	
			data.accnt_rspns = dma_search.get("accnt_rspns");			
			data.accntNo = dma_search.get("accnt_no");
			data.prj_nm = dma_search.get("prj_nm");
			data.prj_orign = dma_search.get("prj_orign_yyyy");
			data.accnt_state = dma_search.get("accnt_state");
			data.prj_state = dma_search.get("prj_state");
			data.accnt_no = dlt_list.getCellData(row, "accnt_no");
			data.prj_no = dlt_list.getCellData(row, "prj_no");
			data.accnt_clsf = dlt_list.getCellData(row, "accnt_clsf");
			data.itm_solve_base = dlt_list.getCellData(row, "itm_solve_base_pm");
			data.cryfwd_amt = dlt_list.getCellData(row, "cryfwd_amt");
			data.intrst_amt = dlt_list.getCellData(row, "intrst_amt");																				
	
			com.openTabMenu("RF51", data, function(retObj) {
				// 콜백
				if (retObj) {
					scwin.search();	
				}
			});
		}			
	};	
	
	/**
	 * 계정기간 display format 날짜형식으로 지정
	 */			
	scwin.dateFormat = function(val) {
		var res = val;
		var dateArray = [];
		if (val != null && val.length >= 19) {
			dateArray = val.split("~");
			res = com.formatDate(dateArray[0].trim(), "dash") + " ~ " + com.formatDate(dateArray[1].trim(), "dash");
		}
		return res;
	};

]]></script>
