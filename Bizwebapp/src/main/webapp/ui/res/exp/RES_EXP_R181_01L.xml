		btn_del.show();
		btn_cancel.hide();
		btn_save.show();
		selectboxUseYn.setReadOnly(false);
		selectboxUseYn.setDisabled(false);
		if((gridAllowanceRete.getCellDisplayData(row, "close_ymd") == null) || (gridAllowanceRete.getCellDisplayData(row, "close_ymd") == "")) {
			inputCalCloseYmd.setReadOnly(false);
		}
		
		if(selectboxUseYn.getValue() == 'N') {
			scwin.disableTrueAll();
		}
