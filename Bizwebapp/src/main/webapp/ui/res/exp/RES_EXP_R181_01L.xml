	/*
	* 유효성검사
	*/
	scwin.validate = function(dataMap) {
		if(CRUDType == "U") {
			return com.validateData(dataMap, [
			{id : "bsns_cd_inout", mandatory : true},
			{id : "bsns_cd", mandatory : true},
			{id : "use_yn", mandatory : true},
			{id : "start_ymd", mandatory : true},
			
			]);
			
		} else if (CRUDType == "C") {
			return com.validateData(dataMap, [
			{id : "bsns_cd_inout", mandatory : true},
			{id : "bsns_cd", mandatory : true},
			{id : "use_yn", mandatory : true},
			{id : "start_ymd", mandatory : true},
	//		{id : "close_ymd", mandatory : true},
			{id : "psn_limit_rate_01", mandatory : true},
			{id : "psn_limit_rate_02", mandatory : true},
			{id : "psn_limit_rate_03", mandatory : true},
			
			]);
			
		}
		
	}	
	/*
	* 동일 수탁,부처일 때 기간 겹칠 시 검증 및 안내문구 출력
	*/
	scwin.sameVali = function(e) {
		var countRow = dlt_list.getTotalRow();
		
		var writeBsnsCdIO   = selectboxBsnsCdInout.getValue();
		var writeBsnsCd     = selectboxBsnsCd.getValue();
		var writeStartYmd   = inputCalStartYmd.getValue().slice(0, 4) + "-" + inputCalStartYmd.getValue().slice(4, 6) + "-" + inputCalStartYmd.getValue().slice(6);
		var writeUseYn 		= selectboxUseYn.getValue(); 
		var tempCdIO        = "";
		var tempCd          = "";
		var tempStartYmd    = "";
		var tempCdIOName    = "";
		var tempCdName      = "";
		var tempUseYn       = "";

	for(var i = 0 ; i < countRow; i++) {
		tempCdIO     = gridAllowanceRete.getCellDisplayData(i, "bsns_cd_inout");
		tempCd       = gridAllowanceRete.getCellDisplayData(i, "bsns_cd");
		tempStartYmd = gridAllowanceRete.getCellDisplayData(i, "start_ymd");
		
		tempCdIOName = gridAllowanceRete.getCellDisplayData(i, "bsns_inout_nm");
		tempCdName   = gridAllowanceRete.getCellDisplayData(i, "bsns_nm");
		tempUseYn    = gridAllowanceRete.getCellDisplayData(i, "use_yn");
		debugger;
		if((writeBsnsCdIO == tempCdIO)&&(writeBsnsCd == tempCd)){
			if((writeStartYmd == tempStartYmd) && (tempUseYn == "사용")){
				
				if(CRUDType=="C"){
					alert("시작일이 동일한 수탁-부처 상한비율이 이미 존재합니다.\n\n[" + tempCdIOName + " - " + tempCdName + " - " + tempStartYmd + "]" );	
					inputCalStartYmd.focus();
					return false;
				}
		//		else if(CRUDType=="U") {
		//			return true;
		//		}
			
		} else if((writeStartYmd == tempStartYmd) && (tempUseYn == "미사용") && (writeUseYn == "N")){
				if(CRUDType=="U"){
					alert("미사용 상태인 '시작일이 동일한 수탁-부처 상한비율'이 이미 존재합니다.\n\n[" + tempCdIOName + " - " + tempCdName + " - " + tempStartYmd + " - " + tempUseYn + "]" );	
					selectboxUseYn.focus();
					return false;
				}
		  }
		}
		
	}
	//	if((inputPsnRate01.getValue()=='') && (inputPsnRate02.getValue()=='') && (inputPsnRate03.getValue()=='')) {
	//		alert("인원별 상한비율은 미 입력시 기본값으로 자동 등록되며 \n이후 수정을 통해 변경 가능합니다.\n\n[기본값]\n1인 : 100%\n2인 : 70%\n3인 : 50%");
	//		}
			return true;
	}
	
	
