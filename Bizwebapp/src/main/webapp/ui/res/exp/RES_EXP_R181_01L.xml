		CRUDType = "C";
		scwin.disableFalseAll();
		grp_detail_view.show();
		
		btn_del.hide();
		btn_cancel.show();
		btn_save.show();
		
		scwin.initDetail();
		selectboxUseYn.setValue('Y');
		selectboxUseYn.setReadOnly(true);
		selectboxUseYn.setDisabled(true);
	};
	
	/*
	* 저장 버튼 클릭 이벤트
	*/
	scwin.btn_save_onclick = function(e) {
		if(inputCalStartYmd.getValue() > inputCalCloseYmd.getValue()){
			alert("적용 종료일은 적용 시작일보다 이전이어야 합니다.");		
			inputCalCloseYmd.focus();
			inputCalCloseYmd.setValue("");
			return ;
		}
		if (!scwin.validate(dma_insert)) {
			return ;
		}else{
		if(!scwin.sameVali()){
			return ;
		}
		
		}
		
		com.confirm("message.xom.wq.017", function(ret) {
			if (ret) {
				scwin.update();
			}
		});
		
	};
    
    /*
	* 삭제 버튼 클릭 이벤트
