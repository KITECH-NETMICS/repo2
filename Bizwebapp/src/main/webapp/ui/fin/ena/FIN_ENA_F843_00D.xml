         <script type="text/javascript" lazy="false"><![CDATA[
    /****************************************************
 	 * 화면 설명 : 교부금신청
 	 * 변경 이력 : 2023-10-18	김준하	최초 작성
 	 * serviceId : F843
 	 ****************************************************/  
 	/**
	* 페이지 로드
	*/
 	scwin.data ={saveTag : "I",
 				 tag : "",
 				 recv_ymd : "",
 				 apr_state : "XAD010",
 				 reqNo : "",
 				 req_clsf : ""
 	             };        
	scwin.onpageload = function() {
		var codeOptions = [];
		com.setCmnCd(codeOptions, function() {
		 	// 첨부파일 컴포넌트 초기화
			com.file.init();
			var param = com.getParameter();
			var session = com.getLoginInfo();
			
			// enter key event
			com.setEnterKeyEvent(grp_emp, scwin.btn_hum_onclick);			
			
			if (param.param4 == "apr") {
				dma_search.set("req_no", param.req_no);
				dma_search.set("approval", "true");
			} else {			
				dma_search.setJSON(param);
			}			
			if (dma_search.get("req_no") == "") {
				dma_form_result.set("cud_type", "I");
				dma_form_result.set("req_psn_no", session.empno);
				dma_form_result.set("req_psn_nm", session.cn);
				dma_form_result.set("req_psn", session.sn);
				dma_form_result.set("apr_state", "XAD005");										
			}
			scwin.search();																						
		})					
	};
	
	scwin.search = function(e) {
		com.biz.checkRole({ serviceId : "F843", roleCodes : com.getLoginInfo('roleCode')}, function(e){
			scwin.data.isRoleYn = e.isRoleYn;			
			com.execute(sbm_selectData, { 
		        successCallback: function(e) { // 성공 콜백
	//				데이터콜렉션세팅					
					var session = com.getLoginInfo();

					if (e.responseJSON.elData.finGrantReqVo.unslip_no === undefined) {
						dma_form_result.set("unslip_no", "");
						dma_form_result.set("decsn_ex", "");
					}					
					if (e.responseJSON.elData.finGrantReqVo.req_psn === undefined || e.responseJSON.elData.finGrantReqVo.req_psn === null) {
						dma_form_result.set("req_psn_no", "");
						dma_form_result.set("req_psn_nm", "");
						dma_form_result.set("req_psn", "");
						dma_form_result.set("email", "");
						dma_form_result.set("ext_no", "");
					} else {
						// 신청자와 세션이 다를 경우 화면 비활성화					
						if((dma_form_result.get("req_psn") != com.getLoginInfo("sn")) || !(scwin.data.isRoleYn)){						
							btn_del.setDisabled(true);
							btn_save.setDisabled(true);
							btn_apr.setDisabled(true);
						}					
					}
					
					if (dma_form_result.get("decsn_ex") == "Y") text_decsn_ex_nm.setValue("확정");
					else text_decsn_ex_nm.setValue("미확정");					
																																
					if (dma_form_result.get("apr_state") == "XAD100") {
						group_apr.show();
						btn_save.hide();
						btn_del.hide();
						btn_apr.hide();
						if (dma_form_result.get("unslip_no") == "") {
							btn_slip.show("");
							btn_cancel.show("");
							btn_cancel.setDisabled(false);
							btn_slip.setDisabled(false);							
						} else {
							if (dma_form_result.get("decsn_ex") == "N") {
								btn_cancel.setDisabled(false);
								btn_slip.setDisabled(true);
							} else {
								btn_cancel.setDisabled(true);
								btn_slip.setDisabled(true);
							}
						}						
						if (session.roleCode.indexOf("1002258") > -1 || session.roleCode.indexOf("1002259") > -1 || session.roleCode.indexOf("1002138") > -1 ) {
							btn_slip.show("");
							btn_cancel.show("");
						} else {
							btn_slip.hide("");
							btn_cancel.hide("");
						}																																
					} else {
						btn_slip.hide("");
						btn_cancel.hide("");									
					}						
									
					// 버튼 제어
					com.biz.disabledButtons(dma_form_result.get("apr_state"), dma_form_result.get("req_no")); // 버튼 세팅
														
					if (e.responseJSON.elData.finGrantReqVo.req_no === undefined || e.responseJSON.elData.finGrantReqVo.req_no === null) {
						dma_form_result.set("cud_type", "I");
						btn_del.setDisabled(true);
					} else {					
						dma_form_result.set("cud_type", "U");
					}								
		      	}		      	
	        });
	    });
	}		
	
	scwin.onpageunload = function() {
		
	};
	
	//담당자 버튼클릭
	scwin.btn_hum_onclick = function(e) {
		var data={ work_clsf : "HAG010",
				   empno : ibx_emp_no.getValue(),
				   nm : ibx_emp_no.getValue(),
				   ref_yn : (ibx_emp_no.getValue() == "")? "Y" : "N" };	
		com.pop.open( com.pop.TYPE.HUM, data, function(ret){			
			dma_form_result.set("req_psn_no", ret.empno);
			dma_form_result.set("req_psn_nm", ret.nm);
			dma_form_result.set("req_psn", ret.syspayno);
			dma_form_result.set("email", ret.email);
			dma_form_result.set("ext_no", ret.ext_no);			
		});				   	
		
	};
	
	// 신청자 빈값일경우 이름, 시스템사번 데이터셋 초기화	
	scwin.ibx_emp_no_onchange = function(info) {
		if (info.newValue == "") {
			dma_form_result.set("req_psn_no", "");
			dma_form_result.set("req_psn_nm", "");
			dma_form_result.set("req_psn", "");
			dma_form_result.set("email", "");
			dma_form_result.set("ext_no", "");
		}					
	};

	// 저장 버튼 클릭	
	scwin.btn_save_onclick = function(e) {			
		dma_form_result.set("approval", "false");
		scwin.data.isApproval = false;
		scwin.callSave();		
	};
	
	/*
     * 저장 호출
     */
	scwin.callSave = function(){
		if(dma_form_result.get("req_psn") == ""){
			alert("신청자를 입력하세요");
			return;
		}		
		com.execute(sbm_saveData, {
			successCallback : function(e) { // 성공 콜백					
					//결재 이동
					if (scwin.data.isApproval) {												
						com.movePageMenu("A001", { "reqNo" : dma_form_result.get("req_no")});
					} else {
						com.alert("message.xom.wq.023"/*정상적으로 처리되었습니다.*/, function() {
							scwin.data.saveFlag = true;
							dma_search.set("req_no", e.responseJSON.elData.req_no);				
							// 조회
							scwin.search();
						});										
					}					
			}
		});			
	};
		
	// 삭제 버튼 클릭	
	scwin.btn_del_onclick = function(e) {
		var req_no = ibx_req_no.getValue();
		if (req_no == "") {
			alert("신청서가 존재하지 않습니다.");
			return;
		}
		com.confirm("message.xom.wq.022" /*삭제 하시겠습니까?*/ , function(ret) {
			if (ret) {
				com.execute(sbm_deleteData, {
					successCallback : function(e) { // 성공 콜백					
						com.alert("message.xom.wq.023"/*정상적으로 처리되었습니다.*/, function() {
							scwin.data.save_flag = true;
							com.moveList("F842", scwin.data.save_flag);
						});										
					}
				});								
			}
		});				
	};
	
	//결재신청	
	scwin.btn_apr_onclick = function(e) {
		dma_form_result.set("approval", "true");	
		scwin.data.isApproval = true;
		scwin.callSave();
	};
	
	//목록이동
	scwin.btn_list_onclick = function(e) {
		var data={};
//		com.openTabUrl("가지급금신청", "/ui/fin/spa/FIN_SPA_F416_00L.xml", 1, data, function() {
//			scwin.search();
//		});
		com.moveList("F842", data);				
	};
	
	//결의생성
	scwin.btn_slip_onclick = function(e) {	
		if (ibx_unslip_no.getValue() != "") {
			alert("이미 결의생성이 완료되었습니다.");
			return;
		}
		
		if (ica_slip_ymd.getValue() == "") {
			alert("결의일자를 입력하세요");
			return;
		}
		
		if (WebSquare.util.getNumber(ibx_req_dvrq_amount.getValue()) == 0) {
			alert("교부신청금액이 존재하지 않습니다.");
			return;
		}
		
		var slip_ymd = ica_slip_ymd.getValue();
		var slip_typ_cd = "156";

		var unslip_no_seq = slip_typ_cd + slip_ymd.substring(2,4);

		if ( slip_ymd.substring(4,6) == "10") {
			unslip_no_seq = unslip_no_seq + "A";
		} else if( slip_ymd.substring(4,6) == "11") {
			unslip_no_seq = unslip_no_seq + "B";
		} else if( slip_ymd.substring(4,6) == "12") {
			unslip_no_seq = unslip_no_seq + "C";
		} else {
			unslip_no_seq = unslip_no_seq + slip_ymd.substring(5,6);
		}
		dma_slip.set("slip_typ_cd", slip_typ_cd);
		dma_slip.set("unslip_no_seq", unslip_no_seq);
		dma_slip.set("slip_ymd", slip_ymd);
		dma_slip.set("wrte_ymd", com.getServerDateTime());
		dma_slip.set("wrte_psn", com.getLoginInfo().sn);
		dma_slip.set("wrte_dept", com.getLoginInfo().deptCode);
		dma_slip.set("accnt_no", dma_form_result.get("accnt_no"));
		dma_slip.set("unslip_amt", ibx_req_dvrq_amount.getValue());
		dma_slip.set("bank", "FAA003");
		dma_slip.set("bank_nm", "기업은행");			
		dma_slip.set("bankaccnt_no", "391-006608-04-017");			
		dma_slip.set("depstr_nm", ibx_rcpmny_dpstr_nm.getValue());			
		dma_slip.set("rmk_2", ibx_reqst_instt_nm.getValue());			
		dma_slip.set("req_no", ibx_req_no.getValue());
		
		com.execute(sbm_saveSlip, { 
	        successCallback: function(e) { // 성공 콜백
				com.alert("message.xom.wq.023"/*정상적으로 처리되었습니다.*/, function() {
					scwin.data.saveFlag = true;
					dma_search.set("req_no", e.responseJSON.elData.req_no);				
					// 조회
					scwin.search();				
				});		        	        
	        }
		});												
	};

	//결의취소	
	scwin.btn_cancel_onclick = function(e) {
		if(ibx_unslip_no.getValue() == ""){
			alert("결의서가 존재하지 않습니다. 확인바랍니다.");
			return;
		}
		
		if(dma_form_result.get("decsn_ex") == "Y"){
			alert("결의확정이 되었으므로 결의취소할 수 없습니다.");
			return;
		}
		
		com.execute(sbm_cancelSlip, { 
	        successCallback: function(e) { // 성공 콜백
				com.alert("message.xom.wq.023"/*정상적으로 처리되었습니다.*/, function() {
					scwin.data.saveFlag = true;
					dma_search.set("req_no", e.responseJSON.elData.req_no);				
					// 조회
					scwin.search();				
				});		        	        
	        }
		});					
		
	};
	]]></script>
    </head>
    <body ev:onpageload="scwin.onpageload" ev:onpageunload="scwin.onpageunload">
