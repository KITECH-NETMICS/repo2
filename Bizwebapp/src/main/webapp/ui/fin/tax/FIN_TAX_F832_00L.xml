    							<w2:attributes></w2:attributes>
    							<xf:input adjustMaxLength="false" class="inp" id="ibx_selr_corp_no" style="width: 150px;" ref="data:dma_search.selr_corp_no" allowChar="0-9" displayFormat="###-##-#####"></xf:input>
    						</xf:group>
    						<xf:group class="w2tb_th" tagname="th">
    							<w2:attributes>
    								<w2:colspan>1</w2:colspan>
    								<w2:rowspan>1</w2:rowspan>
    							</w2:attributes>
    							<w2:textbox id="" label="거래처명" style="" tagname="span"></w2:textbox>
    						</xf:group>
    						<xf:group class="w2tb_td" tagname="td">
    							<w2:attributes></w2:attributes>
    							<xf:input adjustMaxLength="false" class="inp" id="" ref="data:dma_search.selr_corp_nm"
    								style="width: 150px;">
    							</xf:input>
    						</xf:group>
    						<xf:group tagname="th" class="w2tb_th">
    							<w2:attributes></w2:attributes>
    							<w2:textbox id="" label="계산서 구분" style="" tagname="span"></w2:textbox>
