            			<w2:column id="regs_date" name="작성일자" dataType="text"></w2:column>
            			<w2:column id="chrg_amt" name="공급가액" dataType="text"></w2:column>
            			<w2:column id="tax_amt" name="부가세액" dataType="text"></w2:column>
            			<w2:column id="totl_amt" name="합계금액" dataType="text"></w2:column>
            			<w2:column id="selr_code" name="공급자 종사업자번호" dataType="text"></w2:column>
            			<w2:column id="selr_ceo" name="공급자 대표자명" dataType="text"></w2:column>
            			<w2:column id="selr_corp_no" name="공급자사업자번호" dataType="text"></w2:column>
            			<w2:column id="selr_corp_nm" name="공급자상호" dataType="text"></w2:column>
            			<w2:column id="buyr_chrg_email1" name="공급받는자 담당자 이메일" dataType="text"></w2:column>
            			<w2:column id="bill_type" name="구분" dataType="text"></w2:column>
            			<w2:column id="match_cd" name="대사결과" dataType="text"></w2:column>
            			<w2:column id="tax_type" name="세금계산서종류" dataType="text"></w2:column>
            			<w2:column id="chk" name="선택" dataType="text"></w2:column>
            		</w2:columnInfo>
            	</w2:dataList>
            	<w2:dataList baseNode="list" id="dlt_grd" repeatNode="map" saveRemovedData="true" style="">
            		<w2:columnInfo>
            			<w2:column dataType="text" id="issu_seqno" name="국세청승인일련번호"></w2:column>
            			<w2:column dataType="text" id="issu_id" name="국세청전송일련번호"></w2:column>
            			<w2:column dataType="text" id="regs_date" name="작성일자"></w2:column>
            			<w2:column dataType="text" id="chrg_amt" name="공급가액"></w2:column>
            			<w2:column dataType="text" id="tax_amt" name="부가세액"></w2:column>
            			<w2:column dataType="text" id="totl_amt" name="합계금액"></w2:column>
            			<w2:column dataType="text" id="tax_returns_yn" name="부가세신고확인"></w2:column>
            			<w2:column dataType="text" id="unslip_no" name="결의번호"></w2:column>
            			<w2:column dataType="text" id="selr_chrg_email" name="공급자담당자이메일"></w2:column>
            			<w2:column dataType="text" id="selr_code" name="공급자 종사업자번호"></w2:column>
            			<w2:column dataType="text" id="selr_ceo" name="공급자 대표자명"></w2:column>
            			<w2:column dataType="text" id="selr_corp_no" name="공급자사업자번호"></w2:column>
            			<w2:column dataType="text" id="selr_corp_nm" name="공급자상호"></w2:column>
            			<w2:column dataType="text" id="buyr_chrg_email1" name="공급받는자 담당자 이메일"></w2:column>
            			<w2:column dataType="text" id="bill_type" name="구분"></w2:column>
            			<w2:column dataType="text" id="match_cd" name="대사결과"></w2:column>
            			<w2:column dataType="text" id="tax_type" name="세금계산서종류"></w2:column>
            			<w2:column id="pops_code" name="영수청구구분" dataType="text"></w2:column>
            			<w2:column id="buy_date" name="구매일자" dataType="text"></w2:column>
            		</w2:columnInfo>
            	</w2:dataList>
            </w2:dataCollection>
            <w2:workflowCollection/>
            <!-- 국세청 데이터 조회 -->
    		<xf:submission id="sbm_selectKtxData" ref="data:json,dma_search" target='' action="SvcFINPURTAXR06.pwkjson" method="post"
    			mediatype="application/json" encoding="UTF-8" instance="" replace="" errorHandler="" customHandler="" mode="asynchronous"
    			processMsg="" ev:submit="" ev:submitdone="" ev:submiterror="" abortTrigger="">
    		</xf:submission>
    		<!-- 매입계산서 데이터 조회 -->
    		<xf:submission id="sbm_selectData" ref="data:json,dma_search" target='' action="SvcFINPURTAXR07.pwkjson" method="post"
    			mediatype="application/json" encoding="UTF-8" instance="" replace="" errorHandler="" customHandler="" mode="asynchronous"
    			processMsg="" ev:submit="" ev:submitdone="" ev:submiterror="" abortTrigger="">
    		</xf:submission>
    		<!-- 전체이관 -->
    		<xf:submission id="sbm_saveMove" ref="data:json,dma_search" target='' action="SvcFINPURTAXI01.pwkjson" method="post"
    			mediatype="application/json" encoding="UTF-8" instance="" replace="" errorHandler="" customHandler="" mode="asynchronous"
    			processMsg="" ev:submit="" ev:submitdone="" ev:submiterror="" abortTrigger="">
    		</xf:submission>
        </xf:model>
         <script type="text/javascript" lazy="false"><![CDATA[
    /****************************************************
 	 * 화면 설명 : 자동대사
	 * 변경 이력 : 2022-10-13	이영희	최초 작성
	 * serviceId : F832
	 * 2023.10.17. 이영희 자동대사, 수동대사, 대사확인 안보이게 처리
 	 ****************************************************/  
 	scwin.data ={cnt : 0};
 	
 	/**
	 * 페이지 로딩 이벤트
	 */
	scwin.onpageload = function() {		
		var today = com.getServerDateTime();
		dma_search.set("from_ymd", today.substring(0, 6) + "01");
		dma_search.set("to_ymd", today);
		
		// 자동대사, 수동대사는 사용하지 않으므로 보이지 않게 변경
//		cbx_replc_cond.checkAll(true);
//		grp_compare.hide();
		
		// 기간 역전 체크
		com.checkPeriod(ica_from, ica_to);		
	};
	
	scwin.onpageunload = function() {
		
	};
	
	/**
	 * 국세청 데이터 조회 리스트 조회
	 */
	scwin.searchKtx = function(){	
		com.execute(sbm_selectKtxData, {
			successCallback : function(e) {
				dlt_ktx.setJSON(e.responseJSON.ktxIssuMstrVoList);
				tbx_cnt.setValue(dlt_ktx.getRowCount());				
			}
		});
	};
	
	/**
	 * 이관 데이터 조회 리스트 조회
	 */
	scwin.search = function(){			
		if(ibx_selr_corp_no.getValue() != ""){
			dma_search.set("selr_corp_no_1", ibx_selr_corp_no.getDisplayValue());
		}
		
		com.execute(sbm_selectData, {
			successCallback : function(e) {
				dlt_grd.setJSON(e.responseJSON.ktxIssuMstrVoList);
				spn_cnt.setValue(dlt_grd.getRowCount());				
			}
		});
	};
	
	/**
	 * 대산 전 체크
	 */
	scwin.preCopy = function(){
		if(cbx_replc_cond.getValue() == ""){
			alert("대사 대상 컬럼을 선택하십시오.");
			return false;
		}
		
		return true;
	};
	
	/**
	 * 국세청 데이터를 내부자료로 복사
	 */
	scwin.copyRow = function(fromIdx, toIdx){
		var diffCount = 0; // 불일치 건수
		// 승인번호,세금계산서종류,거래처코드는 기본적으로 비교한다. 
		//또, 승인번호,세금계산서종류,거래처코드는 불일치 판단여부에서 제외되며 무조건 복사한다.
		dlt_grd.setCellData(toIdx, "issu_id", dlt_ktx.getCellData(fromIdx, "issu_id"));
		if(dlt_ktx.getCellData(fromIdx, "tax_type") != "9999"){
			dlt_grd.setCellData(toIdx, "tax_type", dlt_ktx.getCellData(fromIdx, "tax_type"));
		}
		dlt_grd.setCellData(toIdx, "selr_code", dlt_ktx.getCellData(fromIdx, "selr_code"));

		var chkConds = cbx_replc_cond.getValue().split(",");
		
		for(var i=0; i<chkConds.length; i++){
			debugger;
			if(dlt_ktx.getCellData(fromIdx, chkConds[i]) != dlt_grd.getCellData(toIdx, chkConds[i])){
				diffCount++;
			}
		}
		
		return diffCount;
	};
	
	/**
	 * 국세청 데이터와 내부자료 일치여부에 따른 컬러 셋팅
	 */
	scwin.setColor = function(rowIdx, colIdx, sameYn){
		if(sameYn == "Y"){
			grd_bill.setCellBackgroundColor(rowIdx, colIdx, "#FFFFFF");
			grd_bill.setCellColor(rowIdx, colIdx, "#333333");
		}else{
			grd_bill.setCellBackgroundColor(rowIdx, colIdx, "#FFFF80");
			grd_bill.setCellColor(rowIdx, colIdx, "#FF0000");
		}
	};
	
	/**
	 * 국세청 데이터 이관
	 */
	scwin.moveData = function(){
		if(dma_search.get("from_ymd") == "" || dma_search.get("to_ymd") == ""){
			com.alert(com.getMsg("message.xom.ui.fin.026", "조회기간"));
			return;
		}
		
		com.confirm("국세청데이터를 이관하시겠습니까?", function(ret){
			com.execute(sbm_saveMove, {
				successCallback : function(e){
					btn_search.trigger("click");
				}
			});
		});		
	};
	
	//--------------------------------------------------------------------------- event --------------------------------//	
	/**
	 * 조회
	 */
	scwin.btn_search_onclick = function(e) {
		if(dma_search.get("from_ymd") == "" || dma_search.get("to_ymd") == ""){
			com.alert(com.getMsg("message.xom.ui.fin.026", "조회기간"));
			return;
		}
		
		// 1) 국세청 데이터 조회
		scwin.searchKtx();
		
		// 2) 이관 데이터 조회
		scwin.search();	
	};		
	
	/**
	 * 엑셀다운로드 버튼 클릭 event
	 */
	scwin.btn_excel_onclick = function(e) {
		var option = {
			    fileName : "내부자료_" + com.getServerDateTime() + ".xls"
			  , useDataFormat : "true"
//			  , hiddenVisible : true
		};
		com.downloadGridExcel(grd_bill, option);
	};
	
	/**
	 * 엑셀다운로드 버튼 클릭 event
	 */
	scwin.btn_ktx_excel_onclick = function(e) {
		var option = {
			    fileName : "국세청자료_" + com.getServerDateTime() + ".xls"
			  , useDataFormat : "true"
//			  , hiddenVisible : true
			  , removeColumns : "0"
		};
		com.downloadGridExcel(grd_ktx, option);
	};
	
	/**
	 * 자동대사 버튼 클릭 event
	 */
	scwin.btn_auto_replc_onclick = function(e) {
		com.confirm("자동대사를 진행하시겠습니까?", function(ret){
			if(ret){
				if(!scwin.preCopy()) return;		
		
				for(var i=0; i<dlt_ktx.getRowCount(); i++){
					var rowIdx = dlt_grd.getMatchedIndex("issu_id", dlt_ktx.getCellData(i, "issu_id", true))[0];
					//전자계산서 행 정보를 세금계산서 행으로 복사한다.
					if(rowIdx >= 0){
						if(scwin.copyRow(i, rowIdx) > 0){
							dlt_grd.setCellData(rowIdx, "match_cd", "3"); // 불일치
							scwin.setColor(rowIdx, "match_cd", "N");
						}else{
							dlt_grd.setCellData(rowIdx, "match_cd", "1"); // 자동일치
							scwin.setColor(rowIdx, "match_cd", "Y");
						}
					}			
				}
				
				alert("자동대사를 완료하였습니다.");
			}
		});
	};
	
	/**
	 * 수동대사 버튼 클릭 event
	 */
	scwin.btn_mnl_replc_onclick = function(e) {
		var fromIdx = grd_ktx.getFocusedRowIndex();
		if(fromIdx < 0){
			alert("국세청 자료를 선택하세요.");
			return;
		}		
		
		var toIdx = grd_bill.getFocusedRowIndex();
		if(toIdx < 0){
			alert("내부 자료를 선택하세요.");
			return;
		}
		
		com.confirm("수동대사를 진행하시겠습니까?", function(ret){
			if(ret){
				if(dlt_ktx.getCellData(fromIdx, "match_cd") != ""){
					if(!confirm("해당 국세청 자료는 이미 대사처리하였습니다. 다시 대사처리 하시겠습니까?")) return;
				}
				
				if(scwin.copyRow(fromIdx, toIdx) > 0){
					dlt_grd.setCellData(toIdx, "match_cd", "3"); // 불일치
					scwin.setColor(toIdx, "match_cd", "N");
				}else{
					dlt_grd.setCellData(toIdx, "match_cd", "2"); // 수동일치
					scwin.setColor(toIdx, "match_cd", "Y");
				}
				
				alert("수동대사를 완료하였습니다.");
			}
		});
	};
	
	/**
	 * 대사확인 버튼 클릭 event
	 */
	scwin.btn_save_onclick = function(e) {
		if(dlt_grd.getUpdatedIndex().length == 0){
			alert("변경된 데이터가 없습니다.");
			return;
		}
		
		com.confirm("수정된 정보를 저장하시겠습니까?", function(ret){
			
		});
	};
	
	/**
	 * 전체이관 버튼 클릭 event
	 */
	scwin.btn_move_onclick = function(e) {
		dma_search.set("inqr_clsf", "A");
		scwin.moveData();
	};
	
	/**
	 * 선택이관 버튼 클릭 event
	 */
	scwin.btn_move_sel_onclick = function(e) {
		dma_search.set("inqr_clsf", "S");
		
		var rows = grd_ktx.getCheckedIndex("chk");
		if(rows.length == 0){
			alert("이관할 데이터를 선택하세요.");
			return;	
		}
		
		var issuIds = [];
		for(var i=0; i<rows.length; i++){
			issuIds.push(dlt_ktx.getCellData(rows[i], "issu_id"));	
		}
		
		dma_search.set("issu_id", issuIds.join(","));
		
		scwin.moveData();
	};
	]]></script>
    </head>
