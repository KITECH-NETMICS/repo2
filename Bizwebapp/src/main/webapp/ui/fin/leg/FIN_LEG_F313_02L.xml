				dlt_grd.setJSON(e.responseJSON.fspRamtCrateVoList)
				
				
				if(sbx_accnt_cd.getValue() == ''|| sbx_accnt_cd.getValue()==null || sbx_accnt_cd.getValue() === undefined) {
					if(inp_empno.getValue() == '' || inp_empno.getValue() == '') {
					 alert('계정책임자 조회시 연구책임자 입력은 필수입니다.');
					 return;
					}				
				}
				
				if(dlt_grd.getRowCount() == 0) {
					alert("엑셀다운로드 받을 데이터가 없습니다.");
					return;
				}
		
				var option = {
					    fileName : "채권채무현황.xlsx"
					  , useDataFormat : true
//					  , hiddenVisible : true
				};
				
				com.downloadGridExcel(grd_list, option);
			}
			
		});
		dlt_grd.setJSON('');
	}
	
