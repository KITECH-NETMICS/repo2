	/**
	 * 회계코드 변경 이벤트
	 */
	scwin.sbx_accnt_cd_onviewchange = function(info) {
		dlt_grd.setJSON('');
		var data = dlt_code.getMatchedJSON("accnt_cd", this.getValue(), true)[0]; 
		if(data === undefined){
		grp_mngmt.hide();
		textResLabel.show();
		grpResEmpno.show();
		}
		scwin.data=data;
	//	alert(sbx_accnt_cd.getValue()); //코드확인
		debugger;
		
		//회계코드 변경시마다 입력 값들 초기화 (입력의 종류가 바뀌기 때문)
		ibx_mngmt_item_1.setValue('');
		ibx_mngmt_item_nm_1.setValue('');
		mngmt_item_1.setValue('');
		mngmt_item_nm_1.setValue('');
		ibx_mngmt_item_2.setValue('');
		ibx_mngmt_item_nm_2.setValue('');
		mngmt_item_2.setValue('');
		mngmt_item_nm_2.setValue('');
		inp_empno.setValue('');
		inp_nm.setValue('');
		
		
		if(data.mngmt_item_cd_1.trim() == ""){ //가져온 데이터의 1번째 요소가 비어 있을 때
			grp_mngmt.hide();
			tbx_mngmt_item_1.setValue("");
			grp_mngmt_item_cd_1.hide();
			ibx_mngmt_item_nm_1.hide();

		}else{
			
			tbx_mngmt_item_1.setValue(data.mngmt_item_nm_1);
			grp_mngmt_item_cd_1.show();
			ibx_mngmt_item_nm_1.show();
			grp_mngmt.show("");
			
			textResLabel.hide(); //연구책임자 관련 숨기기
			grpResEmpno.hide();
			
			if(data.data_form_1 == "popup" || scwin.data.mngmt_item_cd_1 == "8005"){
				ibx_mngmt_item_1.setStyle("width", "58px");
				mngmt_item_1.setStyle("width", "58px");
				
				grp_mngmt_item_cd_1.show();
				grp_mngmt_item_cd_1_last.show();
				
				mngmt_item_nm_1.show();
				btn_mngmt_item_1.show("");
				btn_mngmt_item_1_end.show("");
				ibx_mngmt_item_nm_1.show("");
				ibx_mngmt_item_nm_1.setReadOnly(true);
				mngmt_item_nm_1.setReadOnly(true);
			
				ibx_mngmt_item_nm_1.setStyle("margin-left", "4px");
				
				
				btn_mngmt_item_1.setDisabled(false);
			}else if(data.data_form_1 == "string" && scwin.data.mngmt_item_cd_1 != "8005" ){

			ibx_mngmt_item_1.setStyle("width", "88px");
			mngmt_item_1.setStyle("width", "88px");
			
			ibx_mngmt_item_nm_1.hide();
			mngmt_item_nm_1.hide();
			btn_mngmt_item_1.hide();
			btn_mngmt_item_1_end.hide();
			}else{
				btn_mngmt_item_1.hide();
				ibx_mngmt_item_nm_1.hide();
				btn_mngmt_item_1.setDisabled(true);
			}			
		}
		
		//가져온 데이터의 2번째 요소가 비어 있을 때 //통제번호,기술지원접수번호 임시 제외
		if(data.mngmt_item_cd_2.trim() == "" || scwin.data.mngmt_item_cd_2 == "2080" || scwin.data.mngmt_item_cd_2 == "3006"){ 
			allSecond.hide();
			tbx_mngmt_item_2.setValue("");
			grp_mngmt_item_cd_2.hide();
			ibx_mngmt_item_nm_2.hide();

		}else{
			allSecond.show();
			tbx_mngmt_item_2.setValue(data.mngmt_item_nm_2);
			grp_mngmt_item_cd_2.show("");

			if(data.data_form_2 == "popup"){
				btn_mngmt_item_2.show("");
				ibx_mngmt_item_nm_2.show("");
				btn_mngmt_item_2.setDisabled(false);
				ibx_mngmt_item_nm_2.setReadOnly(true);
				mngmt_item_nm_2.setReadOnly(true);
				ibx_mngmt_item_nm_2.setStyle("margin-left", "4px");
				
			}else if(data.data_form_2 == "string"){
				ibx_mngmt_item_nm_2.hide();
			}
			else{
				btn_mngmt_item_2.hide();
				ibx_mngmt_item_nm_2.hide();
				btn_mngmt_item_2.setDisabled(true);
			}			
		}
	};	

	

	
