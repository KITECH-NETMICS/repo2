			<!-- 조회 -->
			<xf:submission id="sbm_selectData" ref="data:json,dma_search" target='' action="SvcFINFINRESMR10.pwkjson" method="post"
				mediatype="application/json" encoding="UTF-8" instance="" replace="" errorHandler="" customHandler="" mode="asynchronous" processMsg=""
				ev:submit="" ev:submitdone="" ev:submiterror="" abortTrigger="">
			</xf:submission>
			<!-- 회계코드 조회 -->
			<xf:submission id="sbm_selectAccntList" ref="data:json,dma_search" target='data:json,{"id":"dlt_code","key":"accntList"}' 
        	    action="SvcFINSPMPOPR08.pwkjson" method="post" mediatype="application/json"
        		encoding="UTF-8" instance="" replace="" errorHandler="" customHandler="" mode="asynchronous" processMsg="" ev:submit=""
        		ev:submitdone="" ev:submiterror="" abortTrigger="">
        	</xf:submission>
		</xf:model>
		<script type="text/javascript" lazy="false"><![CDATA[

	scwin.service_id = "";
	var sn = com.getLoginInfo("sn");
	
	/*
     *  날짜 설정
     */
	var now = new Date();
	var defalutday = gcm.date.formatDateTime(now, "yyyyMMddHHmm"); // yyyyMMddHHmm
	var ymd = gcm.date.formatDateTime(now); // yyyyMMdd 
	var year = ymd.substr(0, 4); // yyyy
 	
 	
 	 
    /*
     * 페이지 열기
     */          
	scwin.onpageload = function() {
		scwin.service_id = com.getParameter("menuObj").serviceID;
		ica_from_ymd.setValue(year + "0101");
		ica_to_ymd.setValue(ymd);
		com.setEnterKeyEvent(grp_accnt_no, scwin.btn_accnt_no_onclick);
		com.setEnterKeyEvent(grp_accnt_cd, scwin.btn_accnt_cd_onclick);
		
		grp_mngmt_item_nm_1.hide();
		btn_mngmt_item_1.setDisabled(true);
		tbx_mngmt_item_1.setValue("관리항목1");
		btn_mngmt_item_2.setDisabled(true);
		tbx_mngmt_item_2.setValue("관리항목2");
		
		/*
	     *  프로젝트 예산편성 -> 계정원장상세
	     */ 
	 	var p_accnt_no = com.getParameter("accnt_no");
		var p_inputYr = com.getParameter("inputYr");
		var p_xml_id = com.getParameter("xml_id");
		var p_inputAccntCd = com.getParameter("inputAccntCd"); 
		
		if (p_xml_id == "JF32" || p_xml_id == "RF33") {//과제관리>과제수행>프로젝트예산편성관리
			dma_search.set("from_ymd", p_inputYr + "0101");
			dma_search.set("to_ymd", p_inputYr + "1231");
			dma_search.set("accnt_cd", p_inputAccntCd);
			
			if(p_inputAccntCd == '21021082' || p_inputAccntCd == '21021084'){
				dma_search.set("mngmt_item_1", p_accnt_no);
			} else {
				dma_search.set("accnt_no", p_accnt_no);
				scwin.btn_accnt_no_onclick();
			}
			
			grp_search.setDisabled(true);

			scwin.search();
		}
		
		if(p_inputAccntCd == '21021082' || p_inputAccntCd == '21021084'){
			dma_search.set("mngmt_item_1", p_accnt_no);
		} else {
			dma_search.set("accnt_no", p_accnt_no);
		}
	};
	
	scwin.onpageunload = function() {
		
	};
	
	scwin.btn_accnt_cd_onclick = function(e) {
		var data ={accnt_cd : ibx_accnt_cd.getValue(),
				   ref_yn : (ibx_accnt_cd.getValue() == "")? "Y" : "N"
				  };
				  
		com.pop.open(com.pop.TYPE.ACNCD, data, function(retObj) {
			if (retObj) {
				dma_search.set("accnt_cd", retObj.accnt_cd);
				ibx_accnt_cd_abbr.setValue(retObj.accnt_cd_abbr);
				
				com.execute(sbm_selectAccntList, {
					successCallback : function(e) {
						scwin.accnt_cd_change(retObj.accnt_cd);
					}
				});
			}
		});	
	};
	
	scwin.btn_accnt_no_onclick = function(e) {
		var data={
			accnt_no : ibx_accnt_no.getValue(),
			ref_yn : (ibx_accnt_no.getValue() == "")? "Y" : "N"
		};
		
		com.pop.open(com.pop.TYPE.ACNNO, data, function(ret){
			ibx_accnt_no.setValue(ret.accnt_no);
			ibx_accnt_no_nm.setValue(ret.accnt_no_nm);
		});
	};
	
	scwin.btn_search_onclick = function(e) {
		// 기간 체크
		com.checkPeriod(ica_from_ymd, ica_to_ymd);
		
		var accnt_no_chk = ibx_accnt_no.getValue();
		var accnt_cd_chk = ibx_accnt_cd.getValue();
		
		if (accnt_cd_chk.length != 8) {
			alert("회계코드는 8자리를 입력해주세요.");
			return;
		}

		if (accnt_no_chk != "" && accnt_no_chk.length != 8) {
			alert("계정번호는 8자리를 입력해주세요.");
			return;
		}
		
		scwin.search();
	};
	
	/*
    *  조회
    */
	scwin.search = function() {		
		com.execute(sbm_selectData, {
			successCallback : function(e){
				dlt_grd.setJSON(e.responseJSON.finBgaCctmDetlVoList);
				tbx_result.setValue(dlt_grd.getRowCount());
			},
			
			isImplExcel: true,
			excelCallback : function(e) {
				// DB 다운로드
				var option = {
					fileName : "계정원장상세.xlsx"
					, providerInfo : {
						  service : "finBgaCctmService"
						, method : "selectFinBgaCctmAccntInfoExcel_1"
						, vo : "fin.leg.vo.FinBgaCctmVo"
						, paramMap : dma_search
					}
				};
				com.downloadGridExcel(grd_listView, option);
			}
		});
	};
	
	scwin.btn_excel_onclick_1 = function(e) {
		if(dlt_grd.getRowCount() == 0) return;
		
		var option = {
			fileName : "계정원장상세.xlsx"
			, useDataFormat : true
//			, hiddenVisible : true
			, useSubTotal : "false"
		};
		com.downloadGridExcel(grd_list, option);
	};
	
	scwin.btn_excel_onclick_2 = function(e) {
		dma_search.set("clsf", "EXCEL");
	
		var option = {
			  fileName : "계정원장상세(부가세신고용).xlsx"
			, hiddenVisible : false
			, useDataFormat : true
			, providerInfo : {
				  service : "finBgaCctmService"
				, method : "selectFinBgaCctmAccntInfoExcel_2"
				, vo : "fin.leg.vo.FinBgaCctmVo"
				, paramMap : dma_search
			}
		};

		com.downloadGridExcel(grd_excel_list, option);
	};
	
	scwin.btn_oz_print_onclick = function(e) {
		var accnt_cd = ibx_accnt_cd.getValue().trim(); 			// 회계코드
		var accntName = ibx_accnt_cd_abbr.getValue().trim(); 	// 회계코드명
		var startYmd = ica_from_ymd.getValue(); 				// 조회시작기간
		var endYmd = ica_to_ymd.getValue(); 					// 조회종료기간
		var accntNo = ibx_accnt_no.getValue();
		var accntNoNm = ibx_accnt_no_nm.getValue();
		var ramtMngmt1 = ibx_mngmt_item_1.getValue();
		var ramtMngmt2 = ibx_mngmt_item_2.getValue();
		var startMod = ""; //조회시작전 년도의 01월
		var endMod = ""; //조회시작전까지의 달
		var startCum = ""; //조회시작 해당달의 1일
		var endCum = ""; //조회시작 해당달의시작일 하루전

		//조회일자가 1일 일경우에는 확정결의서에서 자료를 가져올 필요가 없으므로 00으로 처리하고 원장집계에서만 자료를 가져옴
		if (startYmd.substring(6, 8) == '01') {
			startCum = startYmd.substring(0, 6) + "00";
			endCum = startYmd.substring(0, 6) + "00";
		} else {
			startCum = startYmd.substring(0, 6) + '01';
			endCum = WebSquare.date.dateAdd(startYmd, -1);
		}

		//조회일이 1월부터 시작할 경우 이월금액의 from to를 각각 01월로..아닐경우 01월부터 조회일의 전월까지..
		if (startYmd.substring(4, 6) == "01") {
			startMod = startYmd.substring(0, 4) + "01";
			endMod = startYmd.substring(0, 4) + "01";
		} else {
			startMod = startYmd.substring(0, 4) + "01";
			endMod = WebSquare.date.dateAdd(startYmd.substring(0, 6) + "01", -1).substring(0, 6);
		}

		var param = "accnt_cd=" + accnt_cd + "&accntName=" + accntName + "&startYmd=" + startYmd + "&endYmd=" + endYmd;
		param = param + "&startMod=" + startMod + "&endMod=" + endMod + "&startCum=" + startCum + "&endCum=" + endCum;
		param = param + "&barcode_1=" + defalutday + "&barcode_2=" + sn + "&barcode_3=" + ymd + "&scrn_id=" + scwin.service_id;
		param = param + "&accntNo=" + accntNo + "&ramtMngmt1=%" + ramtMngmt1 + "%&ramtMngmt2=%" + ramtMngmt2 + "%&accntNoNm=" + accntNoNm;
		
		var reportObj = {
			ozrCode : "F306", // fin/fin/leg/FinAccntLedger3Sel.ozr
			param : param
		};
		
		let option = {
			id : "1",     // default ""
		    width : 1400,       // defualt 1012
		    height : 800        // default 712
		};
		
		com.oz.open(reportObj, option);
	};
	
	scwin.ibx_accnt_no_onchange = function(info) {
		if (this.getValue() == "") {
			ibx_accnt_no_nm.setValue("");
		}
	};
	
	scwin.ibx_accnt_cd_onchange = function(info) {
		if (this.getValue() == "") {
			ibx_accnt_cd_abbr.setValue("");
			scwin.accnt_cd_change();
		}
	};
	
	/*
    *  회계코드 event
    */
	scwin.accnt_cd_change = function(accnt_cd) {
		var data = dlt_code.getMatchedJSON("accnt_cd", accnt_cd, true)[0];
		if(data === undefined){
			grp_mngmt_item_nm_1.hide();
			btn_mngmt_item_1.setDisabled(true);
			tbx_mngmt_item_1.setValue("관리항목1");
			btn_mngmt_item_2.setDisabled(true);
			tbx_mngmt_item_2.setValue("관리항목2");
			return;
		}
		
		scwin.data = data;
		
		if(data.mngmt_item_cd_1.trim() == ""){
			grp_mngmt_item_nm_1.hide();
			btn_mngmt_item_1.setDisabled(true);
			tbx_mngmt_item_1.setValue("관리항목1");
			btn_mngmt_item_2.setDisabled(true);
			tbx_mngmt_item_2.setValue("관리항목2");
		}else{
			grp_mngmt_1.show("");
			grp_mngmt_2.show("");
			grp_mngmt_3.show("");
			grp_mngmt_4.show("");
		}
		
		if(data.mngmt_item_cd_1.trim() == ""){
			grp_mngmt_item_nm_1.hide();
			btn_mngmt_item_1.setDisabled(true);
			tbx_mngmt_item_1.setValue("관리항목1");
		}else{
			tbx_mngmt_item_1.setValue(data.mngmt_item_nm_1);
			grp_mngmt_item_cd_1.show("");
			if(data.data_form_1 == "popup"){
				btn_mngmt_item_1.show("");
				grp_mngmt_item_nm_1.show("");
				btn_mngmt_item_1.setDisabled(false);
			}else{
				grp_mngmt_item_nm_1.hide();
				btn_mngmt_item_1.setDisabled(true);
				tbx_mngmt_item_1.setValue("관리항목1");
			}			
		}
		
		if(data.mngmt_item_cd_2.trim() == ""){
			btn_mngmt_item_2.setDisabled(true);
			tbx_mngmt_item_2.setValue("관리항목2");
		}else{
			tbx_mngmt_item_2.setValue(data.mngmt_item_nm_2);
			grp_mngmt_item_cd_2.show("");
			if(data.data_form_2 == "popup"){
				btn_mngmt_item_2.show("");
				btn_mngmt_item_2.setDisabled(false);
			}else{
				btn_mngmt_item_2.setDisabled(true);
				tbx_mngmt_item_2.setValue("관리항목2");
			}			
		}
	};
	
	scwin.btn_mngmt_item_1_onclick = function(e) {
		if(scwin.data.mngmt_item_cd_1 == "1003"){ // 거래처코드
			com.pop.open(com.pop.TYPE.VEND, "", function(ret) {
				dma_search.set("mngmt_item_1", ret.vend_cd);
				dma_search.set("mngmt_item_nm_1", ret.vend_abbr);
			});
		}else if(scwin.data.mngmt_item_cd_1 == "1200"){ // 관련계정번호
			com.pop.open(com.pop.TYPE.ACNNO, {accnt_no:ibx_mngmt_item_1.getValue()}, function(ret) {
				dma_search.set("mngmt_item_1", ret.accnt_no);
				dma_search.set("mngmt_item_nm_1", ret.accnt_no_nm);
			});
		}else if(scwin.data.mngmt_item_cd_1 == "1001"){ // 개인번호
			com.pop.open(com.pop.TYPE.HUM, {empno:ibx_mngmt_item_1.getValue()}, function(ret) {
				dma_search.set("mngmt_item_1", ret.empno);
				dma_search.set("mngmt_item_nm_1", ret.nm);
			});
		}
	};
	
