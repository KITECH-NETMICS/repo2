         <script type="text/javascript" lazy="false"><![CDATA[
    /****************************************************
 	 * 화면 설명 : 내부지원신청(유상) 조회
 	 * 변경 이력 : 2023-10-25	김준하	최초 작성
 	 * serviceId : S331
 	 ****************************************************/  
 	/**
	* 페이지 로드
	*/
 	scwin.data ={imptAccntNo : "",
 				 reqNm : "",
 				 accntNo : "",
 				 reqState : "",
 	             };	          
	scwin.onpageload = function() {
		var today = com.getServerDateTime();
		var session = com.getLoginInfo();
		
		// enter key event
		com.setEnterKeyEvent(grp_emp, scwin.btn_hum_onclick);
		com.setEnterKeyEvent(grp_accnt, scwin.btn_accnt_onclick);
		com.setEnterKeyEvent(grp_tlmpt_accnt, scwin.btn_lmpt_accnt_no_onclick);						
		ica_req_ymd_st.setValue(today.substring(0, 6) + "01");		
		ica_req_ymd_ed.setValue(today);
		
		// 기간 역전 체크
		com.checkPeriod(ica_req_ymd_st, ica_req_ymd_ed);
		ibx_req_nm.setValue(session.cn);
		ibx_req_empno.setValue(session.empno);
		dma_search.set("req_psn_syspayno", session.sn);
		radio_req_state.setValue("XAD010");  
		// 데이터 유효성 체크
		com.setValidGroup(grp_search, [
			  {id:"req_ymd_st", mandatory :true}
			, {id:"req_ymd_ed", mandatory :true}
		]);
		
		// 공통코드
		var codeOptions = [{ code : "SLC", compID :"sbx_supt_team" }];
		com.setCmnCd(codeOptions, function() {				
		});																
	};	
	scwin.onpageunload = function() {
		
	};
	
	// 처리상태 변경 이벤트
	scwin.radio_req_state_onchange = function(info) {
		if (radio_req_state.getValue() == "XAD010") {
			output_search_date.setValue("신청일자");		
		}		
		if (radio_req_state.getValue() == "XAD070") {
			output_search_date.setValue("지원일자");
		}				
	};
	
	// 신청계정번호 클릭 이벤트
	scwin.btn_accnt_onclick = function(e) {
		scwin.acnNo("Accnt");	
	};
	
	// 수익계정번호 클릭 이벤트
	scwin.btn_lmpt_accnt_no_onclick = function(e) {
		scwin.acnNo("Tlmpt_Accnt");	
	};
	
	/*
     * 계정번호 팝업
     */
	scwin.acnNo = function(clsf){
		var data={clsf : "gen"};				
		if (clsf == "Accnt") {
			data.accnt_no = ibx_accnt_no.getValue();			
		} else if (clsf == "Tlmpt_Accnt") {
			data.accnt_no = ibx_lmpt_accnt_no.getValue();
		}	
		data.ref_yn = (data.accnt_no == "") ? "Y": "N"				
		com.pop.open(com.pop.TYPE.ACNNO, data, function(ret){
			if (clsf == "Accnt") {
				ibx_accnt_no.setValue(ret.accnt_no);
				ibx_accnt_no_nm.setValue(ret.accnt_no_nm); 			
			} else if (clsf == "Tlmpt_Accnt") {
				ibx_lmpt_accnt_no.setValue(ret.accnt_no);
			}
		});
	}	
	
	//신청자 버튼클릭	
	scwin.btn_hum_onclick = function(e) {
		var data = { work_clsf : "HAG010",
				     empno : ibx_req_empno.getValue(),
				     nm : ibx_req_nm.getValue(),
				     ref_yn : (ibx_req_nm.getValue() == "")? "Y" : "N" };	
		com.pop.open( com.pop.TYPE.HUM, data, function(ret){
			ibx_req_nm.setValue(ret.nm);
			ibx_req_empno.setValue(ret.empno);
			dma_search.set("req_psn_syspayno", ret.syspayno);	 				
		});		
	};
	
	scwin.ibx_req_nm_onchange = function(info) {
		if (info.newValue == "") {
			ibx_req_nm.setValue("");
			ibx_req_empno.setValue("");
			dma_search.set("req_psn_syspayno", "");
		}			
	};

	//조회 버튼 클릭		
	scwin.btn_search_onclick = function(e) {
		if (com.validateGroup(grp_search)) {
			scwin.data.imptAccntNo = ibx_lmpt_accnt_no.getValue();
			scwin.data.reqNm = ibx_req_nm.getValue();
			scwin.data.accntNo = ibx_accnt_no.getValue();
			scwin.data.reqState = radio_req_state.getValue();
			if (e == "Excel") {	
				scwin.excel_detail();			
			} else {
				scwin.search();
			}
		}		
	};
		
	/**	
     *	조회
     **/
	scwin.search = function() {
		var imptAccntNo = scwin.data.imptAccntNo;
		var reqNm = scwin.data.reqNm;
		var accntNo = scwin.data.accntNo;
		var reqState = scwin.data.reqState;
		var reqYmdSt = dma_search.get("req_ymd_st");
		var reqYmdEd = dma_search.get("req_ymd_ed");

		// 수익 계정
		if (imptAccntNo.length == 8) {
			dma_search.set("impt_accnt_no", imptAccntNo);
		} else {
		    if (imptAccntNo.length > 0) {
			    dma_search.set("impt_accnt_no_like", imptAccntNo + "%");
		    }
		}
		   
		// 계정 번호
		if (accntNo.length == 8) {
			dma_search.set("accnt_no", accntNo);
		} else {
		    if (accntNo.length > 0) {
			    dma_search.set("impt_accnt_no_like", accntNo);
		    }                    
		}          		
		var unSlipNo = ibx_unslip_no.getValue();
		dma_search.set("unslip_no", unSlipNo);	
		var taxCd = sbx_tax_cd.getValue();
		dma_search.set("tax_cd", taxCd);
		dma_search.set("rcpt_no", ibx_rcpt_no.getValue());		
		var syspayGubun = sbx_syspay.getValue();
		reqSyspayno = dma_search.get("req_psn_syspayno");
		
		if (syspayGubun == "A") {
			dma_search.set("req_psn_syspayno", reqSyspayno);
			dma_search.set("rcpt_psn_syspayno", "");
			dma_search.set("charg_syspayno", "");
			dma_search.set("supt_respn_syspayno", "");
		} else if (syspayGubun == "B") {
			dma_search.set("req_psn_syspayno", "");
			dma_search.set("rcpt_psn_syspayno", reqSyspayno);
			dma_search.set("charg_syspayno", "");
			dma_search.set("supt_respn_syspayno", "");			
		} else if (syspayGubun == "C") {
			dma_search.set("req_psn_syspayno", "");
			dma_search.set("rcpt_psn_syspayno", "");
			dma_search.set("charg_syspayno", reqSyspayno);
			dma_search.set("supt_respn_syspayno", "");			
		} else if (syspayGubun == "D") {
			dma_search.set("req_psn_syspayno", "");
			dma_search.set("rcpt_psn_syspayno", "");
			dma_search.set("charg_syspayno", "");
			dma_search.set("supt_respn_syspayno", reqSyspayno);			
		} 
		
		dma_search.set("supt_clsf", sbx_supt_team.getValue());

		reqState = radio_req_state.getValue();
		
              // 처리 상태
              if (reqState == 'XAD010') {             
               dma_search.set("req_clsf", "FBN002");	
              }                        

              if (reqState == 'XAD070') {
                  // 접수 일자
                  if (reqYmdSt != "") {
	                  dma_search.set("complt_demnd_ymd_st", reqYmdSt);					                
                  }            
                  if (reqYmdEd != "") {
	                  dma_search.set("complt_demnd_ymd_ed", reqYmdEd);                                
                  }
                  dma_search.set("req_clsf", "FBN002");                
              }                        
		dma_search.set("req_state", reqState);   			
		com.execute(sbm_selectData, {				
			successCallback : function(e){
				tbx_result.setValue(dlt_list.getTotalRow());			
			}	
		});		
	};	
	
	//액셀 내리기
	scwin.btn_excel_onclick = function(e) {
		if (grd_list.getRowCount() == "0") {
			alert("데이터가 없습니다");
			return;
		}		
		var option = {fileName : "내부지원신청(유상) 그리드 엑셀내리기.xlsx", hiddenVisible : "true"};
		com.downloadGridExcel(grd_list, option);					
	};
	
	//액셀 내리기 상세
	scwin.btn_excel_detail_onclick = function(e) {
		scwin.btn_search_onclick("Excel");	
	};
	
	/**	
     *	인쇄
     **/
	scwin.excel_detail = function(row) {
		var imptAccntNo = scwin.data.imptAccntNo;
		var reqNm = scwin.data.reqNm;
		var accntNo = scwin.data.accntNo;
		var reqState = scwin.data.reqState;
		var reqYmdSt = dma_search.get("req_ymd_st");
		var reqYmdEd = dma_search.get("req_ymd_ed");
		var	reqSyspayno = dma_search.get("req_psn_syspayno");
				
		if (reqState == 'XAD010') {
			dma_excel_search.set("req_ymd_st", reqYmdSt);
			dma_excel_search.set("req_ymd_ed", reqYmdEd);
			dma_excel_search.set("req_clsf", "FBN002");
            // 신청자
            if (reqSyspayno.length == 8) {
	            dma_excel_search.set("req_psn_syspayno", reqSyspayno);
            }                         
            // 계정 번호
            if (accntNo.length == 8) {
	            dma_excel_search.set("accnt_no", accntNo);
            } else {
                if (accntNo.length > 0) {
	                dma_excel_search.set("impt_accnt_no_like", accntNo);
                }                    
            }                                       
            dma_excel_search.set("req_state_in1", "XAD005");
            dma_excel_search.set("req_state_in2", "XAD010"); 
            dma_excel_search.set("req_state_in3", "XAD020"); 
            dma_excel_search.set("req_state_in4", "XAD100");  					
		}	
        if (reqState == 'XAD070') {
			dma_excel_search.set("complt_demnd_ymd_st", reqYmdSt);
			dma_excel_search.set("complt_demnd_ymd_ed", reqYmdEd);
			dma_excel_search.set("req_clsf", "FBN002");
            // 접수자
            if (reqSyspayno.length == 8) {
	            dma_excel_search.set("rcpt_psn_syspayno", reqSyspayno);
            }
            // 수익 계정
            if (accntNo.length == 8) {
	            dma_excel_search.set("impt_accnt_no", accntNo);
            } else {
                if (accntNo.length > 0) {
	                dma_excel_search.set("impt_accnt_no_like", accntNo + "%");
                }
            }
            dma_excel_search.set("req_state_in", "'XAD070', 'XAD100'");                                                                  				       
	    }
		com.execute(sbm_excelData, {				
			successCallback : function(e){
				if (grd_excel_list.getRowCount() == "0") {
					alert("데이터가 없습니다");
